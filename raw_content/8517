<script>

$(function() {

  // Variables
  var tabs = $('div.top-five > ul > li');
  var top5 = $('div.top-five > div');
    
  // On Click
  $(tabs).click(function() {
    
    $(tabs).removeClass("active");
    $(this).addClass("active");
    
    $(top5).hide();
    var activeTab = $(this).find("a").attr("href");
    $(activeTab).show();
    
    return false;
  });
  
  
  // Hot Carousel
  $("#hot-carousel li").show();
  $("#hot-carousel").jcarousel({
  scroll: 1,
  wrap: 'both',
  auto: 6,
  itemFallbackDimension: 280,
  initCallback: 
  function (carousel) {
    $('.control div.slide-btn', carousel.container).bind('click', function() {
      carousel.scroll(jQuery.jcarousel.intval($(this).attr('image_id')));
      $('.control div',carousel.container).removeClass('selected');
      $(this).addClass('selected');
      return false;
    });
    $('.control div.next', carousel.container).bind('click', function() {
      carousel.next();
      $('.control div',carousel.container).removeClass('selected');
      $('.control div[image_id='+carousel.first+']',carousel.container).addClass('selected');
      return false;
    });
    $('.control div.prev', carousel.container).bind('click', function() {
      carousel.prev();
      $('.control div',carousel.container).removeClass('selected');
      $('.control div[image_id='+carousel.first+']',carousel.container).addClass('selected');
      return false;
    });
  },
  itemVisibleInCallback: function(carousel, item, idx, state) {
    $('.control div',carousel.container).removeClass('selected');
    $('.control div[image_id='+idx+']',carousel.container).addClass('selected');  
  },
  buttonNextHTML: null,buttonPrevHTML: null
  });

  // Category Carousel
  
  $("#category-carousel").jcarousel({
  scroll: 1,
  wrap: 'both',
  itemFallbackDimension: 562,
  initCallback: 
  function (carousel) {
    $('.control a.next', carousel.container).bind('click', function() {
      carousel.next();
      return false;
    });
    $('.control a.prev', carousel.container).bind('click', function() {
      carousel.prev();
      return false;
    });
  },
  buttonNextHTML: null,buttonPrevHTML: null
  });
  
  // Hot Category Carousel
  $("#category-hot-carousel li").show();
  $("#category-hot-carousel").jcarousel({
  scroll: 1,
  wrap: 'both',
  auto: 6,
  itemFallbackDimension: 608,
  initCallback: 
  function (carousel) {
    $('.control div.slide-btn', carousel.container).bind('click', function() {
      carousel.scroll(jQuery.jcarousel.intval($(this).attr('image_id')));
      $('.control div',carousel.container).removeClass('selected');
      $(this).addClass('selected');
      return false;
    });
    $('.control div.next', carousel.container).bind('click', function() {
      carousel.next();
      $('.control div',carousel.container).removeClass('selected');
      $('.control div[image_id='+carousel.first+']',carousel.container).addClass('selected');
      return false;
    });
    $('.control div.prev', carousel.container).bind('click', function() {
      carousel.prev();
      $('.control div',carousel.container).removeClass('selected');
      $('.control div[image_id='+carousel.first+']',carousel.container).addClass('selected');
      return false;
    });
  },
  itemVisibleInCallback: function(carousel, item, idx, state) {
    $('.control div',carousel.container).removeClass('selected');
    $('.control div[image_id='+idx+']',carousel.container).addClass('selected');  
  },
  buttonNextHTML: null,buttonPrevHTML: null
  });


  // Related Videos Carousel
  
/*
