<script type="text/javascript">
// <!--
	(function() {
		var
			phase = 0.25,
			increments = 60,
			panes = {
				mybag: {
					handler: new AjaxSimpleHandler(),
					preview: $("header_mybag_preview"),
					link: $("header_mybag_link"),
					timeout: null,
					active: 0,
					automatic: 0				},
				wishlist: {
					handler: new AjaxSimpleHandler(),
					preview: $("header_wishlist_preview"),
					link: $("header_wishlist_link"),
					timeout: null,
					active: 0,
					automatic: 0 }
			},
			show_preview = function(pane, type, fade) {
				var dimensions = {
					link: new ElementDimensions(pane["link"]),
					preview: new ElementDimensions(pane["preview"])
				};
				with (pane["preview"].style) {
					top = (dimensions["link"].top + dimensions["link"].outer.height) + "px";
					left = (dimensions["link"].left + dimensions["link"].outer.width - dimensions["preview"].inner.width) + "px";
				}
				if (!pane["preview"].innerHTML.trim().length) {
					pane["preview"].innerHTML = '<img height="37" width="60" onmousedown="this.blur()" src="http://cdn11.my-wardrobe.com/images/site/item_details/loading_logo.gif" alt="Loading..."/>';
					pane["handler"].request( "get", ("http://www.my-wardrobe.com/core/call_backs/ajax/preview.php?type=" + type), {
						success: function(responseText, responseXML) {
							pane["preview"].innerHTML = responseText;
						},
						failure: function(e) {

						}
					} );
				}
				clearTimeout( pane["timeout"] );
				var out = (pane["preview"].style.display !== "none")? true : false;
				fadeElement( pane["preview"], out, phase, increments);
			};
		for (var pane in panes) {
			var type = pane;
			pane = panes[pane];
			if (!pane["preview"] || !pane["link"]) {
				continue;
			}
			if (pane["active"]) {
				with (pane) {
					link.removeAttribute("href");
					link.title = ("show " + link.name + " preview");
					link.style.cursor = "pointer";
				}
				XBrowserAddEvent(
					pane["link"],
					"click",
					function(pane, type) {
						return function( ){
							show_preview(pane, type, false);
						}
					}(pane, type)
				);
			}
			if (pane["active"] && pane["automatic"]) {
				show_preview(pane, type, false);
				pane["timeout"] = setTimeout( function(pane, type) {
					return function( ){
						show_preview(pane, type, false);
					}
				}(pane, type), 4000 );
			}
		}
	}());
