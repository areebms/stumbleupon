                	<script type="text/javascript">
//clear les search box on click
function clickclear(thisfield, defaulttext,attr_name) {
	if (thisfield.value == defaulttext) {
		thisfield.value = "" ;
		thisfield.name = attr_name ;
	}
}
function clickrecall(thisfield, defaulttext) {
	if (thisfield.value == "") {
		thisfield.value = defaulttext ;
		thisfield.name = "" ;
	}
}
</script>
<form action="http://recherche.cyberpresse.ca/cyberpresse/search/theme/lapresse/">
	<input class="champsRecherche" type="text" value="Rechercher" onfocus="clickclear(this, 'Rechercher','q')" onblur="clickrecall(this,'Rechercher')" />
