                    </script></div><script type="text/javascript">setTimeout(function(){if(typeof social_campaign != 'undefined' && jQuery('.print-link')[0].href.indexOf(social_campaign) == -1 ) { var _print_href = jQuery('.print-link')[0].href; jQuery('.print-link').attr( 'href' , _print_href + '&comm_ref=' + social_campaign )} },3000)</script>		<!-- =================================================================== -->
	<!-- =================================================================== -->
	<!-- Now, goes hack's for different layouts -->


	
	<div id="badges_v2_1"  ><!-- Badge layout is: 8 --></div>

	
	<script type="text/javascript">
	
	
		var inst_def = Badges.getUniqName();

		window[inst_def.obj_name] = new Badges({
			unique_id: inst_def.unique_id,
			holder_id: "badges_v2_1",
			human_readable: 0,
			panel_layout: 8,
			complete_callback_func_name: "",
			share_details_callback: false,
			additional_panel_classes: "",
			entry_params: {
				"id" : 1057732,
				"title" : "Grilled Fish Tacos with Pico de Gallo",
				"created_on": 1319748900,
				"vertical_name": "KitchenDaily",
				"tweet_comm_hash" : "",
				"tweet_comm_text" : "",
				"force_fb_like"	  : 0,
                "custom_tweet_text" : false			},
						
			global_name: "window." + inst_def.obj_name
		});


	// =====================================================
	// Now goes logic for every layout


	


	


	
		window[inst_def.obj_name].setPanelBorderStyle("glamorous_4");

        var slices = {
		1: "facebook_glamorous",
		2: "retweet_glamorous",
		3: "email_glamorous",
		4: "google_plusone_glamorous"
        };
		
		// checking for ie7 safari 3 and ff 3.5 browsers because +1 badge not work
		if ((jQuery.browser.mozilla && (jQuery.browser.version <= '1.9.2')) 
			|| (jQuery.browser.msie && (jQuery.browser.version <= '7.0')) 
			|| (jQuery.browser.safari && (jQuery.browser.version <= '526.12.2')))
			slices[4] = "comment_glamorous";

		window[inst_def.obj_name].setSlices(slices);

	

	
	
	
	


			// Finaly, launch our badges
		Badges.launch("badges_v2_1", inst_def);
		</script>
	
