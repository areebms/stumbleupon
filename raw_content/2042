<script type="text/javascript">
    showWt('wt','wtab', '3',"3") 
   var grouppage
   var memberpage
   var discussionpage
   var contenttype
    grouppage=1
    memberpage=1
    discussionpage=1
    contenttype=""
  
 
function stateViewReply() 
{ 
	if (xmlHttp.readyState==4){
	    if (contenttype!=""){
	   	    document.getElementById(contenttype).innerHTML =xmlHttp.responseText;
	        eval("document.getElementById('loading"+contenttype+"')").style.display='none';	 
	   	}
	    contenttype="";
	}
}
function GetXmlHttpObject()
{
	var xmlHttp=null;
	try {xmlHttp=new XMLHttpRequest();}
	catch (e)
  	{
		try { xmlHttp=new ActiveXObject("Msxml2.XMLHTTP"); }
	  	catch (e)  	{ xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");}
  	}
	return xmlHttp;
}


</script>
