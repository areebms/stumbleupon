					var slideImages = [];
					var srcArray = [];
					var originalBackup = '';
 
					function collectSlide () {
        			if ($('.center').find('img').length<2) return;
						originalBackup = $('#main-contents').html();
        				$('.center').each (function (index) {
							if ($(this).find('img').length == 0) return;
                			slideImages.push ($(this).find ('a').html());
                			srcArray.push ($(this).find ('img').attr ('src'));
                			$(this).find ('img').each (function () {
                        	var self = this;
                        	$(self).attr("src", '/images/grey.gif');
                        	$(self).attr("id", 'img-'+(slideImages.length-1));
                        	self.loaded = false;
                		});
        			});
        			slideShow ();
					}
