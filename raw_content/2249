	 	  <div class="social-widget-slug flickr-slug">
	 	    <div class="social-widget-link flickr-link">
	 	      <a href="http://www.flickr.com/photos/abcnewsradio" target="_blank">
	 	       <img id="flickr-slug-avatar-image-7226166-1" class="avatar" src="/universal/images/social/flickr-link.png"/>
	 	       <span id="flickr-slug-text-7226166-1"></span>
	 	      </a>
	 	    </div>
	 	  </div>
	 	 
 

</div>




<script type="text/javascript">
<!--

Squarespace.Social.Flickr = {

  flickrWidget7226166  : "",
  FADE_FAST_SPEED         : 0.2,
  FADE_SPEED              : 0.5,
  SWIPE_SPEED             : 0.5,   
  slideDelay              : {},
  sidebarWidths           : {},  
  containerXOffsets       : {},
  containerXPositions     : {},  
  activeModule            : "",
  modules                 : new Array(),
  activeSlideIndex        : {},
  nextSlideIndex          : {},
  containerElements       : {},
  navElements             : {},  
  loadCounts              : {},
  displayCounts           : {},  
  timerID                 : {},
  isAutomated             : {},
  isPreloaded             : {},
  isGridPreset            : {},
  isSlidePreset           : {},  
  showSlideTransition     : {},
  isSlideTransitionFade   : {},
  isSlideTransitionSwipe  : {},  
  thumbsPerRow            : {},
  thumbPadding            : {},
  autoCrop                : {},
  autoPlay                : {},
  displayControls         : {},
  userList                : new Array(),
  

    
  onImagesPreloaded : function(mod) {        
    if(this.isSlidePreset[mod]) {
    	if (this.autoPlay[mod]) {
     		// Start slideshow
     		this.startTimer(mod);      		
    		this.isAutomated[mod] = true; 
    		this.swapPausePlayButton(mod);    		
    	} else {
    		this.isAutomated[mod] = false;
    	} 	
  	}
  	this.isPreloaded[mod] = true;
  },
  
  startTimer : function(mod) {
    this.timerID[mod] = YAHOO.lang.later( this.slideDelay[mod], this, 'doAutoPlay', mod, true ); 
  },
  
  stopTimer : function(mod) {
    this.timerID[mod].cancel();
  },
    
  unselectSlide : function() {
    var el = this.getEl();
    D.removeClass(el, 'selected-slide');
  },
		
  selectSlide : function() {
    var el = this.getEl();
    D.addClass(el, 'selected-slide');		
  },
  
  animateNoTransition : function(mod) {

    // unselect active slide
    D.removeClass('flickr-container-' + mod + '-' + this.activeSlideIndex[mod], 'selected-slide');
    
    if (this.nextSlideIndex[mod] == 0) {
    
      // jump to end
      this.nextSlideIndex[mod] = this.loadCounts[mod];
      this.activeSlideIndex[mod] = this.loadCounts[mod] + 1;
    
    } else if (this.nextSlideIndex[mod] == (this.loadCounts[mod] + 1)) {
    
      //jump to beginning
      this.nextSlideIndex[mod] =  1;
      this.activeSlideIndex[mod] = 0;
    }   

    // select next slide
    D.addClass('flickr-container-' + mod + '-' + this.nextSlideIndex[mod], 'selected-slide');
    
  },
  
  animateFade : function(mod) {
  	// fade out active slide  	
  	var slide = D.get('flickr-container-' + mod + '-' + this.activeSlideIndex[mod]);
  	var anim = new YAHOO.util.Anim(slide, { 
  		opacity: { from: 1, to: 0 }
  	}, this.FADE_SPEED);
  	anim.onComplete.subscribe(this.unselectSlide);
  	anim.onStart.subscribe(this.onFadeFinish);
  	anim.animate(); 
  },
  
	onFadeFinish : function() {
	
	  // get current module
    var el = this.getEl();        
    var container = D.getAncestorByClassName(el, 'flickr-container');
    var stringLength = "flickr-container-";
    var mod = container.id.substr(stringLength.length); 
    var that = Squarespace.Social.Flickr;

		if (that.nextSlideIndex[mod] == 0) {
		
			// jump to end
			that.nextSlideIndex[mod] = that.loadCounts[mod];
			that.activeSlideIndex[mod] = that.loadCounts[mod] + 1;
		
		} else if (that.nextSlideIndex[mod] == (that.loadCounts[mod] + 1)) {
			//jump to beginning
			that.nextSlideIndex[mod] =  1;
			that.activeSlideIndex[mod] = 0;
		}		
    
		// fade in next slide
		var slide = D.get('flickr-container-' + mod + '-' + that.nextSlideIndex[mod]);
		D.setStyle(slide, 'opacity', '0');
		var anim = new YAHOO.util.Anim(slide, { 
			opacity: { from: 0, to: 1 }
		}, that.FADE_SPEED);
		anim.onStart.subscribe(that.selectSlide);
		anim.animate();  
	},		
  
	animateSwipe : function(mod) {
	  // swipe out active slide
	 	var anim = new YAHOO.util.Motion(this.containerElements[mod], {
		    points: {
		        to: [this.containerXPositions[mod], D.getY(this.containerElements[mod])]
		    } 
		}, this.SWIPE_SPEED, YAHOO.util.Easing.easeOutStrong);
		anim.onComplete.subscribe(this.onSwipeFinish);
		anim.animate(); 
	},
	 
	onSwipeFinish : function() {
	
	  // get current module
	  var el = this.getEl();	  
	  var stringLength = "flickr-container-";
	  var mod = el.id.substr(stringLength.length);
	  var that = Squarespace.Social.Flickr;
    
    if (that.activeSlideIndex[mod] == 0) {

      // jump to post slide
      that.containerXPositions[mod] = D.getX(that.containerElements[mod]) - ( that.loadCounts[mod] * that.sidebarWidths[mod]);
      D.setX(that.containerElements[mod], that.containerXPositions[mod]);
      that.activeSlideIndex[mod] = that.loadCounts[mod];      
     
    } else if (that.activeSlideIndex[mod] == (that.loadCounts[mod] + 1)) {

      // jump to pre slide    
      D.setX(that.containerElements[mod], that.containerXOffsets[mod] - that.sidebarWidths[mod]);
      that.containerXPositions[mod] = D.getX(that.containerElements[mod]);
      that.activeSlideIndex[mod] = 1;

    }
    
	},
	
	doAutoPlay : function(mod) {

	  if (this.showSlideTransition[mod]) {
	  	  
  		if (this.isSlideTransitionFade[mod]) {
  			this.nextSlideIndex[mod] = this.activeSlideIndex[mod] + 1;
  			this.animateFade(mod);					
  			this.activeSlideIndex[mod] += 1;	
  		
  		} else if (this.isSlideTransitionSwipe[mod]) {
  			this.containerXPositions[mod] = this.containerXPositions[mod] - this.sidebarWidths[mod];
  			this.animateSwipe(mod);					
  			this.activeSlideIndex[mod] += 1;	
  		}  
  		
    } else {
    
        // no animation
        this.nextSlideIndex[mod] = this.activeSlideIndex[mod] + 1;
        this.animateNoTransition(mod);          
        this.activeSlideIndex[mod] += 1;
    }	
 	},
 	 
 	swapPausePlayButton : function(mod) {
		var buttonElement = D.get('flickr-nav-playPause-' + mod);		
		if (this.isAutomated[mod]) {
			buttonElement.src = "/universal/images/slideshow/pause-button.png";
			
		} else {
			buttonElement.src = "/universal/images/slideshow/play-button.png";
		} 	 
 	 }

};	
	
	
  E.onDOMReady( function() {
    
    var flickr = Squarespace.Social.Flickr;
    flickr.modules.push(7226166);
    flickr.activeSlideIndex[7226166] = 1;
    flickr.nextSlideIndex[7226166] = 2;    
    flickr.navElements[7226166] = D.getElementsByClassName('flickr-nav-button', 'div', 'flickr-nav-7226166');
    flickr.loadCounts[7226166] = 0;
    flickr.displayCounts[7226166] = 9;
    flickr.showSlideTransition[7226166] = (true) ? true : false;
    flickr.isSlideTransitionFade[7226166] = (true) ? true : false;
    flickr.isSlideTransitionSwipe[7226166] = (false) ? true : false;
    flickr.isGridPreset[7226166] = (true) ? true : false;
    flickr.isSlidePreset[7226166] = (false) ? true : false;
    flickr.thumbsPerRow[7226166] = 3;
    flickr.thumbPadding[7226166] = 2;
    flickr.autoCrop[7226166] = true;
    flickr.autoPlay[7226166] = true;
    flickr.displayControls[7226166] = (true) ? true : false;
    flickr.slideDelay[7226166] = 3 * 1000;
    flickr.isAutomated[7226166] = false;
    flickr.isPreloaded[7226166] = false;
    
    // get containing sidebar
    var sidebar = D.getAncestorByClassName('moduleContentWrapper7226166', 'verticalNavigationBarWrapper');
    var section = D.getAncestorByClassName('moduleContentWrapper7226166', 'section');
    flickr.sidebarWidths[7226166] = parseInt(D.getStyle(sidebar, "width"));    
    var content = D.get('flickr-content-7226166');
    
    // adjust sidebar for borders
    var borderRightWidth = parseInt(D.getStyle(content, 'borderRightWidth'));
    var borderLeftWidth = parseInt(D.getStyle(content, 'borderLeftWidth'));
    if (borderRightWidth) {
      flickr.sidebarWidths[7226166] -= borderRightWidth;
    }
    if (borderLeftWidth) {
      flickr.sidebarWidths[7226166] -= borderLeftWidth;
    }

    // adjust sidebar for section padding
    var sectionLeftPaddingWidth = parseInt(D.getStyle(section, 'paddingLeft'));
    var sectionRightPaddingWidth = parseInt(D.getStyle(section, 'paddingRight'));
    if (sectionLeftPaddingWidth) {
      flickr.sidebarWidths[7226166] -= sectionLeftPaddingWidth;
    }
    if (sectionRightPaddingWidth) {
      flickr.sidebarWidths[7226166] -= sectionRightPaddingWidth;
    }
    
    // adjust sidebar for section margins
    var sectionLeftMarginWidth = parseInt(D.getStyle(section, 'marginLeft'));
    var sectionRightMarginWidth = parseInt(D.getStyle(section, 'marginRight'));
    if (sectionLeftMarginWidth) {
      flickr.sidebarWidths[7226166] -= sectionLeftMarginWidth;
    }
    if (sectionRightMarginWidth) {
      flickr.sidebarWidths[7226166] -= sectionRightMarginWidth;
    }
    


    //Setup status messages
    if (flickr.isGridPreset[7226166]) {      
      var thumbW = Math.floor(flickr.sidebarWidths[7226166] / flickr.thumbsPerRow[7226166]);
      var estHeight = Math.round( (flickr.displayCounts[7226166] / flickr.thumbsPerRow[7226166])) * thumbW;
      D.setStyle('flickr-wrapper-7226166', 'height', estHeight + 'px');
      D.setStyle('flickr-wrapper-7226166', 'width', flickr.sidebarWidths[7226166] + 'px'); 
      D.setStyle('flickr-content-7226166', 'height', estHeight + 'px'); 
      D.setStyle('flickr-content-7226166', 'width', flickr.sidebarWidths[7226166] + 'px');       
    }
    
          // get user flickr links
      var flickrLinkText = "@user's photostream";
          
              D.get('flickr-slug-text-7226166-1').innerHTML = flickrLinkText.replace("@user", "abcnewsradio"); 
            
    
    

    flickrWidget7226166 = new Squarespace.Social.FlickrWidget(7226166, {
      preset : 'grid', 
      displayCount : flickr.displayCounts[7226166], 
      loadCount : flickr.loadCounts[7226166],       
      showSlideTransition : flickr.showSlideTransition[7226166],
      slideTransitionFade : flickr.isSlideTransitionFade[7226166],
      slideTransitionSwipe : flickr.isSlideTransitionSwipe[7226166],
      sidebarWidth : flickr.sidebarWidths[7226166],
      autoCrop : flickr.autoCrop[7226166],      
      thumbsPerRow : flickr.thumbsPerRow[7226166],
      thumbPadding : flickr.thumbPadding[7226166]     
    });
    flickrWidget7226166.show();  
    
    if(flickr.isSlidePreset[7226166]) {        
      D.setStyle('flickr-wrapper-7226166', 'width', flickr.sidebarWidths[7226166] + 'px');      
      D.setStyle('flickr-content-7226166', 'height', flickr.sidebarWidths[7226166] + 'px');     
      D.setStyle('flickr-container-7226166', 'height', flickr.sidebarWidths[7226166] + 'px');
      
      // adjust container
      flickr.containerElements[7226166] = new YAHOO.util.Element('flickr-container-7226166');
      flickr.containerElements[7226166].setStyle('opacity', 0);
    }
    
    if (flickr.showSlideTransition[7226166]) {

      if (flickr.isSlideTransitionFade[7226166]) {
      
        // set individual container widths
        for (var i = 1; i <= flickr.loadCounts[7226166]; i++) {      
          D.setStyle('flickr-container-7226166-' + i, 'width', flickr.sidebarWidths[7226166] + 'px');
          D.setStyle('flickr-container-7226166-' + i, 'height', flickr.sidebarWidths[7226166] + 'px');
          D.addClass('flickr-container-7226166-' + i, 'flickr-animation-fade');       
        }    
      
      } else if (flickr.isSlideTransitionSwipe[7226166]) {
        
        // adjust container props
        flickr.containerXOffsets[7226166] = D.getX(flickr.containerElements[7226166]);        
        flickr.containerElements[7226166].setStyle('min-width', ((flickr.sidebarWidths[7226166] * flickr.loadCounts[7226166]) + (flickr.sidebarWidths[7226166] * 2)) + 'px');       
        D.setX(flickr.containerElements[7226166], D.getX(flickr.containerElements[7226166]) - flickr.sidebarWidths[7226166] );
        flickr.containerXPositions[7226166] = D.getX(flickr.containerElements[7226166]); 
    
        // set individual container widths
        for (var i = 1; i <= flickr.loadCounts[7226166]; i++) {      
          D.setStyle('flickr-container-7226166-' + i, 'width', flickr.sidebarWidths[7226166] + 'px');
          D.setStyle('flickr-container-7226166-' + i, 'height', flickr.sidebarWidths[7226166] + 'px');
          D.addClass('flickr-container-7226166-' + i, 'flickr-animation-swipe');
        }
        D.setStyle('flickr-container-7226166-pre', 'width', flickr.sidebarWidths[7226166] + 'px');
        D.setStyle('flickr-container-7226166-post', 'width', flickr.sidebarWidths[7226166] + 'px');
        D.setStyle('flickr-container-7226166-pre', 'height', flickr.sidebarWidths[7226166] + 'px');
        D.setStyle('flickr-container-7226166-post', 'height', flickr.sidebarWidths[7226166] + 'px');
        D.addClass('flickr-container-7226166-pre', 'flickr-animation-swipe');
        D.addClass('flickr-container-7226166-post', 'flickr-animation-swipe');
      }   
      
     } else {
     
        // set individual container widths
        for (var i = 1; i <= flickr.loadCounts[7226166]; i++) {      
          D.setStyle('flickr-container-7226166-' + i, 'width', flickr.sidebarWidths[7226166] + 'px');
          D.setStyle('flickr-container-7226166-' + i, 'height', flickr.sidebarWidths[7226166] + 'px');
          D.addClass('flickr-container-7226166-' + i, 'flickr-animation-none');       
        } 
        
     }
     
     YAHOO.util.Event.on("flickr-content-7226166", "mouseenter", function(e) {      
        if (flickr.isPreloaded[7226166] && flickr.displayControls[7226166]) {        
          var anim = new YAHOO.util.Anim("flickr-nav-7226166", { 
            opacity: { from: 0, to: 1 }
          }, flickr.FADE_FAST_SPEED);
          anim.animate();
          
          var anim2 = new YAHOO.util.Anim(flickr.navElements[7226166], { 
            opacity: { from: 0, to: 0.7 }
          }, flickr.FADE_FAST_SPEED);
          anim2.animate();  
        }    
    });
    
      
    YAHOO.util.Event.on("flickr-content-7226166", "mouseleave", function(e) {
      if (flickr.isPreloaded[7226166] && flickr.displayControls[7226166]) {
        var anim = new YAHOO.util.Anim("flickr-nav-7226166", { 
          opacity: { from: 1, to: 0 }
        }, flickr.FADE_FAST_SPEED);
        anim.animate();  
        
        var anim2 = new YAHOO.util.Anim(flickr.navElements[7226166], { 
          opacity: { from: 0.7, to: 0 }
        }, flickr.FADE_FAST_SPEED);
        anim2.animate();  
      }
    });
    

    YAHOO.util.Event.on(flickr.navElements[7226166], "click", function(e) {
    
      // get current module
      var mod = 7226166;
      
      if (this.id == "flickr-nav-button-center-" + mod) {
      
        // start/stop autoplay
        if (flickr.isAutomated[mod]) {
          flickr.stopTimer(mod);   
          flickr.isAutomated[mod] = false;
        
        } else {
          flickr.startTimer(mod);
          flickr.isAutomated[mod] = true;
        } 
        flickr.swapPausePlayButton(mod);
      
      } else {    
        
        if (flickr.showSlideTransition[mod]) {
        
          if (flickr.isSlideTransitionFade[mod]) { 

              // fade transition 

              if (this.id == "flickr-nav-button-left-" + mod) {                  
                flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] - 1;
                flickr.animateFade(mod);
                flickr.activeSlideIndex[mod] -= 1;  
              
              } else if (this.id == "flickr-nav-button-right-" + mod) {  
                flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] + 1;
                flickr.animateFade(mod);
                flickr.activeSlideIndex[mod] += 1;      
              }   
              
           } else if (flickr.isSlideTransitionSwipe[mod]) {
            
              // swipe transition 
            
              if (this.id == "flickr-nav-button-left-" + mod) {  
                flickr.containerXPositions[mod] = flickr.containerXPositions[mod] + flickr.sidebarWidths[mod];
                flickr.animateSwipe(mod);         
                flickr.activeSlideIndex[mod] -= 1;
              
              } else if (this.id == "flickr-nav-button-right-" + mod) {        
                flickr.containerXPositions[mod] = flickr.containerXPositions[mod] - flickr.sidebarWidths[mod];
                flickr.animateSwipe(mod);         
                flickr.activeSlideIndex[mod] += 1;  
              }
            }
          
          } else {
            
            // no animated transition

            if (this.id == "flickr-nav-button-left-" + mod) {
              flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] - 1;
              flickr.animateNoTransition(mod);
              flickr.activeSlideIndex[mod] -= 1;  
            
            } else if (this.id == "flickr-nav-button-right-" + mod) { 
              flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] + 1;
              flickr.animateNoTransition(mod);
              flickr.activeSlideIndex[mod] += 1;      
            }             
          }
        }
    });
  
  
  });  
  

  

//-->
</script></div></div></div>

        

      
  
    </div></div></div></div></div></div>

  

