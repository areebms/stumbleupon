			function changeTab(el, contentSrc) {
				var tabs = document.getElementById('tabs').getElementsByTagName('div');	
				var contentBlocks = document.getElementById('contentTarget').getElementsByTagName('ul');	
				for (var i=0; i<tabs.length; i++) {
					if (tabs[i] === el) {
						tabs[i].className = 'here';	
					} else {
						tabs[i].className = '';	
					}
				}
				for (var c=0; c<contentBlocks.length; c++) {
					
					if (contentBlocks[c].id !== contentSrc) {
						contentBlocks[c].style.display = 'none';
					} else {
						contentBlocks[c].style.display = 'block';
					}	
				}				
			}
