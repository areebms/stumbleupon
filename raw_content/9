var referrerHostname = document.referrer.split('/')[2];
var searchEngines = ['google.com','yahoo.com','bing.com','ask.com','stumbleupon.com'];
if inArray(referrerHostname,searchEngines) {
	document.write('http://www.thedailymeal.com/sites/all/themes/external/whiteglove.php?source=' + referrerHostname[1] + '&qs=' + document.referrer.substring(1));
}

function inArray(needle, haystack) {
    var length = haystack.length;
    for(var i = 0; i < length; i++) {
        if(haystack[i] == needle) return true;
    }
    return false;
}

