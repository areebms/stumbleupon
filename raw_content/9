          <div id="logo-floater">
        <h1><a href="/" title="The Daily Meal "><img src="http://images.thedailymeal.net/sites/all/themes/thedailymeal/images/thedailymeal_logo.png" alt="The Daily Meal " id="logo" /></a></h1></div><script type="text/javascript">

// The following was pulled from header_search_form.inc and put here:
function headerSearchFormTabClick(callerRef) { //, href, title) {
    // Style the tabs:
    var tabs = document.getElementsByClassName("header_search_tab");
    var class_str = "header_search_tab";
    
    for (var i=0; i<tabs.length; i++) {
        class_str = tabs[i] == callerRef ? "header_search_tab current" : "header_search_tab";
        tabs[i].setAttribute("class", class_str);
    }
    // Done styling tabs

	// generic formatting:
    document.getElementById("header_search_form_input").setAttribute("value", callerRef.getAttribute("title"));
    document.getElementById("header_search_form").setAttribute("action", callerRef.getAttribute("href"));

    document.getElementById("header_search_form_input_1").setAttribute("value", callerRef.getAttribute("title"));
    document.getElementById("header_search_form_1").setAttribute("action", callerRef.getAttribute("href"));

    var extra_input = document.getElementById("header_search_form_input_2");
    var extra_input1 = document.getElementById("header_search_form_input_1");
    var form = document.getElementById("header_search_form"); //$("#header_search_form");
    var form1 = document.getElementById("header_search_form_1"); //$("#header_search_form_1");
	
	if (callerRef.id == 'header_search_restaurants') {
        // Venue Search:
        form.style.display = "none";
        form1.style.display = "block";

        extra_input.value = "City, State OR Zipcode";
        extra_input.setAttribute("class", "shorten");
        extra_input.style.display = "inline";

        extra_input1.value = "Enter Name or Cuisine...";
        extra_input1.setAttribute("class", "shorten");
        extra_input1.style.dislay = "inline";

	} else if (callerRef.id == 'header_search_recipes') {
        // Recipe search
        form.style.display = "none";
        form1.style.display = "block";
        extra_input1.setAttribute("class", "");
        extra_input1.value = "Search Recipes...";

        extra_input.style.display = "none";
	} else {
        // Site Search:
        form1.style.display = "none";
        form.style.display = "block";
    }
} // headerSearchFormTabClick()

// finally - click handler for the button:
function header_site_search_submit() {
    //var val = document.getElementById("header_search_form_input").getAttribute("value");
	var val = document.getElementById("header_search_form_input").value;
    if (val && val != "") {
        location.href = "/search/apachesolr_search/" + encodeURIComponent(val);
    }
    return false;
}

function header_recipe_venue_search_submit() {
    //var act = document.getElementById("header_search_form_1").getAttribute("action");
	//var act = document.getElementById("header_search_form_1").attr("action");	 
    var act = document.getElementById("header_search_form_1").action;	 
    var val = encodeURIComponent(document.getElementById("header_search_form_input_1").value);
    var loc;

    if (act.indexOf("recipe") > -1) {
        loc = "/recipe_search?recipe=" + val;
    } else if (act.indexOf("where") > -1) {
        var val2 = encodeURIComponent(document.getElementById("header_search_form_input_2").value);
        //location.href = "/search/where?where=" + encodeURIComponent($("#header_search_form_input_2").attr("value")) + "&what=" + encodeURIComponent($("#header_search_form_input_1").attr("value"))+"&publisher=tdm";
        loc = "/search/venues?place=" + val2 + "&term=" + val;
    } 
    location.href = loc;
    return false;
}


</script>


