<script type="text/javascript">

	huff.use('quickread', function(q){
                q.ads = false;
        		q.ad_zone = 'news/quickread';
		q.hint('#featured_content .quickread_link, #news_column .quickread_link', function(e){
			var entry = e.parent().attr('href').match(/_[n|b]_(\d+)\./) ||
				e.attr('id').match(/image_(\d+)/) ||
				e.parent().attr('id').match(/entry_id_(\d+)/);

			entry && entry[1] && q.show(entry[1]);
		});

		jQuery(document).delegate('.quick_read_link', 'click', function(e){
			var entry = jQuery(e.currentTarget).attr('href').match(/_[n|b]_(\d+)\./);
			entry && entry[1] && q.show(entry[1]);
			e.preventDefault();
		});
	});

</script>


