		jQuery('#af-android_bubble .s2disclaimer').slideUp(500, function(){
			jQuery('#af-android_bubble .s2disclaimer').html(jQuery('#af-android_bubble .s2legal').html()).slideDown(500);
		});
	}
	return false;
});
jQuery('#af-android_bubble input[value="Send"]').live('click', function(){
	s2title = jQuery('#af-android_bubble .s2title');
	phone_n = jQuery('#af-android_bubble input[name="p1"]').val().replace(/-/g, '');
	if(phone_n == 5555555555 || isNaN(phone_n) || phone_n.length != 10) {
		jQuery('#af-android_bubble .s2subtitle').fadeOut(500, function () {
			jQuery('#af-android_bubble .s2subtitle').html('Please use a valid 10-digit mobile number.').css('color', '#FF0000').fadeIn(500);
		});
		return false;
	}
	jQuery('#af-android_bubble .s2title').html('Sending...');
	var appid = '26';
	var appkey = 'ao1qvHJSh3Z7hgQQ';
	var phone = phone_n;
	var message = 'Huffington Post for Android';
	var link = 'http://market.android.com/details?id=com.huffingtonpost.android';
	var url = 'http://s2c.aol.com/s2c/sendtocell.jsp?appid='+encodeURIComponent(appid)+'&appkey='+encodeURIComponent(appkey)+'&phone='+encodeURIComponent(phone)+'&message='+encodeURIComponent(message)+'&link='+encodeURIComponent(link);
	console.log(url);
	jQuery.ajax({
		url: url,
		dataType: "jsonp",
		crossDomain: true,
		timeout: 15 * 1000,
		error: errorHandle
	});
	return false;
});
jQuery('#af-android_bubble .closable').live('click', function(){
	jQuery('#af-android_bubble').parent().addClass('hidden');
});
</script>

<script type="text/javascript">
jQuery('#af-blackberry_bubble .showlegal').live('click', function(){
	if(jQuery('#af-blackberry_bubble .s2disclaimer').html() != jQuery('#af-blackberry_bubble .s2legal').html()) {
		jQuery('#af-blackberry_bubble .s2disclaimer').slideUp(500, function(){
			jQuery('#af-blackberry_bubble .s2disclaimer').html(jQuery('#af-blackberry_bubble .s2legal').html()).slideDown(500);
		});
	}
	return false;
});
jQuery('#af-blackberry_bubble input[value="Send"]').live('click', function(){
	s2title = jQuery('#af-blackberry_bubble .s2title');
	phone_n = jQuery('#af-blackberry_bubble input[name="p1"]').val().replace(/-/g, '');
	if(phone_n == 5555555555 || isNaN(phone_n) || phone_n.length != 10) {
		jQuery('#af-blackberry_bubble .s2subtitle').fadeOut(500, function () {
			jQuery('#af-blackberry_bubble .s2subtitle').html('Please use a valid 10-digit mobile number.').css('color', '#FF0000').fadeIn(500);
		});
		return false;
	}
	jQuery('#af-blackberry_bubble .s2title').html('Sending...');
	var appid = '26';
	var appkey = 'ao1qvHJSh3Z7hgQQ';
	var phone = phone_n;
	var message = 'Huffington Post for Blackberry';
	var link = 'http://appworld.blackberry.com/webstore/content/19143';
	var url = 'http://s2c.aol.com/s2c/sendtocell.jsp?appid='+encodeURIComponent(appid)+'&appkey='+encodeURIComponent(appkey)+'&phone='+encodeURIComponent(phone)+'&message='+encodeURIComponent(message)+'&link='+encodeURIComponent(link);
	console.log(url);
	jQuery.ajax({
		url: url,
		dataType: "jsonp",
		crossDomain: true,
		timeout: 15 * 1000,
		error: errorHandle
	});
	return false;
});
jQuery('#af-blackberry_bubble .closable').live('click', function(){
	jQuery('#af-blackberry_bubble').parent().addClass('hidden');
});
</script>
