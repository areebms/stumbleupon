        var counter = 0;
        Array.prototype.in_array = function (p_val) {
            for (var j = 0, l = this.length; j < l; j++) {
                if (this[j] == p_val) {
                    return true;
                }
            }
            return false;
        }


        function play(index) {
            index = typeof(index) != 'undefined' ? index : window.player_index;
            index = typeof(players[index]) != 'undefined' ? index : 0;
            window.player_index = index;

            if (players[index].player == 'Grab_Networks' || players[index].player == 'RG') {
                clearRecipeContainers();
            }

            var timeout = players[index].timeout;

            console.log('play - ' + players[index].player + '; video index - ' + index + '; timeout - ' + timeout);

            if (/document\.write/ig.test(players[index].playerHTML)) {
                if (typeof(IE) == 'undefined') var IE = /*@cc_on!@*/false;
                if (!IE) jQuery.writeCapture.forceLastScriptTag = true;
                jQuery('#player-companion').writeCapture().html(players[index].playerHTML);
            }
            else
                jQuery('#player-companion').html(players[index].playerHTML);

            if (
                players[index].player == '5_Min' ||
                    players[index].player == '5_Min_2' ||
                    players[index].player == 'RG' ||
                    players[index].player == 'RG_New2') {

                loadRecipe(players[index].video);
                loadCompanionAd(5);
            }

            if ((
                players[index].player == '5_Min' ||
                    players[index].player == '5_Min_2' ||
                    players[index].player == 'RG' ||
                    players[index].player == 'RG_New2' ||
                    players[index].player == 'Grab_Networks'
                ) && timeout != 'stop'
                ) {
                window.player_index = typeof(players[index + 1]) == 'undefined' ? 0 : index + 1;
                setTimeout(function () {
                    play(window.player_index);
                }, timeout * 1000);
            }
        }

        function beginVideoHook(clip) {
            loadCompanionAd(4);
            if (player_videos.in_array(clip.url)) {
                loadRecipe(clip.url);
            }
        }

        function finishVideoHook(clip) {
            //console.log('i-'+window.player_index);
            if (player_videos.in_array(clip.url)) {
                //console.log(players[window.player_index+1]);
                var timeout = players[window.player_index].timeout;
                //timeout = 10;
                if (timeout != 'stop') {
                    window.player_index = typeof(players[window.player_index + 1]) == 'undefined' ? 0 : window.player_index + 1;

                    setTimeout(function () {
                        play(window.player_index);
                    }, timeout * 1000);
                }
            }
        }

        function clearRecipeContainers() {
                            jQuery('h1.page-heading').html('');
                jQuery('#tabcontentcontainer div.tabcontent1').html('');
                        }

        function loadRecipe(video) {
                            jQuery.ajax({
                    url:'http://www.gourmandia.de/advertisement/includes/api.php?v=' + video,
                    success:function (data) {
                        data = data.split('|||');

                        jQuery('h1.page-heading').html(data[0]);

                        jQuery('#tabcontentcontainer div.tabcontent1').html(data[1]);
                    }
                });
                        }

        function loadScriptToHead(sScriptSrc, oCallback) {
            var oHead = document.getElementsByTagName('head')[0];
            var oScript = document.createElement('script');
            var bIsExternal = /^http/i.test(sScriptSrc);

            oScript.type = 'text/javascript';

            if(bIsExternal) {
                oScript.src = sScriptSrc;
            } else {

                if(/*@cc_on!@*/false)
                    oScript.text = sScriptSrc;
                else {
                    var oScriptNode = document.createTextNode(sScriptSrc);
                    oScript.appendChild(oScriptNode)
                }
            }

            if(oCallback) {
                if(bIsExternal) {
                    // most browsers
                    oScript.onload = oCallback;
                    // IE 6 & 7
                    oScript.onreadystatechange = function() {
                        if (this.readyState == 'complete'||this.readyState == 'loaded') {
                            oCallback();
                        }
                    }
                } else {
                    if(!bIsExternal) oHead.appendChild(oScript);
                    oCallback();
                }
            }
            if(bIsExternal) oHead.appendChild(oScript);
        }

        function insertCompanion(c) {
                            if (typeof(IE) == 'undefined') var IE = /*@cc_on!@*/false;
                if (typeof(console) != 'undefined' && !jQuery.writeCapture) console.log('writeCapture is not defined, waiting...');
                if (!jQuery.writeCapture && counter < 5) {
                    counter++;
                    setTimeout(function () {
                        insertCompanion(c);
                        return;
                    }, 10000);
                    return;
                } else if(counter >= 5) {
                    loadScriptToHead('http://cdn.gourmandia.com/js/write_capture/writeCapture'+(IE||window.opera?'':'__e')+'.js', function(){
                        loadScriptToHead('http://cdn.gourmandia.com/js/write_capture/jquery.writeCapture.js');
                    });
                }

                if (!IE) {
                    jQuery.writeCapture.forceLastScriptTag = true;
                }
                var banner = '', time = 120, next, url = 'http://www.gourmandia.de/advertisement/includes/api.php?z=300x250_ATF';
                if (typeof(c) != 'undefined') url += '&c=' + c;
                jQuery.ajax({
                    url:url,
                    cache:false,
                    success:function (data) {
                        if (data) {
                            data = data.split('|||');
                            banner = data[0];

                            data = data[1].split(',');

                            time = data[0];
                            next = data[1];
                            var ind = next;
                            if (time != 0 && jQuery.trim(banner)) {
                                jQuery('#lcBannerDiv').css('position', 'relative');
                                if (typeof(console) != 'undefined') console.log('Default banner is set');
                                jQuery('#lcBannerDiv').writeCapture().html(banner);
                                if (time != 'stop') {
                                    setTimeout(function () {
                                        insertCompanion(ind);
                                    }, time * 1000);
                                }
                            } else if (time != 'stop') {
                                insertCompanion(ind);
                            }
                        }
                    }
                });
                        }


        function loadCompanionAd(time) {
            if(!jQuery('#lcBannerDiv').size()) return;
            time = time ? time : 10;
            setTimeout(function () {
                var banner = jQuery('#lcBannerDiv').html();
                banner = jQuery.trim(banner);
                if (typeof(console) != 'undefined') console.log('in companion div:' + banner);
                if (!banner ||
                    banner == '<div id="video_companion"></div>' ||
                    banner == '<div id="adaptvcompanion"></div>' ||
                    banner == '<div id="adaptvcompanion" style="display: inline; position: absolute; margin-left: 10px; margin-top: 12px; width: 300px; height: 250px;"></div>' ||
                    /ads_\d{2,3}[^\d]\d{2,3}/i.test(banner) // ||
                    // /<div([^>]*)?>(\s*)?<\/div>/i.test(banner)
                    ) { // check and put default banner in case if no advertiser's companion
                    if (typeof(console) != 'undefined') console.log('companion test passed');
                    insertCompanion();
                }
            }, time * 1000); // Wait 'time' seconds for advertiser's companion, then set default
        }


        /* ]]> */
    </script>
    
