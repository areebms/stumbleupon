<script type="text/javascript">
	var o = {};
	
	// Required parameters
	o.instance = 'tpLatestVideoPlaylist';
	o.containerId = 'tpLatestVideoClips';
	
	o.categoryTitle = 'News/Canada';
	o.endIndex = '6';
	o.itemTemplate = tpItemTemplate;
	
	var isApple = (navigator.userAgent.match(/iPad/i) != null || navigator.userAgent.match(/iPhone/i) != null || navigator.userAgent.match(/iPod/i) != null);
	
// if user agent is an apple product
	if (isApple) {
		var callback = {
			success: function(response){
				document.getElementById(response.argument.containerId).innerHTML = response.responseText;
			},
			failure: function(response){
				//alert('FAIL');
			},
			argument: o
		};
		YAHOO.util.Connect.asyncRequest('GET', '(none)', callback);
	}
// if user agent is NOT an apple product
	else {
		CBC.register({
			_name: 'tpLatestVideoPlaylist',
			_class: ThePlatformPlaylist,
			_nameSpace: CBC.APP,
			_params: o
		});
	}

var audioCarousel_tpl = "<li><div class='promo med'><a class='clearfix' onclick='#{playVideoCommand}' href='#{uberHref}'><img src='#{thumbnailURL}' border='0' height='80' width='140' alt='' style='padding:0;margin:0;' />#[liveState: {not:<span class='flag fhidden'>Live</span>||now:<span class='flag fhidden'>Live</span>||later:<span class='flag fhidden'>Live</span>}]</span><strong class='headline'>#{title}</strong><span class='playIcon'></span></a></div></li>";
//var audioCarousel_tpl = "<li>'#{playVideoCommand}'</li>";

function myCreatePlayAction ( o ){
	if( o ) {
		if( o.hasOwnProperty( "category" ) && o.hasOwnProperty( "ID" ) ) {
			o.uberHref =  "/video/#/Radio/" + encodeURI(o.category) + "/ID=" + o.ID;
		}
	}
}
</script>





















<script type="text/javascript">
	var o = {};
	
	// Required parameters
	o.instance = 'tpPlaylist1';
	o.containerId = 'tpLatestAudioClips';
	
	// Either/or required parameters - specify either the title or the ID
	o.categoryTitle = "Radio News";
	
	
	// Optional parameters
	
		
	
	
	o.endIndex = "10";
	
	
	
	
	
	
	o.createPlayAction = myCreatePlayAction;
	
	// Set the Audio Template Parameter
	o.itemTemplate = '<a class="playlistItem clearfix" onclick="#{playVideoCommand}" href="#{uberHref}" target="_blank" title="#{title}"><span class="meta"><span class="title">#{title}</span><span class="desc">#{description}</span></span><span class="cta">Listen #[liveState: {not:<em class="dur">#{dur}</em>||now:<em class="livenow">Live Now</em>||later:<em class="livelater">Live #{liveDate} at #{liveTime}</em>}] </span></a>';
	o.itemTemplate = audioCarousel_tpl; 
	
	var tpPlaylist1 = new CBC.APP.AudioPlaylist(o);
	
