<style type="text/css">
.tweet_result {
	border-bottom:dotted 1px #000;
	padding:4px;
	font-family:arial,helvetica,san-serif;	
	font-size:12px;
	min-height:35px;
}

.tweet_result img {
	width:32px;
	height:32px;
	margin:2px 4px 2px 2px;
}

</style>
<script>

	function load_tweets(keywords) {
		//var search_query = '"' + keywords.split(',').join('" OR "') + '"';
		var search_query = '"' + keywords.replace(/^\s+|\s+$/g,"").split(/\s*,\s*/).join('" OR "') + '"';
		var censor = ['-shit', '-fuck', '-piss', '-cunt', '-motherfucker', '-cocksucker', '-tits'];
		search_query += " OR theweek.com"+document.location.pathname+" AND " + censor.join(" AND ");
		jQuery("#debug-text").val(search_query);
		return execute_search(search_query);
	}

	function execute_search(search_query) {
		var last_ID = jQuery(".tweet_result:first").attr("id");
		var url = "http://search.twitter.com/search.json?q="+encodeURIComponent(search_query);
		

	    var num_posts = 5; // number of posts to get
		if (last_ID) {
			last_ID = last_ID.replace("tweet", "");
			url += "&since_id=" + last_ID;
			num_posts = 3;
	     } 
		url += "&lang=en&rpp="+num_posts+"&callback=?";
     
		jQuery.getJSON(url, function(json) { 
			var results = '';
			jQuery(json.results).each(function() {
				if (this.id == undefined || this.id==last_ID) return;
				var divid = 'tweet' + this.id;
				results += "<div class='tweet_result' style='display:none' id='" + divid + "'>"+
					"<a href='http://twitter.com/" + this.from_user + "' class='tweet_user'>"+
					"<img alt='" + this.from_user + " on Twitter' src='" + 
					this.profile_image_url + "' align='left' valign='top'/></a>" + linkify(this.text) + "</div>";
				if (last_ID) {
					jQuery("#twitter_results").prepend(results);
				} else {
					jQuery("#twitter_results").append(results);
				}
				jQuery("#"+divid).fadeIn();
			});
		});

		jQuery.each(jQuery(".tweet_result:gt(10)"), function(key, val) {
			jQuery(this).remove();
		});
}

function linkify(text){
    if (text) {
        text = text.replace(
            /((https?\:\/\/)|(www\.))(\S+)(\w{2,4})(:[0-9]+)?(\/|\/([\w#!:.?+=&%@!\-\/]))?/gi,
            function(url){
                var full_url = url;
                if (!full_url.match('^https?:\/\/')) {
                    full_url = 'http://' + full_url;
                }
                return '<a href="' + full_url + '">' + url + '</a>';
            }
        );
        text = text.replace(
            /@+[a-z0-9-_]*/gi,
            function(user){
                var twit_url = "http://twitter.com/"+user;
                return '<a href="' + twit_url + '">' + user + '</a>';
            }
        );
        text = text.replace(
            /#+[a-z0-9_-]*/gi,
            function(hash){
                var hash_url = "http://twitter.com/#!/search/"+hash.replace('#', '%23');
                return '<a href="' + hash_url + '">' + hash + '</a>';
            }
        );

    }
    return text;
}

jQuery(function() {
	var keywords = jQuery('meta[name=keywords]').attr("content"); 
     load_tweets(keywords);
     setInterval(function() { 
    	 load_tweets(keywords);
	}, 30000);

    	jQuery("#debug-button").click(function() {
			var q = jQuery("#debug-text").val();
			jQuery(".tweet_result").remove();
			execute_search(q);
        });
});
</script>

