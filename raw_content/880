<script type="text/javascript">

// set up entire page refresh

var refresh_time = 10000000;
var refresh_timer = setTimeout("", refresh_time);
				
var sectionurl = "science_tech";
var oasPath = "science_tech/technology";

// now look to do page hit and ad rotation on this page.  Only need to do if rotationPeriod Set
        window.setTimeout(OAS_rotate_all_ads_first, 300000);


var rotationCount = 0;

function OAS_rotate_all_ads_first() {OAS_rotate_all_ads(true); };
function OAS_rotate_all_ads_nth() {OAS_rotate_all_ads(false); };

function OAS_rotate_all_ads(first) {
		// drop a click
				
        // rotate ads as needed
		OAS_switch_ad("Top", first);OAS_switch_ad("Right", first);        //if (rotationCount < 3) {
        //        positions.push('Top');
        //        OAS_switch_ad('Top', first);
        //}
		
        new Ajax.Request('/section/updateImpressions/CLICK-'+oasPath.replace(/\//i, '-'),
                                        {asynchronous:true, evalScripts:true})

        if (rotationCount >= 3) return;
        rotationCount++;

        window.setTimeout(OAS_rotate_all_ads_nth, 300000);
}


function OAS_switch_ad(pos, first) {
    OAS_rn = new String (Math.random()); OAS_rns = "1"+ OAS_rn.substring(2, 11);
    OAS_sitepage="theweek.com/"+oasPath;
    var newAdUrl = "http://theweek.com/RealMedia/ads/adstream_sx.ads/"+OAS_sitepage+"/"+OAS_rns+"@"+pos;
//alert(newAdUrl);
	
    var tag, divclass;
	
		if (pos == 'Right') {
			tag = "square";
			divclass = 'boxunit ' + (('science_tech/technology'=='/home')?'hpModule':'col300');
			params = "width='300' height='250'";
		}
		else if (pos == 'Top') {
			tag = "leaderboard";
			divclass = 'leaderboard';
			params = "width='728' height='90'";
		}
		else {
			tag = "tower";
			divclass = 'col160';
			params = "width='160' height='600'";
		}
		var newAdSite = tag + "_main_iframe_anchor";
		var newAdSiteDiv = tag + "_main_div_anchor";
        if (first) {
			var newhtml = "<div class='"+divclass+"'><iframe id='"+newAdSite+"' scrolling='no' frameborder='no' marginheight='0' marginwidth='0' "+ params +" allowtransparency='true'></iframe></div>";
			   jQuery("#"+newAdSiteDiv).replaceWith(newhtml);
        }
            jQuery('#'+newAdSite).attr('src', newAdUrl);
}


</script>
