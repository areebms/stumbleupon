<script>

var publish_controls_fixed = false;

jQuery(document).ready(function() {
    var orig_sidebar_top = jQuery('#sidebar_follower .shareholder').position().top;

    jQuery(window).scroll( function() {
        var viewportHeight = parseInt( jQuery( window ).height() );  
        var pageScroll =  parseInt( jQuery(document).scrollTop() );
        var global_wrapper_top = jQuery('#socialAnchorTop').position().top;
        
        share_button_positioning();

        if ( pageScroll >= (jQuery('#partners').position().top + 310) ) {
			//Floater hits bottom
            if (publish_controls_fixed) {

                jQuery('#sidebar_follower').css({
                    position: 'absolute'
                    ,top: '0'
                });

 				jQuery('#sidebar_follower .shareholder').css({
					top: parseInt(jQuery('#partners').position().top + 310) + 'px'
				});
 
                publish_controls_fixed = false;
            }

        } else if (pageScroll > (jQuery('#socialAnchorTop').position().top + 20)  ) {
			//Floater hits the top
            jQuery('#sidebar_follower').css({
                position: 'fixed'
                ,top: '0'
            });
            
            jQuery('#sidebar_follower .shareholder').css({
				top: '20px'
			});

            publish_controls_fixed = true;

        } else {
			//Floater is in the middle. Leave as is or force to '0' if fixed
            if (publish_controls_fixed) {

                jQuery('#sidebar_follower').css({
                    position: 'absolute',
                    top: '0'
                });

                jQuery('#sidebar_follower .shareholder').css({
                    top: orig_sidebar_top + 'px'
                });

                publish_controls_fixed = false;
            }

        }
    });

    window_resizing();

    // window resizing
    jQuery(window).resize(function() {
        window_resizing();
    });

    // share button positioning
    jQuery('#sharebutton').click(function(){
        share_button_positioning();
    });

});

/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */

function window_resizing() {

    if ( jQuery( window ).width() < 1165 ) {
        jQuery('#sidebar_follower').hide();
        if ((window.location.pathname.indexOf("article") == -1) && (window.location.pathname.indexOf("cartoon") == -1) && (window.location.pathname.indexOf("bullpen") == -1)) {
        	//non-article page
        	jQuery('#socialtop').show();
        } else {
        	//article page
        	jQuery('#socialarticletop').show();
        	jQuery('#st_email').show();
        }	
    } else {
        jQuery('#sidebar_follower').show();
        if ((window.location.pathname.indexOf("article") == -1) && (window.location.pathname.indexOf("cartoon") == -1) && (window.location.pathname.indexOf("bullpen") == -1)) {
        	//non-article page
        	jQuery('#socialtop').show();
        } else {
        	//article page
        	jQuery('#socialarticletop').hide();
        	jQuery('#st_email').hide();
        }	
    }
}

/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */

function share_button_positioning() {
    if (publish_controls_fixed) {
        jQuery('#stwrapper').css({
            'top' : parseInt( jQuery('#sidebar_follower').offset().top + 190 ) + 'px'
        });
    }
}

/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
</script>

