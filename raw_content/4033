	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js" type="text/javascript"></script>	<script type="text/javascript" src="http://travel.justluxe.com/includes/js/jquery-ui-1.8.12.custom.min.js"></script>
