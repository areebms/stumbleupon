<script>	   
$(document).ready(function(){
	$(".menu > li").click(function(e){
		switch(e.target.id){
			case "nationalpop":
				//change status & style menu
				$("#nationalpop").addClass("active");
				$("#localpop").removeClass("active");
				//display selected division, hide others
				$("div.nationalpop").fadeIn();
				$("div.localpop").hide();
			break;
			case "localpop":
				//change status & style menu
				$("#nationalpop").removeClass("active");
				$("#localpop").addClass("active");
				//display selected division, hide others
				$("div.localpop").fadeIn();
				$("div.nationalpop").hide();
			break;
		}
		return false;
	});
	

	$("#pop_edition_change").change(function () {
	 	$('div[class^=most_popular_]').hide();
		$('div.most_popular_'+ $(this).val()).fadeIn();
	})
	.change();
});
</script>
