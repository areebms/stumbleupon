<script>	   
function clearInput()
	{
		var input = document.getElementById("searchTerm");
		if (input.value == "Search")
		{			
			input.value = ""; 
		}
	}
function changeSearchSidebar(type)
	{
		if(type == 'general')
		{
			$('#tabbedNav-search').removeClass("recipes-bar");
			$('#tabbedNav-search').addClass("general-bar");
			$('#tabbedNav-search-rec').removeClass("recipes");
			$('#tabbedNav-search-gen').addClass("general");		
			$('#tabbedNav-search-gen').css("color", "white");	
			$('#tabbedNav-search-rec').css("color", "black");	
			$("#command").attr("action", "/header/chefs_recipes/search.htm");			
			$("#searchTerm").attr("name", "searchTerm");	
			$('input[name="returnCurrentEditionFirst"]').attr("disabled", false);
			$('input[name="exactMatch"]').attr("disabled", false);
			$('input[name="filterEditionId"]').attr("disabled", false);			
		}
		
		if(type == 'recipe')
		{
			$('#tabbedNav-search').removeClass("general-bar");
			$('#tabbedNav-search').addClass("recipes-bar");		
			$('#tabbedNav-search-gen').removeClass("general");
			$('#tabbedNav-search-rec').addClass("recipes");	
			$('#tabbedNav-search-gen').css("color", "black");
			$('#tabbedNav-search-rec').css("color", "white");
			$("#command").attr("action", "http://recipes.tastingtable.com");
			$("#searchTerm").attr("name", "st");
			$('input[name="returnCurrentEditionFirst"]').attr("disabled", true);
			$('input[name="exactMatch"]').attr("disabled", true);
			$('input[name="filterEditionId"]').attr("disabled", true);
		}
		
		if(type == 'drink')
		{
			$('#tabbedNav-search').removeClass("general-bar");
			$('#tabbedNav-search').addClass("recipes-bar");		
			$('#tabbedNav-search-gen').removeClass("general");
			$('#tabbedNav-search-rec').removeClass("recipes");	
			$('#tabbedNav-search-dri').addClass("drinks-search");	
			$('#tabbedNav-search-gen').css("color", "black");
			$('#tabbedNav-search-rec').css("color", "white");
			$("#command").attr("action", "http://recipes.tastingtable.com");
			$("#searchTerm").attr("name", "st");
			$('input[name="returnCurrentEditionFirst"]').attr("disabled", true);
			$('input[name="exactMatch"]').attr("disabled", true);
			$('input[name="filterEditionId"]').attr("disabled", true);
		}
	}
</script>
