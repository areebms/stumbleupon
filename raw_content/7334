                    $(function() {

                        $("#button-share73572").click(function() {
                            $("#menu-share73572").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share73572").data("hidden", false);
                        });
                        $("#menu-share73572").mouseenter(function() {
                            $("#menu-share73572").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share73572").data("hidden", false);
                        });
                        $("#button-share73572, #menu-share73572").mouseleave(function() {
                            $(this).data("in", false);
                            setTimeout(hideMenu, delay);
                        })

                        var delay = 400;
                        var hideMenu = function() {
                            if (!$("#button-share73572").data("in") && !$("#menu-share73572").data("in") && !$("#menu-share73572").data("hidden")) {
                                $("#menu-share73572").fadeOut("fast");
                                $("#menu-share73572").data("hidden", true);
                            }
                        }

                        $("#menu-share73572 a").click(function() {
                            $("#menu-share73572").hide();
                        });
                    });
                </script>
                
