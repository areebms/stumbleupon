                <script type="text/javascript">
                    $(function() {

                        $("#button-share297134").click(function() {
                            $("#menu-share297134").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share297134").data("hidden", false);
                        });
                        $("#menu-share297134").mouseenter(function() {
                            $("#menu-share297134").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share297134").data("hidden", false);
                        });
                        $("#button-share297134, #menu-share297134").mouseleave(function() {
                            $(this).data("in", false);
                            setTimeout(hideMenu, delay);
                        })

                        var delay = 400;
                        var hideMenu = function() {
                            if (!$("#button-share297134").data("in") && !$("#menu-share297134").data("in") && !$("#menu-share297134").data("hidden")) {
                                $("#menu-share297134").fadeOut("fast");
                                $("#menu-share297134").data("hidden", true);
                            }
                        }

                        $("#menu-share297134 a").click(function() {
                            $("#menu-share297134").hide();
                        });
                    });
                </script>
                
