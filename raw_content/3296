<script type="text/javascript" charset="utf-8">
jQuery(function($){
var last_updated = 1339791734000,
dr_reloader = setInterval(function(){
var drfront = $("div.drfront");
$.ajax({
url: "http://ekstrabladet.dk/helium/direct/article1320109.ece",
type: "GET",
data: { last_updated: last_updated },
dataType: "json",
success: function( data ){
// only check stuff if it is new
if( !data.update ) return;
if( data.parts.length < drfront.length ){
clearInterval( dr_reloader );
return;
}
last_updated = data.last_updated;
drfront.each(function(i,e){
$(e).replaceWith(data.parts[i]);
});
},
error: function(){
clearInterval( dr_reloader );
}
});
}, 300000);
});
</script>
<script type="text/javascript">
var Seed = (function() {
var cb = "201205011246",
instances = [],
loaded = {};
function Seed(provider, account, base) {
this.provider = provider;
this.account = account;
this.base = base;
this.queue = [];
instances.push(this);
}
Seed.prototype.spawn = function(name, config){
this.queue.push([name, config]);
};
Seed.load = function(source) {
var o, s;
if (!source || loaded[source]) return;
o = document.createElement("script");
o.type = "text/javascript";
o.charset = "utf-8";
o.async = true;
o.src = source + "?cb=" + cb;
s = document.getElementsByTagName("script")[0];
s.parentNode.insertBefore(o, s);
loaded[source] = true;
};
Seed.loaded = function(Orange) {
var o;
while (o = instances.pop()) {
new Orange(o);
}
};
return Seed;
})();
window.Seed = Seed;
var ebdk = new Seed( "http://orange.ekstrabladet.dk/xd/provider", "ebdk", "http://orange.ekstrabladet.dk");
</script>
