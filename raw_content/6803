<script type="text/javascript" src="/scripts/jquery.cookie.js"></script>
<script type="text/javascript">
  $().ready(function() {
    jQuery('#close-stay-connected-float').click(hidefloatingfooter);
    var floatingfootercookie = $.cookie('saveurfloatingfooter');
    var override = "";
    //alert("floatingfootercookie:"+floatingfootercookie);
    if (floatingfootercookie == null || override.length > 0) {
      showfloatingfooter();
    }
  });
  function hidefloatingfooter() {
    $.cookie('saveurfloatingfooter',"closed",{expires: 3, path: '/'});
    footercloseit("stay_connected_float");
  }
  function showfloatingfooter() {
    footershowit("stay_connected_float");
    $('#scroll-box').hide();
  }
  function footercloseit(closethis) {
    var item = document.getElementById(closethis);
    //alert("saveurfloatingfooter-cookie-set");
    item.style.display = "none";
    $('#scroll-box').show();
  }
  function footershowit(closethis) {
    var item = document.getElementById(closethis);
    item.style.display = "block";
  }
</script>
