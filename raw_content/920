$(document).ready(function(){
	$('#subscribeemail').focus(function(){
		$(this).css('text-align','left');
	});
	$('#subscribeemail').blur(function(){
		
		if ($(this).val() == 'enter e-mail'){$(this).css('text-align','center');}
	});
	$('#sidebarSubscribe').submit(function(){
		var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	   var address = $('#subscribeemail').val();
	   if (address == 'enter e-mail' || address == ''){
		  alert('Oops! It looks like we didn\'t get a valid email address. Please try again!');
		  return false;   
	   }
	   if(reg.test(address) == false) {
	 
		  alert('Oops! It looks like we didn\'t get a valid email address. Please try again!');
		  return false;
	   }
	  
	});
});
