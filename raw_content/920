	<script type="text/javascript">
	
	$(function() {
		$('.iSearch').focus(function() {
			$(this).css({'text-align':'left'});
		});
		
		$('.iSearch').blur(function() {
			if($(this).val() == 'SEARCH') {
				$(this).css({'text-align':'center'});
			}
		});
	});
	
	</script>
