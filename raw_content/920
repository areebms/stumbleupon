<script type="text/javascript">
// Handles the outer sidebar positioning and size to fit the current viewport
// without generating scrollbars.
// This makes the sidebar ad embed code far simpler and less error prone
$(function(){
	var $dsb = [$('#dynamicSidebarLeft'), $('#dynamicSidebarRight')],
		cw = $('div.dBody').outerWidth(true);

	$(window).resize(function() {
		var sbarW = (($(window).width() - cw) / 2) + 12;
		$dsb[0].width(sbarW).height($('div.dBody').height());
		$dsb[1].width(sbarW).height($('div.dBody').height());
	}).resize();

	$('#subscribeemail1').focus(function() {
		$(this).css('text-align','left');
	}).blur(function() {
		if($(this).val() == 'enter e-mail'){
			$(this).css('text-align','center');
		}
	});
	$('#sidebarSubscribe1').submit(function(){
		var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	   var address = $('#subscribeemail1').val();
	   if (address == 'enter e-mail' || address == ''){
		  alert('Oops! It looks like we didn\'t get a valid email address. Please try again!');
		  return false;   
	   }
	   if(reg.test(address) == false) {
	 
		  alert('Please Invalid Email Address');
		  return false;
	   }
		
	});
});
</script>

