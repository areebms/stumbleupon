        // regex: ^/[.]*
        // page_taxonomy: news.nationalgeographic.com::news::Ancient World
        // ad_taxonomy: None
        // taxonomy_match: True
            var only;
            if (typeof only == 'undefined'){only=1;dc=";dcopt=ist";}
            else {only+=1;dc="";}

