<style type="text/css">
    .subsection > div.primary > div.ecommerce-section,
    .subsection > div.secondary > div.ecommerce-section {
        margin-top: 0px;
        border-top: 0px none;
        padding-top: 0px;
    }
</style>

<script type="text/javascript">
    function ecommerce_43331(){
	
	    var o = this;
		o.defaultItems = [],
	    o.totItems = 0,
	    o.isSearchPage =  false,
        o.searchComplete = false,
        o.pageComplete = false,
        o.isFallback = false,
        o.byID = false,
        o.clickTrack = 'NG2D7710016',
        o.pageKeywords = 'Cleopatra&#39;s Eye Makeup Warded Off Infections?',
        o.maxItems = 2,
        o.imageSize,
        o.byID = false 
        
	    o.parseResults = function(data, textStatus, xhr) {
	        var html;
			// take results an make each item into an li
            // for each item totItems++;
            if(data.response.numFound == 0){
                if(!o.isFallback&&!o.byID)o.getFallbackItems();
            } else {
                for(i in data.response.docs) {
				    if(o.totItems == o.maxItems)break;
				    
				    var doc = data.response.docs[i];
				    var sourceCode = (doc._link_t.indexOf('?') > -1) ? '&source='+o.clickTrack : '?source='+o.clickTrack;
					
					
					
					
					    
					if(o.totItems == 0){    
    				    html = '<li class="first">';        
    				} else if(o.totItems == o.maxItems-1){
					    html = '<li class="last">';// add last class.
				    } else {
				        html = '<li>';// no class
				    }// end if totItems 0
			    
				    html += '<a class="teaser-img-thumb-all" href="'+ doc._link_t + sourceCode +'" title="'+ doc._title_t  +'">';
                	html += '<img src="'+ doc["media_thumbnail@_url_t"] +'" alt="'+ doc["media_thumbnail@_url_t"] +'" width="'+o.imageSize+'" height="'+o.imageSize+'"></a>';
                	html += '<div class="description">';
                	html += '<a href="'+ doc._link_t + sourceCode +'" title="'+doc._title_t +'" >'+ doc._title_t  +'</a>';
                	if(doc.ngstore_onsale_b){
                        html += '<div class="clear"></div><span class="item-price-slashed">&nbsp;$'+ doc.ngstore_price_t +'</span>';
                	    html += '&nbsp;<span class="sale-price">$'+ doc.ngstore_saleprice_t +'</span>';
            	    } else {
            	        html += '<div class="clear"></div><span class="item-price">&nbsp;$'+ doc.ngstore_price_t +'</span>';
            	    }
                	html += '</div></li>';
			                
				    $("#ecom_43331 ul.ecommerce_all_img").append(html);
    				    
    				
				
					
				    o.totItems++;
					
				}// end for loop
			} // end if data.response.numFound != 0
			
			if(o.totItems != o.maxItems){
			    if(o.defaultItems.length > 0){
				    o.getItemByID(o.defaultItems.shift());
			    } else if(o.isSearchPage && !o.searchComplete){
			        o.doSearchPage();
			    } else if(!o.searchComplete) {
			        o.byID = false;
			        o.doSearch();
			    }
		    }// end if
		}// end parseResults function
	    
	    o.trim = function(str) {
			return str.replace(/^\s\s*/, '').replace(/\s\s*$/, '');
		}
        
	    o.doSearchPage = function(){
	        o.byID = false;
            
	        var tempSearch = window.location.search;
            var searchTerms ="default";
            var temp;
            
            if( tempSearch.substr(0,7) == "?search"){
                temp = tempSearch.substr(7).split("&");
                searchTerms = temp[0];
            } else {
                temp = tempSearch.split("&");
                for(var j=0;j < temp.length;j++){
                    if(temp[j].substr(0,7) == "search="){
                        searchTerms = temp[j].substr(7);
                        break;
                    }
                }
            }
            
            var params = {
				"q": "media_keywords_t:(" + o.trim(searchTerms).replace(/\++/g, ' OR ') + ')',
				"fq": "ngstore_source_t:ngstore",
				"wt": "json",
				"sort": "ngstore_priority_i desc"
			};
			
			jQuery.ajax({
				url: 'http://api.ngeo.com/solr/',
				cache: false,
				data: params,
				success: o.parseResults,
				dataType: 'jsonp',
				jsonp: 'json.wrf'
			});
	        
	        o.searchComplete = true;
	    }// end doSearchPage function
	    
	    o.doSearch = function() {
	        o.byID = false;
            
	        // use keywords up to first comma and replace the spaces with OR.
			o.pageKeywords = o.pageKeywords.split(',');
			o.pageKeywords = o.pageKeywords[0].split(' ');
			o.pageKeywords = o.pageKeywords[0];
			
			var params = {
				"q": "media_keywords_t:(" + o.trim(o.pageKeywords).replace(/\s+/g, ' OR ') + ')',
				"fq": "ngstore_source_t:ngstore",
				"wt": "json",
				"sort": "ngstore_priority_i desc"
			};
			
			jQuery.ajax({
				url: 'http://api.ngeo.com/solr/',
				cache: false,
				data: params,
				success: o.parseResults,
				dataType: 'jsonp',
				jsonp: 'json.wrf'
			});
			
			o.searchComplete = true;
		}// end doSearch function
	    o.getFallbackItems = function() {
	        var params = {
				"q": "media_keywords_t:(default)",
				"fq": "ngstore_source_t:ngstore",
				"wt": "json",
				"sort": "ngstore_priority_i desc"
			};
			
			jQuery.ajax({
				url: 'http://api.ngeo.com/solr/',
				cache: false,
				data: params,
				success: o.parseResults,
				dataType: 'jsonp',
				jsonp: 'json.wrf'
			});
			
			o.isFallback = true;
	    }
		o.getItemByID = function(id){
            var params = {
				"q": 'ngstore_slug_i:' + id,
				"fq": "ngstore_source_t:ngstore",
				"wt": "json"
			};
            
			jQuery.ajax({
				//remember to change the url from stage!
				url: 'http://api.ngeo.com/solr/',
				cache: false,
				data: params,
				success: o.parseResults,
				dataType: 'jsonp',
				jsonp: 'json.wrf'
			});
            
            o.byID = true;
            
		}// end getItemByID function
		
		o.addDefaultItem = function(id){
		    o.defaultItems.push(id);
		}// end addDefaultItem function
		
		o.init = function(){
		    o.imageSize = 75;
	        
	        if(o.defaultItems.length > 0){
	            o.getItemByID(o.defaultItems.shift());
	        } else if(o.isSearchPage){
	            o.doSearchPage();
			} else {
			    o.doSearch();
			}
			
	    }// end init function
	    
    }// end ecommerce object
    
    var store_43331 = new ecommerce_43331();
    
	
	
	
	
	store_43331.init();
	
</script>
<div class="ecommerce-section Primary">
	<div class="promo_collection" id="ecom_43331">
	    <h3 class="title">Shop National Geographic </h3>
    	 
    	
    	
    	    
        	    <ul class="ecommerce_all_img"></ul>
        	    


	
	<p class="action"><a href="http://shop.nationalgeographic.com/ngs/index.jsp?code=NG2D7710016">SHOP NOW &raquo;</a></p>
	


        	
    	
