		var _l = t.toLocaleTimeString().split(' ');
		var _t = _l[0].split(':');
		if(_l.length == 1) {
			//_l is in 24:00 format, convert to 12:00 am/pm
			var _temp_int = parseInt(_t[0].replace(/^0*(?=[0-9]$)/, ''));
			var _temp_hrs = (_temp_int > 11 ? _temp_int - 12 : _temp_int);
			if(_temp_hrs == 0) _temp_hrs = 12;
			_t[0] = _temp_hrs.toString();
			_l[1] = (_temp_int > 11 ? 'PM' : 'AM');
		}
		var _time = _t[0] + ':' + _t[1] + ' ' + _l[1];
		var _m = t.toString().split(' ')[1];
		return _time+', '+_m+'. '+t.getDate()+', '+t.getFullYear();
	})(_tt);
	_a.year = parseInt(_tt.getFullYear());
	_a.month = parseInt(_tt.getMonth()+1);
	_a.day = parseInt(_tt.getDate());
	_a.hours = parseInt(_tt.getHours());
