  function load_article(sortby,pg,load_id)
  {
  		 $('#article').html('<p><img src="/images/ajax-loader.gif" width="120" height="19"/></p>');
  		$.get("includes/artilce_rightcol.php?pg="+pg+"&sortby="+sortby+"&user="+load_id, function(data){
		if(data!="")
		{
			
			document.getElementById("article").innerHTML=data;
		}
	});
  }
  function load_training(sortby,pg,load_id)
  {
  		$('#training').html('<p><img src="/images/ajax-loader.gif" width="120" height="19"/></p>');
  		$.get("includes/artilce_rightcol.php?pg="+pg+"&sortby="+sortby+"&user="+load_id, function(data){
		if(data!="")
		{
			
			document.getElementById("training").innerHTML=data;
		}
	});
  }
  function load_products(sortby,pg,load_id)
  {
  		$('#productequipment').html('<p><img src="/images/ajax-loader.gif" width="120" height="19"/></p>');
  		$.get("includes/artilce_rightcol.php?pg="+pg+"&sortby="+sortby+"&user="+load_id, function(data){
		if(data!="")
		{
			
			document.getElementById("productequipment").innerHTML=data;
		}
	});
  } 
  function load_books(sortby,pg,load_id)
  {
  		$('#books').html('<p><img src="/images/ajax-loader.gif" width="120" height="19"/></p>');
  		$.get("includes/artilce_rightcol.php?pg="+pg+"&sortby="+sortby+"&user="+load_id, function(data){
		if(data!="")
		{
			
			document.getElementById("books").innerHTML=data;
		}
	});
  } 
  function load_advert(sortby,pg,load_id)
  {
  		$('#advert').html('<p><img src="/images/ajax-loader.gif" width="120" height="19"/></p>');
  		$.get("includes/artilce_rightcol.php?pg="+pg+"&sortby="+sortby+"&user="+load_id, function(data){
		if(data!="")
		{
			
			document.getElementById("advert").innerHTML=data;
		}
	});
  }
  </script> 
