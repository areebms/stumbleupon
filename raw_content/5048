                    $j('#annStartDate').val(new Date(time).print(Zapatec.Calendar._TT["DEF_DATE_FORMAT"]));
                }
            }
            // lazy load zapatec calendar
            Zapatec.Calendar.bootstrap({
                inputField     :    "annStartDate",
                showsTime      :    false,
                button         :    "annStartDate_button",
                step           :    1
            }, setTime);
        });
