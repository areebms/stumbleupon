<script type="text/javascript">
    if (top != self) {
        var isSafeTopUrl = false;
        try {
            var topUrl = top.location.href;
            var topUrlQsIdx = topUrl.indexOf('?');
            if (topUrlQsIdx > 0) {
                topUrl = topUrl.substring(0, topUrlQsIdx);
            }
            topUrl = topUrl.toLowerCase();

            var safeTopUrls = new Array(
                'http://smartenterpriseexchange.com/skin-basic.jspa',
                'http://smartenterpriseexchange.com/skin-advanced.jspa',
                'http://smartenterpriseexchange.com/skin-advanced!UploadSkinZip.jspa',
                'http://smartenterpriseexchange.com/skin-palette.jspa',
                'http://smartenterpriseexchange.com/skin-palette!start.jspa'
            );
            for (var i = 0; i < safeTopUrls.length; i++) {
                if (safeTopUrls[i].toLowerCase() == topUrl) {
                    isSafeTopUrl = true;
                    break;
                }
            }
        } catch (err) {
        }
        if (!isSafeTopUrl) top.location = location;
    }
</script>

