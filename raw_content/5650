			"zone_info": "huffpost.sports/news",
			"ord": 1308254101,
			"tile": 3,
			"width": 300,
			"height": 250,
			"el_id": tag_id + "_js",
			"class_name": "ad_block ad_wide top",
			"type": "iframe"
		}
		HuffPoUtil.WEDGJE.write(ad_spec, tag_id);
	};
</script>

