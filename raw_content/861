<script>
(function($){
  var props = ['Width', 'Height'], prop;
 
  while (prop = props.pop()) {
    (function (natural, prop) {
      $.fn[natural] = (natural in new Image()) ? 
      function () {
        return this[0][natural];
      } : 
      function () {
        var 
        node = this[0],
        img,
        value;
 
        if (node.tagName.toLowerCase() === 'img') {
          img = new Image();
          img.src = node.src,
          value = img[prop];
        }
        return value;
      };
    }('natural' + prop, prop.toLowerCase()));
  }
}(jQuery));

(function($) {
    $("[name='aol-photo-gallery']").aolPhotoGallery({
         photoWidth: 620,
         photoHeight: 600,
         showFullscreen: 0,
         toggleThumbnails: 0,
         showThumbnails: 1,
         showCaptions: 1,
         refreshDivId: "adsDiv1",
         refreshCount: 2        
    });

    var copycontrols = $('.aol-photo-gallery .controls').clone();
    $('.gallery').prepend(copycontrols);

}) (jQuery);


</script>
