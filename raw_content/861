<script type="text/javascript">

    $fbjoin = $('#fb-join,#tw-join');

    if ($fbjoin.length > 0) {

        $fbjoin.jqm().jqmShow();

        $("#facebook-join").click(function(evt) {
            evt.preventDefault();
            var win = window.open('http://www.facebook.com/dialog/oauth/?client_id=261532000546902&redirect_uri=' + encodeURIComponent('http://www.cambio.com/login/facebook/') + '&state=&scope=user_birthday,user_hometown,user_interests,user_likes,user_status,email,friends_likes', 'login', 'height=420,width=900,toolbar=no,scrollbars=yes');
            win.focus();
        });

        $("#twitter-join").click(function(evt) {

            evt.preventDefault();

            var win = window.open('/login/?tw=1', 'login', 'height=420,width=900,toolbar=no,scrollbars=yes');

            win.focus();
        });
    }
</script>



