$(document).ready(function() {

$('#upload-media #upload-photo-wrapper').unbind('click');
$('#upload-media #upload-video-wrapper').unbind('click');
$('#upload-media').delegate( '#upload-photo-wrapper, #upload-video-wrapper', 'click', function( e ){
    e.preventDefault();
    if ( !$('#toscheck').is(':checked') ) {
        alert('You must agree to the Terms of Services agreement before uploading content!');
        return false;
    }
    var $next = $(e.target).parent().is('#upload-video-wrapper') ? $('#upload-media-step-2b') : $('#upload-media-step-2a') ;
    $('#upload-media-step-1').fadeOut(function(){
        $next.fadeIn();
    });
});
});


$('#do-the-craze .thumbnail img').load(function() {

    var imgWidth = $(this).naturalWidth();
    var offsetWidth = -(imgWidth - 165) / 2;
    $(this).css('margin-left', offsetWidth);

    var imgHeight = $(this).naturalHeight();
    var offsetHeight = -(imgHeight - 165) / 2;
    $(this).css('margin-top', offsetHeight);

});


</script><script>
function deployLikeGate() {

    // show the like gate if they have viewed 3 or more pages
    var pageCount = parseInt($.cookie('likeGate'), 10);
    if (pageCount >= 2) {
        $('#like-gate').show();
        $.cookie('likeGate', '0');
        bN.ping('DisplayLikeGate');
    } else {
        pageCount++;
        $.cookie('likeGate', pageCount.toString());
    }
}

function checkLikes(uid, accessToken) {

    if ($.cookie('likeGateDisable') != 'true') {
        showLikeGate = true;
        // fetch the list of likes from facebook for this user
        $.getJSON('https://graph.facebook.com/' + uid + '/likes&access_token=' + accessToken, function(data) {
            // loop through and if we find cambio, they don't get the like gate
            $.each(data.data, function() {
                if (this.name == 'cambio') {
                    showLikeGate = false;
                    $.cookie('likeGateDisable', 'true');
                    return false;
                }
            });
        });
        if (showLikeGate === true) {
            deployLikeGate();
        }
    }
}


function likeGate(uid, accessToken, status) {

    // First Set the cookies if they doesn't exist
    if (!$.cookie('likeGate')) {
        $.cookie('likeGate', '0');
    }
    if (!$.cookie('likeGateDisable')) {
        $.cookie('likeGateDisable', 'false');
    }

    // make it happen

    if (status == 'connected' || status== 'not_authorized') {

        checkLikes(uid, accessToken);

    } else {

        deployLikeGate();

    }

}

// kill the window and set the cookie if the user says no thanks

$(document).ready(function(){
$('#cancel-gate').click(function(){$.cookie('likeGateDisable', 'true');$('#like-gate').hide();});
});
