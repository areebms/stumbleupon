<!-- newsletter validation -->
<script type="text/javascript">
$(function () {
	$("#newsletter-email").val("Enter your email address");
	$("#newsletter-email").focus(function() {
		$("#newsletter-email").val("");
	});
	$("#newsletter-email").blur(function() {
		if ($("#newsletter-email").val() == "") {
			$("#newsletter-email").val("Enter your email address");
		}

	});

	var validator = $("#newsletter-form").validate({
		rules: {
			email: {
				required: true,
				email: true
			}
		},
		messages: {
			email: ""
		},
		submitHandler: function(form) {			
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-cambio-newsletter/",
				data: dataString,
				success: function() {
					$("#newsletter-form").hide();
					$("#newsletter-success").show();
				}
			}); 
		},
		errorLabelContainer: $("#email-error"),
		invalidHandler: function(form, validator) {
		      var errors = validator.numberOfInvalids();
		      if (errors) {
		        var message = errors == 1
		          ? 'Please enter a valid email address.'
		          : 'Please enter a valid email address.';

		        $("#email-error").show();
		        		        $("#email-error").html(message);
		      } else {
		        $("#email-error").hide();
		      }
		    }
	});
	var sweepsvalidator = $("#sweepstakes-form").validate({
		rules: {
			email: {
				required: true,
				email: true
			},
			yearDOB: {
				required: true,
			},
			monthDOB: {
				required: true,
			},
			dayDOB: {
				required: true,
			},
			firstName: {
				required: true,
			},
			lastName: {
				required: true,
			},
                        cell: {
				required: true,
			}
		},
		messages: {
                        firstName: "Please include your first name.",
                        lastName: "Please include your last name.",
                        yearDOB: "Please include your birth year.",
                        monthDOB: "Please include your birth month.",
                        dayDOB: "Please include your date of birth.",
                        cell: "Please include your phone number.",
			email: "Please enter a valid email address."
		},
		submitHandler: function(form) {

                    var date = new Date();
                    date.setFullYear(date.getFullYear()-13);     
                    var birthday = new Date();
                    birthday.setFullYear($('#sweepstakes-year').val(), $('#sweepstakes-month').val(), $('#sweepstakes-day').val());
                    birthday.setDate(birthday.getDate());
                   
                    if (date < birthday) {

                    $('#sweepstakes-form').remove();
                    $('#sweepstakes').append('<p style="color:red; font-size:14px;">Sorry you are ineligible to enter this sweepstakes.</p><p style="margin-top:30px;"><a class="blue-button" style="color:#fff; padding:5px 8px;" href="http://www.cambio.com">&laquo; Back to Home</a></p>');
                    


                    } else {
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-cambio-sweeps/",
				data: dataString,
				success: function() {
					$("#sweepstakes-form").hide();
					$("#sweepstakes-success").show();
				}
			});
                    }
		},
		errorLabelContainer: $("#email-error"),
		invalidHandler: function(form, sweepsvalidator) {
		      var errors = sweepsvalidator.numberOfInvalids();
		      if (errors) {
		         var message = errors == 1
		        

		        $("#email-error").show();
		        		        $("#email-error").html(message);
		      } else {
		        $("#email-error").hide();
		      }
		    }
	});
	var redkettlevalidator = $("#redkettlesweepstakes-form").validate({
		rules: {
			email: {
				required: true,
				email: true
			},
			yearDOB: {
				required: true,
			},
			monthDOB: {
				required: true,
			},
			dayDOB: {
				required: true,
			},
			firstName: {
				required: true,
			},
			lastName: {
				required: true,
			},
			rulesconditions: {
				required: true,
			},
			zip: {
				required: true,
			}
		},
		messages: {
                        firstName: "Please include your first name.",
                        lastName: "Please include your last name.",
                        yearDOB: "Please include your birth year.",
                        monthDOB: "Please include your birth month.",
                        dayDOB: "Please include your date of birth.",
			rulesconditions: "Please agree to the rules.",
			zip: "Please include your zip/postal code.",
			email: "Please enter a valid email address."
		},
		submitHandler: function(form) {

                    var date = new Date();
                    date.setFullYear(date.getFullYear()-13);     
                    var birthday = new Date();
                    birthday.setFullYear($('#sweepstakes-year').val(), $('#sweepstakes-month').val(), $('#sweepstakes-day').val());
                    birthday.setDate(birthday.getDate());
                   
                    if (date < birthday) {

                    $('#sweepstakes-form').remove();
                    $('#sweepstakes').append('<p style="color:red; font-size:14px;">Sorry you are ineligible to enter this sweepstakes.</p><p style="margin-top:30px;"><a class="blue-button" style="color:#fff; padding:5px 8px;" href="http://www.cambio.com">&laquo; Back to Home</a></p>');
                    


                    } else {
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-red-kettle/",
				data: dataString,
				success: function() {
					window.location.href = "/rock-red-kettle-thankyou";

					// $("#sweepstakes-form").hide();
					// $("#sweepstakes-success").show();
				}
			});
                    }
		},
		errorLabelContainer: $("#email-error"),
		invalidHandler: function(form, redkettlevalidator) {
		      var errors = redkettlevalidator.numberOfInvalids();
		      if (errors) {
		         var message = errors == 1
		        

		        $("#email-error").show();
		        		        $("#email-error").html(message);
		      } else {
		        $("#email-error").hide();
		      }
		    }
	});




        var validatorUserbar = $("#user-bar-subscribe").validate({
		rules: {
                        firstName: "required",
			email: {
				required: true,
				email: true
			}
		},
		messages: {
                        firstName: "Please include your first name",
			email: "Please enter an email address"
		},
		submitHandler: function(form) {			
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-cambio-newsletter/",
				data: dataString,
				success: function() {
					$("#user-bar-subscribe").hide();
                                        $("#firstName, #user-bar-newsletter-email").val("");
					$("#user-bar-newsletter-success").show();
				}
			}); 
		},
		errorLabelContainer: $("#user-bar-email-error"),
		invalidHandler: function(form, validatorUserbar) {
		      var errors = validatorUserbar.numberOfInvalids();
		      if (errors) {
		        var message = errors == 1
		          ? 'Please enter the missing data.'
		          : 'Please enter a valid email address.';

		        $("#user-bar-email-error").show();
		        		        $("#user-bar-email-error").html(message);
		      } else {
		        $("#user-bar-email-error").hide();
		      }
		    }
	});
});
