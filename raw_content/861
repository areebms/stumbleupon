<script>
  window.fbAsyncInit = function() {
    FB.init(
     {appId: "261532000546902", status: true, cookie: true, xfbml: true}
    );
    Cambio.FB.initialized = true;

    

    FB.getLoginStatus(function(response) {

        status = response.status;
        console.log("response status = " + status);

        if (response.status == "connected") {
          uid = response.authResponse.userID;
          accessToken = response.authResponse.accessToken;
          console.log(uid + " " + accessToken); 

         likeGate(uid, accessToken, status);
          } else {
            likeGate("", "", status);
          }
    });

    FB.Event.subscribe("edge.create", function(response) {
        $.cookie("likeGateDisable", "true");
        document.getElementById("like-gate").setAttribute("style", "display:none");
    });

  

};

(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) {return;}
  js = d.createElement(s); js.id = id;
  js.src = "https://connect.facebook.net/en_US/all.js#xfbml=1&app_id=261532000546902";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
</script>


