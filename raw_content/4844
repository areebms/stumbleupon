<script type="text/javascript">
function stc_fb_update_user_details() {
	FB.getLoginStatus(function(response) {
		if (response.authResponse) {
			// Show their FB details TODO this should be configurable, or at least prettier...
			if (!jQuery('#fb-user').length) {
				jQuery('#please-introduce').hide();
				jQuery('#comment-user-details').hide().after("<span id='fb-user' style='display:none;'>" +
				"<fb:profile-pic uid='loggedinuser' facebook-logo='true' size='s'></fb:profile-pic>" +
				"<span id='fb-msg'><strong><fb:intl>Hi</fb:intl> <fb:name uid='loggedinuser' useyou='false'></fb:name>!</strong><br /><fb:intl>You are connected with your Facebook account.</fb:intl>" +
				"<a href='#' onclick='FB.logout(function(response) { window.location = \"http://allwomenstalk.com/50-chocolate-desserts/\"; }); return false;'> Logout</a>" +
				"</span><span class='end'></span></span>" +
				"<input type='hidden' name='sct_user_id' value='"+response.authResponse.userID+"' />"+
				"<input type='hidden' name='sct_user_token' value='"+response.authResponse.accessToken+"' />");
                
                jQuery('#commentform input#author').val( "<fb:name uid='loggedinuser' useyou='false'></fb:name>" );

                commtoolsL10n.userAvatar = '<img src="http://graph.facebook.com/' + response.authResponse.userID + '/picture?type=square" class="avatar avatar-40 photo" height="40" width="40" \/>';                

                if ( jQuery('#commentform #comment').val() ) {
                    if ( jQuery('#popup-loginform .form-submit').length ) {
                        jQuery('#popup-loginform #submit').parent('.form-submit').appendTo('#popup-comment');
                    }
                                            
                    jQuery('#commentform .form-submit #submit').trigger('click');
                } else {
                    jQuery('#popup-loginform #fb-user').show(300);
                }
                
                FB.api('/me', function(response) {
                    if ( response.name )
                        jQuery('#commentform input#author').val( response.name );
                    if ( response.email )
                        jQuery('#commentform input#email').val( response.email );
                    if ( response.link )
                        jQuery('#commentform input#url').val( response.link );
                });
			}

			// Refresh the DOM
			FB.XFBML.parse();
		} else {
            initializeCommentForm();
        }
	});
}
</script>
