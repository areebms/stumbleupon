						<script type="text/javascript">
						//<![CDATA[
						var ads = 0;
						if ( typeof adv1 === 'function' )
							ads += 1;
						if ( typeof adv2 === 'function' )
							ads += 1;

						if (ads) {
							//javascript ads
							var sel = Math.floor(Math.random()*(ads+1));
							if (sel) {
								//hide Latest and show one of ads
								jQuery('#more').hide();
								jQuery('#mgid').show();
								if ( sel === (ads-1) )
									adv1();
								if ( sel === ads )
									adv2();
							} //else there are nothing to do
						} else {
							//old-style ads (no javascript)
							var sel = Math.floor(Math.random()*2);
							if (sel) {
								jQuery('#more').hide();
								jQuery('#mgid').show();
							}
						}
						//]]>
						</script>
						</div>
						
