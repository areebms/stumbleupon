                query += 'url' + i + '=' + encodeURIComponent(links[i].href) + '&';
            }
        }
        document.write('<script type="text/javascript" src="http://disqus.com/forums/cerebralcortex/get_num_replies.js' + query + '"></' + 'script>');
    })();
