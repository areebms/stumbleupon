	function votefactory(dom,id,slug,direction){
	    var level = '.level-'+id;
	    var newdirection = direction;
	    var value = $(dom).siblings().hasClass("pressed")? 2:1;
	    if($(dom).hasClass("pressed")){
		newdirection = 'clear';
	    }
	    vote(id, slug, newdirection, 'pub', 'article',direction,value);
	    if($(dom).hasClass("pressed")){
		$(dom).removeClass("pressed");
	    }else{
		$(dom).addClass("pressed");
		if($(dom).siblings().hasClass("pressed")){
		    $(dom).siblings().removeClass("pressed");
		}
	    }		
	};

