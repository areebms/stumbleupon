			entry_params: {
				"id" : 3033583,
				"title" : "Jack Hoffman Touchdown: 7-Year-Old Brain Cancer Patient Scores In Nebraska Spring Game (VIDEO)",
				"created_on": 1365359580,
				"vertical_name": "Sports",
				"tweet_comm_hash" : "",
				"tweet_comm_text" : "",
				"force_fb_like"	  : 0,
                "custom_tweet_text" : ""			},
						
			global_name: "window." + inst_def.obj_name
		});


	// =====================================================
	// Now goes logic for every layout


	


	


	

	
	
		window[inst_def.obj_name].setPanelBorderStyle("glamorous_4");

        
		var slices = {
			1: "facebook_glamorous",
			2: "retweet_glamorous",
