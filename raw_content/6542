<script type="text/javascript">

  $('.signup-mini a.close').click(function(event) {
    event.preventDefault();
    $('.signup-mini').hide();
    R29.cookie.set('signup_mini',1,3600*24*30);
  });

  $('.signup-mini input.submit').click(function(event) {
    R29.cookie.set('signup_mini',1,-1);
    return true;
  });

  if(!R29.cookie.get('signup_mini')) {
    $('.signup-mini').show();
  }

</script>
    
    
