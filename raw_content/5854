function increaseFont() {
   document.getElementById("newscontent").style.fontSize="16px"
   document.getElementById("newscontent").style.lineHeight="25px"
   document.getElementById("newscontent2").style.fontSize="16px"
   document.getElementById("newscontent2").style.lineHeight="25px"
}
function increaseFont1() {
   document.getElementById("newscontent").style.fontSize="22px"
   document.getElementById("newscontent").style.lineHeight="30px"
     document.getElementById("newscontent2").style.fontSize="22px"
   document.getElementById("newscontent2").style.lineHeight="30px"
}
function decreaseFont() {
    document.getElementById("newscontent").style.fontSize="13px"
    document.getElementById("newscontent").style.lineHeight="20px"
     document.getElementById("newscontent2").style.fontSize="13px"
    document.getElementById("newscontent2").style.lineHeight="20px"
  }   
var isReady = true;
function doSaveAs(){
	track('Save','news',window.location,'0')
	if (document.execCommand){
		if (isReady){document.execCommand("SaveAs",false,'medindianews.htm');}
	}else{
		alert('Feature available only in Internet Explorer 4.0 and later.');
	}
} 
function submit1(s){
	track('Email','news',window.location,'0')
	document.frmup.action = "http://www.medindia.net/patients/calculators/calc_mail.asp"
	document.frmup.submit();
	return 	
}
function print_news(g){
	track('Print','news',window.location,'0')
	document.frmup.action = "http://www.medindia.net/news/view_main_print_new.asp"
	document.frmup.submit()
	return 
}
function syndicate(g){
	track('Syndicate','news',window.location,'0')
	document.frmup.action = "http://www.medindia.net/news/news_content_syndication.asp"
	document.frmup.submit()
	return 
}
function link1(){
	track('Link','news',window.location,'0')
	document.frmup.action = "http://www.medindia.net/linktopage.asp"
	document.frmup.submit()
}
function fnsearch1(){
	tmpcatid=document.search1.ncategoryid.value;
	tmpkeyword=document.search1.keyword.value;
	document.search1.action="http://www.medindia.net/news/newsday_list.asp?ncategoryid=" +tmpcatid +"&keyword=" +tmpkeyword	
	return true;
}	
function actTabnews(tabNem, tabCont, num, aTabCont){
	for (i=1;i<=num;i++){
		document.getElementById(tabNem+i).className='';
		document.getElementById(tabCont+i).style.display='none';
	}
	document.getElementById(tabCont+aTabCont).style.display='inline';
	document.getElementById(tabNem+aTabCont).className="selected";
}
function actTab(tabNem, tabCont, num, aTabCont){
	for (i=1;i<=num;i++)	{
		document.getElementById(tabNem+i).className='dAct';
		document.getElementById(tabCont+i).style.display='none';
	}
	document.getElementById(tabCont+aTabCont).style.display='inline';
	document.getElementById(tabNem+aTabCont).className="active";
}
</script> 
<SCRIPT LANGUAGE="Javascript" type="text/javascript">
var haveqt = false;
</SCRIPT>
<SCRIPT LANGUAGE="VBScript">
	On Error Resume Next
	Set theObject = CreateObject("QuickTimeCheckObject.QuickTimeCheck.1")
	On Error goto 0
	If IsObject(theObject) Then
		If theObject.IsQuickTimeAvailable(0) Then
			haveqt = true
		End If
	End If
</SCRIPT>
<SCRIPT LANGUAGE="Javascript" type="text/javascript">

if (navigator.plugins) {
	for (i=0; i < navigator.plugins.length; i++ ) {
		if (navigator.plugins[i].name.indexOf("QuickTime") >= 0)
		{ 
			haveqt = true; 
		}
	}
}

if ((navigator.appVersion.indexOf("Mac") > 0) && (navigator.appName.substring(0,9) == "Microsoft") && (parseInt(navigator.appVersion) < 5) )
{ haveqt = true; }

</SCRIPT>
