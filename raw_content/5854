</div>
<script type="text/javascript">
function isTouchDevice() {
   var el = document.createElement('div');
   el.setAttribute('ongesturestart', 'return;');
   return typeof el.ongesturestart === "function";
}
$(".nav-left-bgmid").mouseenter(function(){
	$(".nav-left").stop(true,true).slideDown(); 
});
$(".pg-col1").mouseleave(function(){
	$(".nav-left").stop(true,true).slideUp(); 
});
$(".nav-left-bgmid").click(function(){
	$(".nav-left").stop(true,true).slideToggle(); 
});

if(isTouchDevice()){
	$("ul.nav-left li").each(function(){
		$(this).find("a:first").click(function(event){
			event.preventDefault(); 
			$(this).mouseenter();
		});
		$(this).find("div.sub-menu").append('<a href="#." class="closeMenu">Close</a>');
	});

	$("div.nav-main-bgmid ul:first li:has(ul.sub-menu)").each(function(){
		$(this).find("a:first").click(function(){
			event.preventDefault(); 
			$(this).mouseenter();
		});
		$(this).find("ul.sub-menu").append('<a href="#" class="closeMenu">Close</a>');
	});
$("a.closeMenu").click(function(event){
	event.preventDefault(); 
	$(".nav-left").slideUp();
	$('.nav-main li').mouseleave();
});
}
</script>
