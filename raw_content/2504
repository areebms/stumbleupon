<script type="text/javascript" language="JavaScript"> 
GEL.thepage.initializer.add({
name: "snlPageTracker",
priority: 8000,
namespace: "remoting",
callback: function(){
		// create <script> element
		var js=document.createElement('script');
		// assign <script> attributes
		js.setAttribute('language','javascript');
		js.setAttribute('src','http://data.news-leader.com/pageview/story/' + GEL.thepage.pageinfo.date.isopuddate.slice(0,4) + GEL.thepage.pageinfo.key + '/' + escape( GEL.thepage.pageinfo.title ) );
		// append element to document tree & send GET request
		document.getElementsByTagName('head')[0].appendChild( js );
	}
});
</script>

						
					</div>
					<div class="clear"></div>
				</div>				
			</div>
