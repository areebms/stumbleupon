			//_l is in 24:00 format, convert to 12:00 am/pm
			var _temp_int = parseInt(_t[0].replace(/^0*(?=[0-9]$)/, ''));
			var _temp_hrs = (_temp_int > 11 ? _temp_int - 12 : _temp_int);
