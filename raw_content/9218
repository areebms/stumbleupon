<script type="text/javascript">
//weak scripts used here because drupal SPoD page is throwing an error that stops anything more hi-tech here
var container = $( "#country_container" );
function fireCountry ()
{		
	if (container.is( ":visible" ))
	{
		container.slideUp( 500 );
	} 
	else 
	{
		container.slideDown( 500 );
	}
}	
function closeCountry()
{
	container.slideUp( 500 );
}
</script> 

