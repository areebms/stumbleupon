    (function(){
        
        function cookie(key) {                
                var val = new RegExp("(^|; )"+key+"=.*?(?=($|;))").exec(document.cookie) || [" "];
                return val[0].substring(1).replace(/.*?=/, '') || "";
            }
        
        if ( cookie("edition") == "us" && ( window.navigator && window.navigator.userAgent || "" ).toLowerCase().indexOf("ipad") >= 0 && window.location.pathname == "/" ) {
            switch ( cookie("mobile") || "" ) {
                case "full":
                    break;
                case "tablet":
                    window.location = "http://t.eonline.com";
                    break;
                default:
                    window.location = "/mobile.choice.jsp";
            }
        }
    })();
    
</script>
