  var valid_type  = "application/rss+xml";
  var list = document.getElementsByTagName("link");
  for (var i = 0; i < list.length; i++) {
    var link = list[i];
    if ((link.type == valid_type) && (link.title == valid_title)) {
      document.write(link.href);
    }
  }
  document.write('\"><img src=\"http://buttons.googlesyndication.com/fusion/add.gif\" width=\"104\" height=\"17\" border=\"0\" alt=\"Add to Google\"></a>');

</script>



	</div>
</div>
