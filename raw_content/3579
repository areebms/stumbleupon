	function webie()
	{
		var call = "mailto";
		var pre = "submit";
		var post = "";
		var last = "";
		var subj = "[Contributed by YourNickHere]CaptionGoesHere";
		
		var commonname = call + ':' + pre + '@' + post + '' + last + '?subject=' +subj;
		
		location.href = commonname;
		
	}
