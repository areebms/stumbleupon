	<script type="text/javascript" language="JavaScript">

	function doVisible(cur,which){
	strength=(which==0)? 1 : 0.6
	
	if (cur.style.MozOpacity)
	cur.style.MozOpacity=strength
	else if (cur.filters)
	cur.filters.alpha.opacity=strength*100
	}
	
	function disableLikeIt(sid)
	{
		eval("$('likes_button_" +  sid +"').hide();");
		eval("$('hates_button_" +  sid +"').hide();");
	}

	function doLikeIt(sid) {
		if(sid) {
		var url = '/likeit.php';
		var params = 'sid=' + sid;
		var ajax = new Ajax.Updater(
		{success: 'likes_text' + sid},
			url,
			{method: 'get', parameters: params, onLoading: $('likes_text'+sid).update('Loading...'), onComplete: disableLikeIt(sid), onFailure: reportError});
			}
		}

	function doHateIt(sid) {
		if(sid) {
		var url = '/likeit.php';
		var params = 'not=1&sid=' + sid;
		var ajax = new Ajax.Updater(
		{success: 'hates_text' + sid},
			url,
			{method: 'get', parameters: params, onLoading: $('hates_text'+sid).update('Loading...'), onComplete: disableLikeIt(sid), onFailure: reportError});
			}
		}

		function reportError(request) {
			alert("Error: "+request);
		}

		function commentLoading()
		{
			document.forms['commentFrm'].comments.value = ''
			document.forms['commentFrm'].commentBtn.value = 'Submitting...';
			document.forms['commentFrm'].commentBtn.disabled = true;
		}
		
		function commentLoaded()
		{
			document.forms['commentFrm'].comments.value = ''
			document.forms['commentFrm'].commentBtn.value = 'Post';
			document.forms['commentFrm'].commentBtn.disabled = false;
		}
		
	function postComment(sid, comments) {
		if(sid) {
		var url = '/post_comments.php';
		var params = 'submissionid=' + sid + '&comments='+escape(comments);
		var ajax = new Ajax.Updater(
		{success: 'comments_data'},
			url,
			{	method: 'post', 
				parameters: params, 
				onFailure: reportError,
				onLoading: commentLoading,
				onSuccess: commentLoaded
			}
			);
			}
		}


	/* HotKeys */

/*
	new HotKey('d',function(event){  
		alert('"d" pressed');  
	},{  
		 ctrlKey: false  
	});  

*/


</script>
