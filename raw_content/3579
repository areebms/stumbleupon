	<script type="text/javascript">
	
	//alert(Get_Cookie('auth'));
	
	function promptLogin()
	{
		url_open('/comments_login.php',150,300,0);
	}
	
	function checkAuth(f)
	{
	
		// little validation.
		if (f.comments.value.length == 0)
		{
			alert('Blank comments are not allowed.');
			f.comments.focus();
			return false;
		}
	
		var auth = Get_Cookie('loginuid');
		
		if (auth == '')
		{
			return true;
		}
		else
		{
			promptLogin();
			return false;
		}
	}
	
	
	</SCRIPT>
	
