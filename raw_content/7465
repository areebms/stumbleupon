            holder.currentPostUrl = "";
            holder.currentPostId = 6951627040213173686
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/3141856470087340306/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/3141856470087340306/comments/default";
          
          holder.currentBlogUrl = "http://www.animegeeksplayland.com/";
          holder.currentBlogId = "3141856470087340306";
        
        return holder;
      });
    }
  </script>
