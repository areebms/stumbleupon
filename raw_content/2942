    <div style="clear:both;"><img src="http://images.alternet.org/images/site/spacer.gif" /></div>
    
    <!-- COMMENT COUNT JS ONLY NEEDED ON ARTICLE PAGE -->
    
    <script type="text/javascript">
    
    
        // Disqus comment count js
        (function() {
            var links = document.getElementsByTagName('a');
            var query = '?';
            for(var i = 0; i < links.length; i++) {
                var dsqid = links[i].getAttribute('dsqid');
                if(dsqid) {
                    query += 'dsqid' + i + '=' + dsqid + '&';
                }
            }
            document.write('<script charset="utf-8" type="text/javascript" src="http://alternet.disqus.com/get_num_replies_from_dsqid.js' + query + '"></' + 'script>');
            
            // retarget disqus comment url to go to comment page
            $(".comments_link").click(function () { 
                window.location.replace("http://www.alternet.org/food/152848/worst_food_additive_ever_its_in_half_of_all_foods_we_eat_and_its_production_destroys_rainforests_and_enslaves_children/comments/");
            });
            
        })();
        
        // Digg Button JS
        
        (function() {
            var s = document.createElement('SCRIPT'), s1 = document.getElementsByTagName('SCRIPT')[0];
            s.type = 'text/javascript';
            s.src = 'http://widgets.digg.com/buttons.js';
            s1.parentNode.insertBefore(s, s1);
        })();
    
    
    </script>
    
