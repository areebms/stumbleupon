<script language = "JavaScript">
var topjobs_count = "5";
var topjobs_option = "";
var topjobs_tablewidth = "160";
var topjobs_tableborder = "0";
var topjobs_tablecellspacing = "0";
var topjobs_tablecellpadding = "0";
var topjobs_tablebgcolor = "#FFFFFF";

// Header
// SiteCatalyst variable
var Prop10 = "Top_Job";
// Set defaults
if (!topjobs_tablewidth) {
	var tableWidth = 150;
}
else {
	var tableWidth = topjobs_tablewidth;
}
if (!topjobs_tableborder) {
var tableBorder = 1;
}
else {
	var tableBorder = topjobs_tableborder;
}
if (!topjobs_tablecellpadding) {
	var tableCellPadding = 1;
}
else {
	var tableCellPadding = topjobs_tablecellpadding;
}
if (!topjobs_tablecellspacing) {
	var tableCellSpacing = 0;
}
else {
	var tableCellSpacing = topjobs_tablecellspacing;
}
if (!topjobs_tablebgcolor) {
	var tableBgcolor = "#ffffff";
}
else {
	var tableBgcolor = topjobs_tablebgcolor;
}

var header = "<table border="+tableBorder+" cellpadding="+tableCellPadding+" cellspacing="+tableCellSpacing+" bgcolor="+tableBgcolor+" width="+tableWidth+"><tr><td align=center><table><tr><td align=center class=gcitopjobs><a href='http://azcentral.gannettonline.com/careerbuilder/' target=_blank><img src='http://www.azcentral.com/imgs/topjobs.gif' border=0></a><hr size=1 color=000066 noshade></td></tr>";

var allHeader = "<html><head><title>Top Jobs</title></head><body><table width=250><tr><td align=center><a href='http://azcentral.gannettonline.com/careerbuilder/' target=_blank><img src='http://www.azcentral.com/imgs/topjobs.gif' border=0></a><hr size=1 color=000066 noshade></td></tr>";

// All the postings
var allPostings = new Array("placeholder"
, Array('RN Home Health Clinical Manager', 'Nursing Solutions', 'Home Health Clinical Manager Phoenix, AZ', 'http://www.careerbuilder.com/jobseeker/jobs/RedirectAOL.aspx?Job_DID=J3F1MZ712X20NTXF1PD&amp;show=yes&amp;lr=cbga_tap&amp;siteid=cbga_tap060&show=yes')
, Array('APARTMENT MANAGER', 'CONFIDENTIAL', 'Apartment Manager Sierra Vista, AZ Need ...', 'http://www.careerbuilder.com/jobseeker/jobs/RedirectAOL.aspx?Job_DID=J3F6SR6SFP1RGH6Q5RZ&amp;show=yes&amp;lr=cbga_tap&amp;siteid=cbga_tap060&show=yes')
