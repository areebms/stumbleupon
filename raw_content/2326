var jstr = "";

for(j=bpCmId.length-1;j>=0;j--){ 
jstr+='<div><a id="c' + bpCmId[j][0] + '"></a><div style="float:left;width:110px" class="cNum">' + ((100-j)+(commTotal-100)) + '</div><div style="float:right;" class="cBod"><div class="commentBodyText">' + bpCmId[j][1] + '</div><span class="author"><span>Posted by ' + bpCmId[j][2] + '</span> <span class="comment-date">' + bpCmId[j][3] + '</span></span></div></div><div style="clear:both;"></div>';
}

setTimeout("document.getElementById('commOl2').innerHTML=jstr",3000)
</script>
