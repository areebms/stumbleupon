<script type="text/javascript">
$(document).ready(function(){

 if (getVCCookie("veracode_soss")) {
   $('#panel').slideDown("slow");
   setTimeout(function(){
        $('#panel').slideUp(500);
        $("#panelclose").hide();
   },5000); 
 }

        
        var container = $( "#panel" );
	$(".btn-slide").click(function(){
		$("#panel").slideToggle("slow");
                $("#panelclose").show();
                if (container.is( ":visible" )){
                    setTimeout(function(){
                         $('#panel').slideUp(500);
                         $("#panelclose").hide();
                    },5000); 
                }
                return false;
	});

        $("#panel").click(function() {
           window.location =  "http://info.veracode.com/state-of-software-security-report-volume3.html";
	});

        if (container.is( ":visible" )){
            $("#panelclose").show();
        } else {
            $("#panelclose").hide();
        }

        $("#panelclose").click(function() {
           setVCCookie("veracode_soss", "1");
           $('#panel').hide();
           $('#panelclose').hide();
        })
});



function setVCCookie(name, value, expireDays, path, domain) {
	var expireDate = null;
	if(expireDays != null && expireDays != 0) {
		expireDate = new Date();
		expireDate.setDate(expireDate.getDate()+expireDays);
	}
	cookieStr = name + "=" + escape(value) + 
	((expireDate == null) ? "" : ";expires=" + expireDate.toGMTString()) +
	((path == null) ? "" : ";path=" + path) +
	((domain == null) ? "" : ";domain=" + domain) + 
	";secure;";
	document.cookie = cookieStr;
}

function getVCCookie(name)
{
	var cname = name + "=";               
	var dc = document.cookie;     
	// indexOf method of arrays
	// searches array for a value      
	// if array has elements  
	if (dc.length > 0) 
	{            
		begin = dc.indexOf(cname);       
		if (begin != -1) 
		{           
			begin += cname.length;       
			end = dc.indexOf(";", begin);
			if (end == -1) 
			{
				end = dc.length;
			}
			// unescape function restores "illegal" chars
			// to a string
			// substring method equivalent to vb's mid function
			return unescape(dc.substring(begin, end));
		} 
	}
	return null;
}

