$(document).ready(function() {
	
	$('form.search input.text').focus(function() { $(this).select(); });
	
   $('#searchword').click(
      function() {
         if (this.value == this.defaultValue) {
           this.value = '';
         }
      }
   );
   $('#searchword').blur(
     function() {
        if (this.value == '') {
          this.value = this.defaultValue;
        }
    }
  );
});
