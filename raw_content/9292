<script type="text/javascript" src="http://cdn.gigya.com/JS/gigya.js?services=socialize"></script>

<script type="text/javascript" charset="utf-8">

	var commentable_id = 181;
	var offset = -1;
    var content_title = "How to Walk in High Heels";
    var content_url = "/videos/how-to-walk-in-high-heels/181";
    var modelinia_url = "http://www.modelinia.com";
    content_url = modelinia_url + content_url + "#comments";
    var content_thumb = modelinia_url + "/_content/videos/181/How.to.Walk.in.High.Heels2_LFT_135x96.jpg";    
    var commentable_type = "video";
    var comment_id =  commentable_type + "_comment_content";

    var gigyaConf = {
	    APIKey: "2_l_78JTD1s-OhnrAcQsuOMw==",
	    enabledProviders: "facebook,twitter",
        signIDs: true
    }
    var gigyaUser, gigyaHandlers, gigyaLoginSignature, gigyaLoginTimestamp;
    
	// clear flash messages 
	function hide_flashes() {
		$$('.flash').each( function(f) {
			f.hide();
		});
	}

	function discuss_flash(css_id, update_str) {
		elem = $(css_id);
		elem.update('<p class="transpng">' +update_str+ '</p>');
		elem.show();
	}

	// utility to prevent multiple form submission
	function lock_submit(id) {
		var elem = $(id);     
		$(elem).disable();
		$(elem).setOpacity(.5);
	}
	
	// release lock when we've updated what we can
	function unlock_submit(id) {
		var elem = $(id);
		$(elem).enable();
		$(elem).setOpacity(1);
	}

	// constructs the comment markup given the comment data
	function commentMarkup(name, date, content) {
		var str = '';
		str = '<div class="comment transpng">\n' +
			'<div class="byline"><span class="name">'+name+'</span><span class="date"> | '+date+'</span></div>\n' +
			'<div class="body">\n' +
			'<div class="content">'+content+'</div>\n' +
			'</div>\n' +
		'</div>\n';
		return str;	
	}

	// on success from get comments (in batch form)
	function updateComments(c) {
		var comments = '';
		var num_comments = c.comments.size();
		for(i=0;i<num_comments;i++) {
			comments+= commentMarkup(c.comments[i].name, c.comments[i].date, c.comments[i].content);
		}
		$('no_comments').hide();
		$('comment_loader').hide();
		$('comments').insert(comments, 'bottom');	
	}

	// show more link if we have more, hide if we don't
	function updateMoreCommentsLink(new_offset) {
		offset = new_offset;
		if (new_offset != '') {
			$('next_comment_collection').show();
		} else {
			$('next_comment_collection').hide();
		}
	}

	// pointer to get comments in batch, for binding
	function get_next_batch(e) {
		$('next_comment_collection').hide();
		get_more_comments(commentable_id,offset);
		Event.stop(e);
	}
	
	
	// get comments run once on dom loaded, and on more comments link
	function get_more_comments(commentable_id, offset) {
		hide_flashes();
		$('comment_loader').show();
		new Ajax.Request("/videos/"+commentable_id+"/get_comments/", {
			method: 'post',
			parameters: {offset: offset},
			requestHeaders: {Accept: 'application/json'},
			onSuccess: function(transport){
				var json = transport.responseText.evalJSON(true);
				if (json['success'] == 'true') {
					if (json['no_comments'] == 'true') {
						$('no_comments').show();
					} else {
						$('no_comments').hide();
						updateComments(json);
					}
					offset = json['offset'];
					updateTotalComments(json['total_comments']);
					updateMoreCommentsLink(offset);
				} else {
					discuss_flash('flash_error', json['flash']['error']);
				}

			},
			onFailure: function(){ discuss_flash('flash_error', "Unable to retrieve comments at this time."); },
			onComplete: function() { 
				$('comment_loader').hide(); 
			}
		});
	}
	
	// on success from post comment
	function prependNewComment(c) {
		var comment = commentMarkup(c['comment']['name'], c['comment']['date'], c['comment']['content']);
		$('no_comments').hide();
		$('comments').insert({'top': comment});
		$('single_comment_loader').hide();
		unlock_submit('add_comment');
		updateTotalComments();
	}
	
	// pointer to post_comment for binding
	function post_new_comment(e) {
		lock_submit('add_comment');
		post_comment(commentable_id, offset);
		Event.stop(e);
	}

	// update total comment count after success of post comment
	function updateTotalComments(new_total) {
		$$('.total_comments').each(function(t) { t.update(new_total)});
	}
	
	function validLoginForm() {
		var email_field = $F('user_email');
		var password_field = $F('user_password');
		if (email_field =='Email Address' || email_field == '' || password_field == '') {
			return false;
		} else {
			return validateEmailString(email_field);
		}
	}

    function validRegForm() {
        // TODO - really validate
        return true;
    }
	
	function valid_comment_form() {
		if ($F("video_comment_content") == '') {
			return false;
		} else {
			return true;
		}
	}
	
	// ajax comment create 
	function post_comment(commentable_id, offset) {
		hide_flashes();
		if (!valid_comment_form()) {
			discuss_flash('flash_error', 'Enter text for your comment.');
			unlock_submit('add_comment');
		} else { 
			var params_obj = $H($('post_new_comment').serialize(true));
            var commentElem = $(comment_id);
            var comment = commentElem.value;

			params_obj.set('offset', offset);
			$('single_comment_loader').show();
			new Ajax.Request("/videos/"+commentable_id+"/post_comment/", {
				method: 'post',
				parameters: params_obj,
				requestHeaders: {Accept: 'application/json'},
				onSuccess: function(transport){
					var json = transport.responseText.evalJSON(true);
					if (json['success']=='true') {
						offset = json['offset'];
						prependNewComment(json);
						updateTotalComments(json['total_comments']);
						updateMoreCommentsLink(offset);
                        shareComment(comment);
						discuss_flash('flash_notice', json['flash']['notice']);
						toggleAfterComment();
                        commentElem.value = "";
					} else {
						// success == false
						if (json['login']) {
							setCookie('is_logged_in');
							toggleLoggedInContent(false);
						}
						discuss_flash('flash_error', json['flash']['error']);
					}
				},
				onFailure: function(){ 
					discuss_flash('flash_error', 'Server error: please try again later.');
					},
				onComplete: function() {
					$('single_comment_loader').hide();
					unlock_submit('add_comment');
				}
			});
		}
	}
	
	function post_login_ajax(e) {
		Event.stop(e);
		lock_submit('login');
		hide_flashes();
		
		// simple validation on email and password.
		if (validLoginForm()) {
			$('login_progress').show();
			params_obj = $H($('login_ajax').serialize(true));
			new Ajax.Request('/user/auth_js', {
				method: 'post',
				parameters: params_obj,
				requestHeaders: {Accept: 'application/json'},
				onSuccess: function(transport){
					var json = transport.responseText.evalJSON(true);
					if (json['success'] == 'true') {
						discuss_flash('flash_notice', json['flash']['notice']);
						triggerPersonalize();
                        notifyLogin(json['siteUID'], json['timestamp'], json['signature']);
					} else {
						discuss_flash('flash_error', json['flash']['error']);
					}
					
				},
				onFailure: function(){ 
					discuss_flash('flash_error', 'Server error: Unable to log you in at this time.');
				 },
				onComplete: function() {
					unlock_submit('login');
					$('login_progress').hide();
				}
			});
		} else {
			discuss_flash('flash_error', 'Invalid email address or password.');
			unlock_submit('login');
			$('login_progress').hide();
		}
	}

	function onRegister(e) {
        console.debug("onRegister", e);
		Event.stop(e);
        post_reg_ajax();
    }

    function post_reg_ajax(){
        console.debug("post_reg_ajax");
		lock_submit('signup');
		hide_flashes();

		if (validRegForm()) {
			$('login_progress').show();
			params_obj = $('reg_ajax').serialize(true);
            if (gigyaUser && gigyaUser.isLoggedIn) {
                addGigyaIdentitiesToPostParams(gigyaUser.identities, params_obj);
            }
			new Ajax.Request('/user/signup_js', {
				method: 'post',
				parameters: params_obj,
				requestHeaders: {Accept: 'application/json'},
				onSuccess: function(transport){
					var json = transport.responseText.evalJSON(true);
					if (json['success'] == 'true') {
						discuss_flash('flash_notice', json['flash']['notice']);
						triggerPersonalize();
                        updateShareControls();
                        notifyLogin(json['siteUID'], json['timestamp'], json['signature']);
					} else {
						discuss_flash('flash_error', json['flash']['error']);
                        if (!$('reg_section').visible()) {
                            hide_flashes();
                            showRegistration();
                        }
					}

				},
				onFailure: function(){
					discuss_flash('flash_error', 'Server error: Unable to log you in at this time.');
				 },
				onComplete: function() {
					unlock_submit('signup');
					$('login_progress').hide();
				}
			});
		} else {
			discuss_flash('flash_error', 'Validationerror');
			unlock_submit('signup');
			$('login_progress').hide();
		}
	}

    function post_identities_ajax(){
        console.debug("post_identities_ajax");

        var params_obj = { UID: gigyaUser.UID, signature: gigyaLoginSignature, timestamp: gigyaLoginTimestamp }
        addGigyaIdentitiesToPostParams(gigyaUser.identities, params_obj);

        new Ajax.Request('/user/update_identities_gigya', {
            method: 'post',
            parameters: params_obj,
            requestHeaders: {Accept: 'application/json'},
            onSuccess: function(transport) {
                var json = transport.responseText.evalJSON(true);
                console.debug("post_identities_ajax onSuccess", json);
                updateShareControls();
            },
            onFailure: function(){
                console.error("post_identities_ajax onFailure");
            }
        });

	}

    function addGigyaIdentitiesToPostParams(identities, param_obj) {
        param_obj.UID = gigyaUser.UID;
        param_obj.signature = gigyaLoginSignature;
        param_obj.timestamp = gigyaLoginTimestamp;
        for (provider in identities) {
            var providerIdentity = identities[provider];
            for (prop in providerIdentity) {
                var pname = "identities[" + provider + "]" + "[" + prop + "]";
                param_obj[pname] = providerIdentity[prop];
            }
        }
        return ret;
    }

	function login_with_gigya(login_params) {
        console.debug("login_with_gigya", login_params);
		lock_submit('login');
		hide_flashes();

		if (gigyaUser.isLoggedIn) {
			$('login_progress').show();
            addGigyaIdentitiesToPostParams(gigyaUser.identities, login_params);

			new Ajax.Request('/user/auth_gigya', {
				method: 'post',
				parameters: login_params,
				requestHeaders: {Accept: 'application/json'},
				onSuccess: function(transport){
                    console.debug("login_with_gigya onSuccess", transport);
					var json = transport.responseText.evalJSON(true);
					if (json['success'] == 'true') {
						discuss_flash('flash_notice', json['flash']['notice']);
						triggerPersonalize();
                        updateShareControls();
					} else {
                        if (json['flash']['error']) {
						    discuss_flash('flash_error', json['flash']['error']);
                            initRegFields();
                            autoRegister();
                        }
                    }

				},
				onFailure: function(){
                    console.debug("login_with_gigya onFailure");
					discuss_flash('flash_error', 'Server error: Unable to log you in at this time.');
				 },
				onComplete: function() {
                    console.debug("login_with_gigya onComplete");
					unlock_submit('login');
					$('login_progress').hide();
				}
			});
		}
	}

	// for comment post success finish
	function toggleAfterComment() {
		disableCommentControls();
		$$('.comment_controls > div').invoke('toggle');
		Effect.toggle('form_containers', 'blind', {duration: .7, afterFinish: function() {
			enableCommentControls();
		}});
	}

	// for the open/close controls
	function toggleCommentControlsAndForms() {
        console.debug("toggleCommentControlsAndForms", gigyaUser);
		disableCommentControls();
		if (!$('form_containers').visible()) {
			hide_flashes();
		    if (isLoggedIn()) {
 		        updateShareControls();
            }
		    $$('.leave_comment_link').each(function(item, index) {
			    item.hide();
		    });
		} else {
		    $$('.leave_comment_link').each(function(item, index) {
			    item.show();
		    });              
        }
		$$('.comment_controls > div').invoke('toggle');
		Effect.toggle('form_containers', 'blind', {duration: .20, afterFinish: function() {
			enableCommentControls();
		}});
	}
	function enableCommentControls() {
		$$('.comment_controls > div > a').each(function(item, index) {
			item.observe('click', toggleCommentControlsAndForms);
		});
		$$('.leave_comment_link').each(function(item, index) {
			item.observe('click', toggleCommentControlsAndForms);
		});
	}
	function disableCommentControls() {
		$$('.comment_controls > div > a').each(function(item, index) {
			item.stopObserving('click', toggleCommentControlsAndForms);
		});
		$$('.leave_comment_link').each(function(item, index) {
			item.stopObserving('click', toggleCommentControlsAndForms);
		});
	}
	function updateShareControls() {
	    console.debug("updateShareControls", gigyaUser);
        if (gigyaUser && gigyaUser.identities && gigyaUser.identities['facebook']) {
            $('connected_to_facebook').show();
            $('not_connected_to_facebook').hide();
        } else {
            $('not_connected_to_facebook').show();
            $('connected_to_facebook').hide();
        }
        if (gigyaUser && gigyaUser.identities && gigyaUser.identities['twitter']) {
            $('connected_to_twitter').show();
            $('not_connected_to_twitter').hide();
        } else {
            $('not_connected_to_twitter').show();
            $('connected_to_twitter').hide();
        }
	}

	
	document.observe('dom:loaded', function() {
		// hook up the ajax submits
		Event.observe($('login_ajax'), 'submit', post_login_ajax, false);
		Event.observe($('reg_ajax'), 'submit', onRegister, false);
		Event.observe($('show_login_link'), 'click', onShowLogin, false);
		Event.observe($('show_reg_link'), 'click', onShowRegistration, false);
		Event.observe($('login_facebook_link'), 'click', onLoginProvider, false);
        $('login_facebook_link').provider = 'facebook';
		Event.observe($('login_twitter_link'), 'click', onLoginProvider, false);
        $('login_twitter_link').provider = 'twitter';
		Event.observe($('connect_facebook_link'), 'click', onConnectProvider, false);
        $('connect_facebook_link').provider = 'facebook';
		Event.observe($('connect_twitter_link'), 'click', onConnectProvider, false);
        $('connect_twitter_link').provider = 'twitter';

		Event.observe($('disconnect_twitter'), 'click', disconnectFromService, false);
		$('disconnect_twitter').provider = 'twitter';
		Event.observe($('disconnect_facebook'), 'click', disconnectFromService, false);
		$('disconnect_facebook').provider = 'facebook';	

		Event.observe($('post_new_comment'), 'submit', post_new_comment, false);
		// get first batch of comments if we need to
		Event.observe($('more_comments_link'), 'click', get_next_batch, false);

		// offset is -1 to start with
		get_more_comments(commentable_id, offset);

		// turn on open/close controls
		enableCommentControls();

        // register for connect status changes
        setGigyaHandlers({
            onConnect: onGigyaConnect,
            onDisconnect: onGigyaDisconnect,
            onLogin: onGigyaLogin
        });

        // get user info
        getGigyaUserInfo();
    });


    function onGigyaGetUserInfo(res) {
        gigyaUser = res.user;
        console.debug("onGigyaGetUserInfo", res);
        if (res.status != 'OK') {
            console.error("onGigyaGetUserInfo", res.status, res.statusMessage);
            return;
        }
        if (gigyaUser && gigyaUser.isLoggedIn) {
            updateShareControls();
            showRegistration();
        }
    }
    function onGigyaConnect(res) {
        console.debug("onGigyaConnect", res);
        gigyaUser = res.user;
        if (!isLoggedIn) {
            console.error("onGigyaConnect called but user not logged into Modelinia");
            return;
        }
        gigyaLoginTimestamp = res.user.timestamp;
        gigyaLoginSignature = res.user.UIDSig;
        if (isLoggedIn()) {
            post_identities_ajax();
            return;
        }
        updateShareControls();

    }
    function onGigyaDisconnect(res) {
        console.debug("onGigyaDisconnect", res);
    }
    function onGigyaLogin(res) {
        console.debug("onGigyaLogin", res);
        if (res.signature == "") {
            return;
        }
        gigyaUser = res.user;
        gigyaLoginTimestamp = res.timestamp;
        gigyaLoginSignature = res.signature;

        if (isLoggedIn()) {
            post_identities_ajax();
            return;
        }

        // Try to login to site
        if (gigyaUser.isLoggedIn) {
            var remember = $('remember').checked;
            login_with_gigya({UID: res.UID, signature: res.signature, timestamp: res.timestamp, remember: remember});
        }
    }



    function onShowRegistration(e) {
        Event.stop(e);
        showRegistration(true);
    }

    function initRegFields() {
        if (gigyaUser && gigyaUser.isLoggedIn) {
            $('reg_first_name').value = gigyaUser.firstName;
            $('reg_last_name').value = gigyaUser.lastName;
            $('reg_email').value = gigyaUser.email;
            $('reg_username').value = gigyaUser.nickname;
            // TODO - If facebook (and myspace, others?) set DOB fields
        }
    }

    function autoRegister() {
        console.debug("autoRegister");
        post_reg_ajax();
    }

    function showRegistration(showPass) {
        var showPasswords = typeof(showPass) != 'undefined' ? showPass : false;

        if (showPasswords) {
            $('reg_passwords').show();
            $('noPassword').value = '0';
        } else {
            $('reg_passwords').hide();
            $('reg_password').value = '';
            $('reg_password_confirmation').value = '';        
            $('noPassword').value = '1';
        }
        $('login_section').hide();
        $('reg_section').show();
        console.debug("showRegistration");
    }

    function onShowLogin(e) {
        Event.stop(e);
        showLogin();
    }
    function showLogin() {
        $('reg_section').hide();
        $('login_section').show();
    }

    function shareComment(comment) {
        if (!(gigyaUser && (gigyaUser.isLoggedIn || gigyaUser.isConnected))) {
            return;
        }
        console.debug("shareComment", comment);
        var shareConf = Object.clone(gigyaConf);
        var act = new gigya.services.socialize.UserAction("{actor} {action} on {content} at {modelinia}: " + comment);
        act.setActionName("commented");
        act.setTemplateField("content", content_title, content_url);
        act.setTemplateField("modelinia", "Modelinia", modelinia_url);
        if (content_thumb != "") {
            console.debug("content_thumb", content_thumb);
            act.addImage(content_thumb, content_url);
        }
        var params = { callback: onShareComplete }
        if (gigyaUser.identities['facebook'] && $('shareOnFacebook').checked) {
            shareConf.enabledProviders = "facebook";
            params.context = "Facebook";
            params.status = "Commented on " + '"' + content_title + '"' + " at Modelinia (" + content_url + "): " + comment;
            gigya.services.socialize.setStatus(shareConf, params);
//            params.userAction = act;
//            gigya.services.socialize.publishUserAction(shareConf, params);
        }
        if (gigyaUser.identities['twitter'] && $('shareOnTwitter').checked) {
            shareConf.enabledProviders = "twitter";
            params.status = "Commented on " + '"' + content_title + '"' + " at Modelinia: " + content_url;
            params.context = "Twitter";
            gigya.services.socialize.setStatus(shareConf, params);
        }
    }

    function onShareComplete(res) {
        console.debug("onShareComplete", res);
        if (res.status != "OK") {
            discuss_flash('flash_error', 'There was a problem sharing your comment on ' + res.context);            
        }
    }

    function onLoginProvider(e) {
        console.debug("onLoginProvider", e);
        Event.stop(e);
        var provider = this.provider;
        gigya.services.socialize.login(gigyaConf, {provider: provider});
    }

    function onConnectProvider(e) {
        Event.stop(e);
        var provider = this.provider;
        console.debug("onConnectProvider", e, provider);
        gigya.services.socialize.connect(gigyaConf, {provider: provider});
    }

    function notifyLogin(siteUID, timestamp, signature) {
        console.debug("notifyLogin", siteUID, timestamp, signature);
        if (gigyaUser && gigyaUser.isLoggedIn) {
            return;
        }
        var params = {
            siteUID: siteUID, timestamp: timestamp, signature: signature,
            callback: onGigyaNotifyLogin,
            context: "notifyLogin"
        }
        gigya.services.socialize.notifyLogin(gigyaConf, params);
    }

    function onGigyaNotifyLogin(res) {
        console.debug("onGigyaNotifyLogin", res);
        gigyaUser = res.user;
        if (res.status != 'OK') {
            console.error("onGigyaNotifyLogin error", res.statusMessage, res);
            return;
        }
        updateShareControls();
    }

    function onGigyaLogoutComplete(res) {
        console.debug("onGigyaLogoutComplete", res);
        getGigyaUserInfo();
    }

    function setGigyaHandlers(handlers) {
        gigyaHandlers = handlers;
        gigya.services.socialize.addEventHandlers(gigyaConf, handlers);
    }
    function logoutGigya() {
        console.debug("logout gigya");
        gigya.services.socialize.logout(gigyaConf, {callback: onGigyaLogoutComplete});
        setCookie("is_logged_in");
        setCookie("remember_me");
        updateShareControls();
        triggerPersonalize();
    }

    function getGigyaUserInfo() {
        gigya.services.socialize.getUserInfo(gigyaConf, {callback: onGigyaGetUserInfo});
    }

	function disconnectFromService(e) {
		Event.stop(e);
        var provider = this.provider;
		console.debug('provider: ', provider);
		gigya.services.socialize.disconnect(gigyaConf, {callback:disconnectResponse, provider: provider});
	}

	function disconnectResponse(response) {
		if (response['status'] == 'OK') {
			// hide share + disconnect link, show connect to link
			hide_flashes();
			getGigyaUserInfo(); // need to get updated user data, for updateShareControls() 
			updateShareControls();
		} else {
			discuss_flash('flash_error', 'Unable to disconnect from service. Please try again.');
		}
	}

</script>
