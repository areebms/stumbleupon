function closeHidden(){
	jQuery('#related_hidden').remove();
	jQuery('#related_hidden_button').remove();
}
var alreadyRan = false;
jQuery(document).ready(function(){
	jQuery(window).scroll(function(){
		var y = jQuery(window).scrollTop();
		//console.log(y);
		//console.log(alreadyRan);
		if(y > 800){
			if(!alreadyRan){
				jQuery('#related_hidden').slideDown('slow');
				alreadyRan = true;
			}
		}else{
			jQuery('#related_hidden').slideUp('slow');
			if(alreadyRan){
				jQuery('#related_hidden_button').slideDown('slow');
			}
		}
})});	
function showHidden(){
	jQuery('#related_hidden').slideDown();
	jQuery('#related_hidden_button').slideUp();
}
</script>
