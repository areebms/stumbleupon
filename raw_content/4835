<div id="mod_lower_newsletters">

<div id="newsletters" class="module radius_3">

		<p class="newsletterTitle">Subscribe to the Apps & iPods Newsletter <span class="frequency">- weekly</span></p>
		
		<form name="newslettermodule" id="newsletterModuleForm" method="post" action="/newsletters/pcw_module_processor">
			<input type="hidden" name="sourceid" id="sourceid" value="1" />
			<input type="hidden" name="subsource" id="subsource" value="NL_MODULE" />
			<input type="hidden" name="submitted" id="submitted" value="show" />
			<input type="hidden" name="caller" id="caller" value="nav" />
			<input type="hidden" name="prodid" value="598" />
			
            <div class="submitField">
                <div class="textField">
                    <input class="field" name="email" value="Enter e-mail" onFocus="clearFld(this);" type="text">
                </div>
                <input type="submit" value="Go" class="submitBtn" onClick="submitNewsletterSubscription();return false;">
            </div>
		</form>
		<a class="moreLink" href="/newsletters/index" rel="nofollow">See All Newsletters &raquo;</a>
	
	<script>
		function submitNewsletterSubscription(){
			var postData=$("#newsletterModuleForm").serialize();
			if(postData.indexOf("prodid")==-1){
				alert("Please Select at least one Newsletter");
				return false;
			}
			var requestOptions={
						url: '/newsletters/pcw_module_processor',
						data: postData,
						success: function(data,textStatus){
								$("#newsletters").fadeOut("slow",function(){
										$(this).html(data).fadeIn("slow");
									})
							}
				};
			$.ajax(requestOptions)
		}	
	</script>

</div> <!--  END #newsletters -->

</div>



</div> <!-- END .rightWrap -->

