<p id="p2container" style="margin-top: 0; margin-bottom: 0px; padding-top: 0; width: 424px;"><a href="http://videos.ksl.com/video/widget/10947928.jpg"><img src="http://videos.ksl.com/video/widget/10947928.jpg" border=0></a><br><br>To view this story, you need to download the latest version <a href="http://www.macromedia.com/go/getflashplayer"><b>Flash Player</b></a>.</p>

<script type="text/javascript">

var videocount = 0;
var vopen = false; // opened in omniture
var vplaying = false; // currently playing
var vname; // video title
var vitem; // playlist item
var vcur = undefined;
var pobj; // playlist item object


function thisMovie(swf) {
  if(navigator.appName.indexOf("Microsoft") != -1) {
    return window[swf];
  } else {
    return document[swf];
  }
};
function insertTile(obj)
{
	var t = document.getElementById('preroll-tile');
	var d = document.getElementById('preroll-tile-div');
	var a = document.getElementById('preroll-tile-a');
	var fn;
	var tile;

	//alert(obj.file);

	a.href = obj.link;
	d.style.display = 'block';

	fn = obj.file;
    tile = fn.substr(0, fn.length-4) + '.gif';
	t.src = tile;
}

function dumpProps(obj, parent) {
	var str = '';
	for(var i in obj) {
		str += i+", ";
	}
	alert(str);
}

function getUpdate(typ,pr1,pr2,swf)
{
	var s;

	if(typ == 'state') {
		if(pr1 == 0) {	// pause
			if(vplaying == true) {
				s = s_gi('bonintkslcom');
				if(s) { s.Media.stop(vname, vcur); }
				vplaying = false;
			}
		}
		else if(pr1 == 2) {	// play
			if(vopen == false) {
				s = s_gi('bonintkslcom');
				if(s) { s.Media.open(vname, pobj.duration, 'KSL Widget Player'); }
				if(s) { s.Media.play(vname, 0); }
				vplaying = true;
				vopen = true;
				
				// show the tile ad, if we're playing a pre-roll
				if(pobj.title == 'Commercial') {
					insertTile(pobj);
				}
				else {
					videocount = videocount+1;
					document.cookie = 'kslvideo='+videocount+'; domain=ksl.com; expires=Sun, 01 Jan 2012 00:00:00 GMT; path=/';
				}
			}
			else {
				// omniture tracking
				s = s_gi('bonintkslcom');
				if(s) { s.Media.play(vname, vcur); }
				vplaying = true;
			}
		}
		else if(pr1 == 3) {	// end of video
			s = s_gi('bonintkslcom');
			if(s) { s.Media.stop(vname, vcur); }
			if(s) { s.Media.close(vname); }
			vplaying = false;
			vopen = false;
		}
	}
	else if(typ == 'item') {
		vitem = pr1;
		vplaying = false;
		vopen = false;
		vcur = 0;

		//dumpProps(thisMovie(swf));
		//alert(thisMovie(swf).itemData);
		//alert(thisMovie(swf).itemData(vitem));

        // get the relevant video data and save it
		pobj = thisMovie(swf).itemData(vitem);
		//alert('devin');
			
		if(pobj.title == 'Commercial') {
			vname = pobj.file;
		}
		else {
			vname = 'Utah scientist makes breakthrough in mental illness research';
		}
	}
	else if(typ == 'time') {
		// if the current time is more than 2 seconds
		// different from the previous time, then there
		// was probably a seek, so we should tell
		// Omniture
		if(Math.abs(vcur-pr1) > 2 && vplaying == true) {
			s = s_gi('bonintkslcom');
			if(s) { s.Media.stop(vname, vcur); }
			if(s) { s.Media.play(vname, pr1); }
		}
		vcur = pr1;
	}
}

//var so = new SWFObject('http://pandora.bonnint.net/video/mediaplayer.swf','p1','635','244','7');
var so = new SWFObject('http://www.ksl.com/flash/kslplayer-1.00.swf','p1','635','244','7');
//var so = new SWFObject('http://stage-v2.ksl.com/flash/kslplayer-1.00.swf','p1','635','244','7');
so.addParam('allowscriptaccess', 'always');
so.addParam('allowfullscreen', 'true');
so.addParam('wmode', 'transparent');
so.addVariable('width', '635');
so.addVariable('displaywidth', '420');
so.addVariable('height', '244');
//so.addVariable('file', 'http://pandora.bonnint.net/video/playlists/p_10947928.xml');
so.addVariable('file','http://pandora.bonnint.net/video/plist2.php%3Fsid=10947928%26views=%26noad=0');
//so.addVariable('image', 'http://videos.ksl.com/video/widget/10947928.jpg');
so.addVariable('javascriptid', 'p1');
so.addVariable('enablejs', 'true');
so.addVariable('showdigits', 'true');
so.addVariable('showicons', 'true');
so.addVariable('autostart', 'false');
so.addVariable('repeat', 'false');
so.addVariable('shownavigation', 'true');
so.addVariable('screencolor', '#FFFFFF');
so.addVariable('repeat', 'list');
so.addVariable('smoothing', 'true');
so.addVariable('linkfromdisplay', 'false');
//so.addVariable('linktarget', '_top');
so.addVariable('playlist', 'right');
so.addVariable('thumbsinplaylist', 'false');
so.addVariable('recommendations','http://pandora.bonnint.net/video/most_recent.xml');
so.addVariable('callback', 'http://pandora.bonnint.net/video/post.php');
so.write('p2container');
</script>

</div>

