function ReverseDisplay2(show) {

	switch(show) {

case "read":
			document.getElementById("list_video").style.display = "none";
			document.getElementById("list_comments").style.display = "none";
			document.getElementById("list_read").style.display = "block";
			document.getElementById("list_dnews").style.display = "none";
			document.getElementById("dnews").className='';
			document.getElementById("read").className='on';
			document.getElementById("video").className='';
			document.getElementById("comments").className='';
			break;
		case "video":
			document.getElementById("list_video").style.display = "block";
			document.getElementById("list_comments").style.display = "none";
			document.getElementById("list_read").style.display = "none";
			document.getElementById("list_dnews").style.display = "none";
			document.getElementById("dnews").className='';
			document.getElementById("read").className='';
			document.getElementById("video").className='on';
			document.getElementById("comments").className='';
			break;
		case "comments":
			document.getElementById("list_video").style.display = "none";
			document.getElementById("list_comments").style.display = "block";
			document.getElementById("list_read").style.display = "none";
			document.getElementById("list_dnews").style.display = "none";
			document.getElementById("dnews").className='';
			document.getElementById("read").className='';
			document.getElementById("video").className='';
			document.getElementById("comments").className='on';
			break;
		case "dnews":
			document.getElementById("list_video").style.display = "none";
			document.getElementById("list_comments").style.display = "none";
			document.getElementById("list_read").style.display = "none";
			document.getElementById("list_dnews").style.display = "block";
			document.getElementById("dnews").className='on';
			document.getElementById("read").className='';
			document.getElementById("video").className='';
			document.getElementById("comments").className='';
			break;

	}
}
</script>

