<!--jQuery code to toggle between short and long answers-->
$(document).ready(function(){

	var showmoreinfo = "Show full answer";
	var showlessinfo = "Show short answer";
	var morelink = '<p style="text-align:left;"><a href="#" class="toggle" style="font-weight:bold;">' + showmoreinfo + '</a></p>';
	
	$('div.long').each( function(i) {
		$(this).after(morelink);
		$(this).hide();
	});
	
	$('a.toggle').click(function() {
		$(this).parent().prev('.long').toggle();
		var showinfo = ($(this).text() == showmoreinfo) ? showlessinfo : showmoreinfo;
		$(this).text(showinfo);
		return false;
	});
	
});
<!--End jQuery code-->
// end hiding javascript -->
</script>


