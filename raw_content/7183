<script>
function validateWebForm() {
	var email_reg_exp = /^([a-zA-Z0-9_\.\-])+\@(([a-zA-Z0-9\-]{2,})+\.)+([a-zA-Z0-9]{2,})+$/;
	var error = "";

    var email = document.getElementById('email').value;

    var lang = document.getElementById('lingua').value;

    if (!email_reg_exp.test(email) || (email == "")) {
        error += "- Newsletter email is a required field \n";
	}

    if(lang == ""){
        error += "- Insert language \n";
    }else{
        if( lang == "EN"){
            document.getElementById('id_wfc').value =  "720000673569";
            document.getElementById('id_lang').value = "en";
        }
        if( lang == "IT"){
            document.getElementById('id_wfc').value =  "920000675385";
            document.getElementById('id_lang').value = "it";
        }

        if( lang == "ES"){
            document.getElementById('id_wfc').value =  "1020000674511";
            document.getElementById('id_lang').value = "es";
        }

        if( lang == "PT"){
            document.getElementById('id_wfc').value =  "1120000679970";
            document.getElementById('id_lang').value = "pt";
        }
        if( lang == "FR"){
            document.getElementById('id_wfc').value =  "1220000673147";
            document.getElementById('id_lang').value = "fr";
        }

        if( lang == "DE"){
            document.getElementById('id_wfc').value =  "1320000679810";
            document.getElementById('id_lang').value = "de";
        }

        if( lang == "RU"){
            document.getElementById('id_wfc').value =  "1420000674782";
            document.getElementById('id_lang').value = "ru";
        }

        if( lang == "JP"){
            document.getElementById('id_wfc').value =  "1520000675320";
            document.getElementById('id_lang').value = "jp";
        }


    }

    if(error != "") {
		alert("An error has occurred!\n\n" + error);
		return false;
	}
	else {
       return true;
    }

}

</script>
