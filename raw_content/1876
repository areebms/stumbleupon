      if ( "/membership/" ){
        var eventManager = registration.manager.events.getEventManagerInstance();
        registration.manager.config.setConfig( "signUpListener", "reg-signup-override" );
        eventManager.addListener( "reg-signup-override", function(){
          window.document.location.href = "/membership/"; 
        });
      }
      registration.manager.nav = registration.component.navigation.NavigationController.getInstance({
        navigationContainerSelector: "#ssorNavHeader", 
        navigationParams: {
          signUpHandler: function(){
            if ( "/membership/" ){
              window.document.location.href = "/membership/"; 
            } else { 
              registration.manager.showRegistrationDialog();
            }
          },
          originHost: registration.manager.config.getConfig('assetHostname') || "",
          navigationBackgroundColor: "#000000",
          navigationTextColor: "",
          skipNewsletters: "0",
          userName: registration.utils.cookies.getValue( "c_unm" ) || ""
        }
      });
    });
    















