amznJQ.onCompletion("amznJQ.criticalFeature", function() {
amznJQ.available("jQuery", function() {
    var taf = jQuery("#tell-a-friend");
    taf.children("[data-height]").click(function() {
        var height = jQuery(this).attr("data-height");
        window.open(this.href, "_blank", "location=yes,width=700,height=" + height);
        return false;
    });
    
    amznJQ.addLogical("share-with-friends-js-new", ["http://z-ecx.images-amazon.com/images/G/01/nav2/gamma/share-with-friends-js-new/share-with-friends-js-new-share-12795._V1_.js"]);
    amznJQ.available("share-with-friends-js-new", function() {
        taf.children(".email").amazonPopoverTrigger({
            closeText: taf.attr("data-close"),
            closeEventExclude: "CLICK_TRIGGER",
            destination: taf.attr("data-dest"),
            group: "taf",
            location: "centered",
            title: taf.attr("data-title"), 
            width: 530,
            onShow: function(popover) {
                jQuery(document).click(); 
                window.amz_taf_popover = popover;
            }
        });
    });
});
});
