 </div>

<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3CDIV%3EFrom%20casual%20meals%20to%20springtime%20brunch%2C%20classy%20dinners%2C%20and%20summer%20grilling%20parties%2C%20learn%20how%20to%20bring%20in%20wines%2C%20cocktails%2C%20and%20beers%20with%20flexible%20pairings.%20Over%20100%20recipes%20jam-packed%20with%20tips%20and%20suggestions%20for%20every%20occasion%20from%20a%20Sunday%20supper%20to%20a%20savory%20brunch%20to%20a%20ten-minute%20feast%20offer%20up%20inventive%20dishes%20and%20drinks%20like%20grilled%20watermelon%20salad%3B%20a%20bloody%20Mary%20bar%20with%20all%20the%20fixins%3B%20%26%23147%3Boystas%20Rockafella%3B%26%238221%3B%20ricotta%20gnocchi%3B%20fava%20beans%20with%20mint%20and%20bacon%3B%20rack%20of%20lamb%3B%20black%20pepper%20martinis%3B%20and%20chocolate%20and%20salt%20torte.%20The%20pairings%20are%20undemanding%20and%20educational%2C%20designed%20to%20teach%20the%20reader%20how%20to%20find%20great%20drinks%20for%20every%20meal.%3C%2FDIV%3E",
	    isHeightCalculated = 0,
	    bookDescriptionInitialHeight,
	    minBookDescriptionInitialHeight = 30,
	    options = {};
	    
    function resizeCallback() {
    	if(!isHeightCalculated){
        	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
    		var imageBlockOffsetTop = document.getElementById("imageBlock").offsetTop;
    		var imageBlockHeight = document.getElementById("imageBlock").offsetHeight;
    		bookDescriptionInitialHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
    		if(bookDescriptionInitialHeight < minBookDescriptionInitialHeight) {
    	        bookDescriptionInitialHeight = minBookDescriptionInitialHeight;
    		}
    		isHeightCalculated = 1;
        }
    	
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionInitialHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionInitialHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).bind('resize', function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

                    
    

</div>



        
    
        
        

        




