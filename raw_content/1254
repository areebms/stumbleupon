<script type="text/javascript">
  // <![CDATA[
  
  // setTimeout("showFlyOut1('navFlyOut')",1000);
	showFlyOut1('navFlyOut');
  function showFlyOut1(linkParameter) {
 
    // This is for Nav FlyOut http://cdn.womenshealthmag.com/themes/womenshealth/images/header/wh-flyout-arrow-right.gif
    if (linkParameter == "navFlyOut") {
      document.writeln([
		"<div id='flyoutPromoArea' style='z-index: 9999999;'>",
    
			"<div class='promoFlyoutArrowDiv'><img src='http://www.womenshealthmag.com/files/wh6_uploads/wh-flyout-arrow-right_0.gif' height='8' width='4' /><br/></div>",
			"<div class='flyoutPromo'>",
				"<div style='position: absolute;'>",
					"<div class='promo1'>",
						"<a href='http://www.wh15minuteworkoutdvd.com/vid?cm_mmc=wh.com-_-flyout-_-dynamicflyout-_-wh_15min_dvd&amp;keycode=217808' ><img src='http://www.womenshealthmag.com/files/wh6_uploads/flyout-15min-01.gif' width='168px' 	height='146px;' ></a>",
					"</div>",
					"<div class='promo2'>",
						"<a href='http://www.wh15minuteworkoutdvd.com/vid?cm_mmc=wh.com-_-flyout-_-dynamicflyout-_-wh_15min_dvd&amp;keycode=217808' ><img src='http://www.womenshealthmag.com/files/wh6_uploads/flyout-15min-02.gif' width='168px'  	height ='98px'></a>",
					"</div>",
					"<div class='promo3'>",
						"<a href='http://www.wh15minuteworkoutdvd.com/vid?cm_mmc=wh.com-_-flyout-_-dynamicflyout-_-wh_15min_dvd&amp;keycode=217808' ><img src='http://www.womenshealthmag.com/files/wh6_uploads/flyout-15min-03.gif' width='168px'  	height='83px' ></a>",
					"</div>",					
				"</div>", 
			"</div>",
		"</div>"
	  ].join("\n"));
	

  	//ie 7 and below css
  	document.writeln([
		"<!--[if LTE IE 7]>",
		"<style type=\"text\/css\">",
			"#flyoutPromoArea {",
				"margin:-18px 0 0 -384px;",
				"z-index: 10;",
			"}",
			"img.promoFlyoutArrow {",
				"position:absolute;", 
				"margin-top: 32px;",
				"margin-left: -4px;",
			"}",
		"<\/style>",
		"<![endif]-->"
  	].join("\n"));
	
  	var hover = jQuery('#flyoutPromoArea');
  	$('#whAdvertiser').hover(function() { 
    		hover.show();
    	}, function() {
    		hover.hide();
    	});
	
    	$('#whAdvertiser').append(hover);
    	//Below if is for 'Subscribe Nav Flyout'
    }
  }

  var wh_subscribe_dropdown_callstate = 0;
	
  function displaysubscribe(e) {
  	//hideMenuImmed();
  	var gmnuObj = this;
  	jQuery(this).attr('ON','1');
  	jQuery(this).removeAttr('OFF');
  	setTimeout(function() {
  		var mnuObj = gmnuObj;
  		if (jQuery(mnuObj).attr('ON') == '1') {
  			jQuery('#subscribePromoArea').css('display','block');
  			jQuery(mnuObj).removeAttr('ON');
  		}
  	}, 500);
  }

  function hidesubscribe(e) {
  	wh_subscribe_dropdown_callstate = 0;
  	var gmnuObj = this;
  	jQuery(this).attr('OFF','1');
  	jQuery(this).removeAttr('ON');
  	setTimeout(function() {
  		var mnuObj = gmnuObj;
  		if((wh_subscribe_dropdown_callstate == 0) && (jQuery(mnuObj).attr('OFF') == '1')) {
  			jQuery('#subscribePromoArea').css('display','none');
  			jQuery(mnuObj).removeAttr('OFF');
  		}
  	}, 0);
  }
	
  function setCallOnState(e) {
    wh_subscribe_dropdown_callstate = 1;
  }

  function setCalloffState(e) {
  	wh_subscribe_dropdown_callstate = 0;
  	var mnuId = "#subscribePromoArea";
  	jQuery(mnuId).trigger("mouseout");
  }

  // ]]>
</script>

