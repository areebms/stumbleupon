$(document).ready(function() {
    ribbonheight = $("#ribbon").height();
    if (ribbonheight > 0) {
        ribbonheight = ribbonheight + 10;
    }
    textribbonheight = $("#textribbon").height();
    if (textribbonheight > 0) {
        textribbonheight = textribbonheight + 14;
    }
    col1height = $("#col1").height();
    col2height = $("#col2").height();
    col3height = $("#col3").height() - ribbonheight - textribbonheight; 
    maxheight = col1height;
    if (col2height > maxheight) {
        maxheight = col2height;
    }
    if (col3height > maxheight) {
        maxheight = col3height;
    }
    if ($.browser.msie && $.browser.version == 6.0) {
        $("#col1").css({'height': maxheight});
        $("#col2").css({'height': maxheight});
        $("#col3").css({'height': maxheight + ribbonheight + textribbonheight});
    }
    $("#col1").css({'min-height': maxheight});
    $("#col2").css({'min-height': maxheight});
    $("#col3").css({'min-height': maxheight + ribbonheight + textribbonheight});
