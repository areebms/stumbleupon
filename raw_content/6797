GEL.thepage.initializer.addInitRoutine({ 
	name: "followusmodule", 
	callback: initFollowUs, 
	priority: 120, 
	namespace: ["widget.GELTabs", "remoting"]
}); 
function initFollowUs(){ 
	var params= { 
			changeEvent: 'click',
			autoRotate:false,
			modalClass: function(){ 
				return new GEL.Element.Modal({
					width: "140px",
					height: "260px",
					image: "none",
					klass: "loadingwrap"
				}); 
		}, 
		activeIndex:0,
		contentURLList: [ 
		]
	}; 
	params.contentURLList.push("/ajax/60/p1=subscribe/followremote.xhtml"); 
	params.contentURLList.push("/ajax/60/p1=twitter/followremote.xhtml");
	params.contentURLList.push("/ajax/60/p1=facebook/followremote.xhtml");
	params.contentURLList.push("/ajax/60/p1=mobile/followremote.xhtml");
	params.contentURLList.push("/ajax/60/p1=rss/followremote.xhtml");
	params.contentURLList.push("/ajax/60/p1=email/followremote.xhtml");
	params.contentURLList.push("/ajax/60/p1=sms/followremote.xhtml"); 
	GEL.thepage.followus = new GEL.widget.GELTabs("ody-followusmodule",params);  
	GEL.thepage.followus.init();	 	
}
</script>
