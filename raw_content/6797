(function(){ 
GEL.thepage.initializer.addInitRoutine({ 
	name: "frontpagetabs", 
	callback: initFrontpageTabs, 
	namespace: ["widget.GELTabs", "remoting", "widget.TrackTabs"], 
	priority: 150
}); 
function initFrontpageTabs(){
	GEL.log("init FrontPage Tabs"); 
	GEL.thepage.frontpagetabs= 
		new GEL.widget.GELTabs("frontpagetabs", { 
			changeEvent: 'click',
			autoRotate:false,
			activeIndex:0,
			modalClass: function(){ 
				return new GEL.Element.Modal({
					width: "66px",
					height: "66px",
					image: "/graphics/ajax_loading_big.gif"
				}); 
			}
		}); 
	GEL.thepage.frontpagetabs.init();	 	
	var _w = 
		GEL.thepage.TrackTabs= 
			new GEL.widget.TrackTabs("frontpagetabs")
	; 
	_w.init(); 
}
GEL.thepage.initializer.addInitRoutine({ 
	name: "commentcount", 
	callback: initSocial, 
	namespace: ["social.pluck"], 
	priority: 150
}); 
function initSocial(){
GEL.social.pluck.getCommentCount();
}
})();
