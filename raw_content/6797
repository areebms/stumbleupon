<script type="text/javascript"> 
			var ctime;
			var mouse =
			{
				// returns true if c is child of p, or c is parent
				isChildOf: function (p, c)
				{
					p = document.getElementById('large-flash-pop-8474');
					while (c && c !== p) { c = c.parentNode; }
					return c === p;
				},
				// returns true only if mouse was in object and just left
				leave: function (o, e)
				{
					if (typeof(e) == "undefined"){ e = window.event; }
					return !this.isChildOf(o, e.relatedTarget || e.toElement);
				}
			}
			function ShowAd8474()
			{
				var div = document.getElementById("large-flash-pop-8474");
				div.style.display = "block";
				var div2 = document.getElementById("small-flash-pop-8474");
				div2.style.display = "none";
				document.all.buffwhelaniframe.src="http://www.michigan.com/click/redirect.php?o=feldmanopenFP&url=about:blank"; 
			}
			function HideAd8474(e)
			{
				evt = e || window.event;
				if(mouse.leave(this, evt)){
					var div = document.getElementById("large-flash-pop-8474");
					div.style.display = "none";
					var div2 = document.getElementById("small-flash-pop-8474");
					div2.style.display = "block";
				}
			}
		</script>
