  window['checkCookie']=(function(){ 
var _={};
for(
	var
		c=document.cookie.split(/\s*;\s*/),
		i=0,
		l=c.length,
		a=c[i].split("=")
	;
	a=i<l&&c[i].split("=")
	;
	i++
)_[a[0]]= a[1]; 
return function (cookieName){return _[cookieName];}
})();
