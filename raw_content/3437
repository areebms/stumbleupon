<script language="Javascript">

function ToggleIETVNavButtonBg(inButtonName, inOnOrOff) {
	GetObject(inButtonName + '_content').className = 'ietvbuttoncontent' + inOnOrOff;
	GetObject(inButtonName + '_contentouter').className = 'ietvbuttoncontentouter' + inOnOrOff;
	if (inButtonName == 'ietvnav_channel' || inButtonName == 'ietvnav_vblogger') { 
		if (inOnOrOff == 'on') {
			PopMenu(inButtonName + '_arrowpopmenu_toparrow',(returnPosXofObj(GetObject(inButtonName)) + 0),(returnPosYofObj(GetObject(inButtonName)) + 36));
			lockPoppedMenu = true;
			lockedPoppedMenuObjName = inButtonName + '_arrowpopmenu_toparrow';
		} else {
			lockPoppedMenu = false;
			lockedPoppedMenuObjName = '';
			PopMenuOff(inButtonName + '_arrowpopmenu_toparrow', -500, 38);
		}
	}
}

</script>

