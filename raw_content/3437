<script language="Javascript">

var ietvrtcolmorefromSliderHandler = new LRSliderHandler();

ietvrtcolmorefromSliderHandler.thisSliderElementName = 'ietvrtcolmorefromSliderHandler';
ietvrtcolmorefromSliderHandler.sliderElementCount = 5;
ietvrtcolmorefromSliderHandler.sliderElementSize = 288;
ietvrtcolmorefromSliderHandler.sliderObjectViewsize = 1;
ietvrtcolmorefromSliderHandler.sliderRightLimit = ietvrtcolmorefromSliderHandler.sliderElementSize * ((ietvrtcolmorefromSliderHandler.sliderElementCount - ietvrtcolmorefromSliderHandler.sliderObjectViewsize) * -1) + 1;
ietvrtcolmorefromSliderHandler.currentSliderElement = 1;
ietvrtcolmorefromSliderHandler.sliderObjectId = 'ietvrtcolmorefrom_outside';
ietvrtcolmorefromSliderHandler.sliderDoSlide = false;

var ietvrtcolmorefromLeftArrowButtonMouseoutImgSrc = 'http://img.lightreading.com/internetevolution/intevol_videonav_leftarrow_lightgray.gif';
var ietvrtcolmorefromRightArrowButtonMouseoutImgSrc = 'http://img.lightreading.com/internetevolution/intevol_videonav_rightarrow_darkgray.gif';

ietvrtcolmorefromSliderHandler.LRSliderNavGraphicsHandlerSlideLeft = function() {
	// change the input box value
	GetObject('ietvrtcolmorefrom_navform_selector').value = this.currentSliderElement;
	// now deal with the images
	
	var currentImage = GetObject('ietvrtcolmorefrom_rightarrowbutton');
	// if we're at currentSliderElement < sliderElementCount make sure the right button is active
	if (this.currentSliderElement < this.sliderElementCount) { 
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_rightarrow_darkgray.gif';
	} else {
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_rightarrow_lightgray.gif';
	}
	ietvrtcolmorefromRightArrowButtonMouseoutImgSrc = currentImage.src;

	// if sliding left, change when currentSliderElement gets to 1
	currentImage = GetObject('ietvrtcolmorefrom_leftarrowbutton');
	if (this.currentSliderElement == 1) { 
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_leftarrow_lightgray.gif';
	} else {
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_leftarrow_darkgray.gif';
	}
	ietvrtcolmorefromLeftArrowButtonMouseoutImgSrc = currentImage.src;
	
}

ietvrtcolmorefromSliderHandler.LRSliderNavGraphicsHandlerSlideRight = function() {
	// change the input box value
	GetObject('ietvrtcolmorefrom_navform_selector').value = this.currentSliderElement;
	// now deal with the images
	
	var currentImage = GetObject('ietvrtcolmorefrom_leftarrowbutton');
	// if we're at currentSliderElement > 1 make sure the left button is active
	if (this.currentSliderElement > 1) { 
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_leftarrow_darkgray.gif';
	} else {
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_leftarrow_lightgray.gif';
	}
	ietvrtcolmorefromLeftArrowButtonMouseoutImgSrc = currentImage.src;
	
	currentImage = GetObject('ietvrtcolmorefrom_rightarrowbutton');
	// sliding right, change when currentSliderElement gets to this.sliderElementCount
	if (this.currentSliderElement == this.sliderElementCount) {
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_rightarrow_lightgray.gif';
	} else {
		currentImage.src = 'http://img.lightreading.com/internetevolution/intevol_videonav_rightarrow_darkgray.gif';
	}
	ietvrtcolmorefromRightArrowButtonMouseoutImgSrc = currentImage.src;
}

</script>

