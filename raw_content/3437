function intevol_topclannav_analyticsclan_mouseoverFunction () {
	GetObject('intevol_topclannav_analyticsclan').src = 'http://img.deusm.com/internetevolution/intevol_topclannav_analyticsclan_over.gif';
	PopMenu('intevol_topclannav_analyticsclan_arrowpopmenu_toparrow',(returnPosXofObj(GetObject('intevol_topclannav_analyticsclan')) + 0 - 4),(returnPosYofObj(GetObject('intevol_topclannav_analyticsclan')) + GetObject('intevol_topclannav_analyticsclan').height + 2));
}
function intevol_topclannav_analyticsclan_mouseoutFunction () {
	GetObject('intevol_topclannav_analyticsclan').src = 'http://img.deusm.com/internetevolution/intevol_topclannav_analyticsclan.gif';
