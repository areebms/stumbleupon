<script language="javascript" type="text/javascript">
    /* Showcase preloader */
    $j(document).ready(function () {
        var listingsIndex = getCookie("news24Listings");
        var randomnum = Math.floor(Math.random() * 5);
        if (listingsIndex != null && listingsIndex != "") {
            if (listingsIndex == randomnum) {
                for (var i = 0; i <= 10; i++) {
                    randomnum = Math.floor(Math.random() * 5);
                    if (listingsIndex != randomnum) {
                        break;
                    }
                }
            }
            setCookie("news24Listings", randomnum);
        }
        else {
            setCookie("news24Listings", randomnum);
        }

        $j("#accordion").show();
        $j("#accordion").accordion({
            event: 'mouseover',
            autoHeight: false,
            active: randomnum
        });
        $j("#the_accordion").removeClass("tabLoader");
    });

    function setCookie(c_name, value) {
        $j.cookie(c_name, value, { expires: null, path: '/', domain: document.domain });
    }

    function getCookie(c_name) {
        if ($j.cookie(c_name) != null) {
            return $j.cookie(c_name);
        }
        return "";
    }
</script>
