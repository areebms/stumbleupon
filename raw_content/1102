<script type="text/javascript">
    $j('document').ready(function () {
        scrollalert();
        $j('#imgAjaxLoad').hide();
    });

    function scrollalert() {
        var elem = $j('#scrollbox');

        var inner = $j('#scrollbox > #content');
        if (elem.length > 0) {
            if (elem.length > 0 && elem.scrollTop && inner.height() <= (elem.scrollTop() + elem.height() + 20)) {
                $j('#imgAjaxLoad').show();
                var result = TwentyFour.Sites.News.UI.Site.RelatedArticleTagAjax.BuildArticleListByTag($j('#hfTag').val(), $j('#hfTagGroup').val(), '5', '52b43f9e-f4a3-4816-adda-79e07ad99941', $j('#hfBottomIndex').val(), $j('#hfSelectedArticleIndex').val(), 'Down', onSuccessBottom, onFail);

            }

            setTimeout('scrollalert();', 500);
        }
    }

    function onSuccessBottom(result) {
	    var bottomIndex = parseInt($j('#hfBottomIndex').val());

	    if ((result.value != "") && (result.value != null)) {
	        $j('#hfBottomIndex').val(bottomIndex + 5)

	        setTimeout("$j('#imgAjaxLoad').hide();", 1000);
	        var newList = $j('#RelatedLinks').html() + result.value;
	        $j('#RelatedLinks').html(newList);
	    }
	    else {
	        setTimeout("$j('#imgAjaxLoad').hide();", 1000);
	        $j('#hfBottomIndex').val(-1)
	    }
	}

	function onSuccessTop(result) {
	    var topIndex = parseInt($j('#hfTopIndex').val());

	    if ((result.value != "") && (result.value != null)) {
	        $j('#hfTopIndex').val(topIndex - 5)

	        setTimeout("$j('#imgAjaxLoad').hide();", 1000);
	        var newList = result.value + $j('#RelatedLinks').html();
	        $j('#RelatedLinks').html(newList);
	        $j("#scrollbox").scrollTo('10');
	    }
	    else {
	        setTimeout("$j('#imgAjaxLoad').hide();", 1000);
	        $j('#hfTopIndex').val(-1)
	    }
	}

	function onFail() {
	    
	}
</script>

