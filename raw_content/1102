<a href="/SocialSharingPopup.aspx" class="iframe socialSharePopup" style="display:none;"></a>

    
<script type="text/javascript">
    var PageURL;
    var tURL;
    var NewPageNo;
    var ArticleName = 'Lazarus pill miracle for E Cape man';
    var twitterURL;
    var bitLyURL;
    var defaults;
    var currentUser,
        screenName,
        profileImage,
        profileImageTag;
    //keeps checking if you logon or off twitter
    twttr.anywhere(function (T) {
        //
        T.bind("authComplete", function (e, user) {
        });
        //when user disconnects
        T.bind("signOut", function (e) {

            DeleteCookie("TwitterAuth", "", -1);
            DeleteCookie("TwitterAuth_temp", "", -1);
            window.location.reload(true);
        });
    });
    $j(document).ready(function () {

        //ShortURLS();

        (function (S) {

            // set up default options
            defaults = {
                version: '2.0.1',
                login: '24com', //username
                apiKey: 'R_ca79efd5a0978fb80d1d853bac1dda83', //apikey
                history: '0',
                longUrl: getPathFromUrl(window.location.href)
            };

            // Build the URL to query
            bitLyURL = "http://api.bit.ly/shorten?"
            + "version=" + defaults.version
            + "&longUrl=" + defaults.longUrl
            + "&login=" + defaults.login
            + "&apiKey=" + defaults.apiKey
            + "&history=" + defaults.history
            + "&format=json&callback=?";
        })(jQuery);
        CheckStatus();

    });
    function getPathFromUrl(url) {
        return url.split("?")[0];
    }
    function CheckStatus() {

        // Use the bit.ly API
        $j.getJSON(bitLyURL, function (data) {

            // use short URL
            PageURL = (data.results[defaults.longUrl].shortUrl);
            $j("#lblPageURL").text(PageURL);
            twitterURL = $j("#lblPageURL").text();
            //ArticleName = stripCharacters(replaceCharacters());
            $j("#taMessage").val(ArticleName + " - " + twitterURL + " via " + "@news24");
            var myLength = $j("#taMessage").val().length;

            $j("#remaining").text(140 - myLength);

        });
        if (getCookies("TwitterAuth") != null) {
            var PageNo = $j("#PageNo").text();
            $j(".imgAjaxLoad").show();
            TwentyFour.Sites.News.UI.Site.TwitterAjax.GetUserFeedHtml(PageNo, function (res) { GetUserFeedHtmlCallback(res); });
            TwentyFour.Sites.News.UI.Site.TwitterAjax.GetUserImage(function (res) { GetUserImageCallback(res); });

            $j("#logout").show();
            $j("#twitter_logo").hide();
            $j("#twitter_logo").removeClass("twithead");
            $j("#twitterBlock").removeClass("col299").addClass("col299 LoggedIn");
        }
        else {
            $j("#twitter_logo").show();
            $j("#logout").hide();
            $j("#Signon").show();
            $j("#twitterBlock").removeClass("col299 LoggedIn").addClass("col299");
            $j(".twithead").hide();
            $j(".imgAjaxLoad").hide();
        }
    }
    function shortener() {
    }
    function GetTokens() {

        if (getCookies("TwitterAuth") == null) {
            //
            TwentyFour.Sites.News.UI.Site.TwitterAjax.GetRequestToken(function (res) { GetRequestTokenCallback(res); });
            $j(".imgAjaxLoad").show();
        }
        else {
            //ArticleName = stripCharacters(replaceCharacters());
            TwentyFour.Sites.News.UI.Site.TwitterAjax.TweetMessage($j("#taMessage").val(), function (res) { TweetMessageCallback(res); });
            $j(".imgAjaxLoad").show();

        }
    }
    //returns the name of the Document you currently viewing
    function returnDocument() {
        var file_name = document.location.href;
        ArticleName = (file_name.indexOf("?") == -1) ? file_name.length : file_name.indexOf("?");
        ArticleName = file_name.substring(file_name.lastIndexOf("/") + 1);
        ArticleName = ArticleName.replace('-', ' ');
        return ArticleName;
    }

    //gets 1st token from twitter
    function GetRequestTokenCallback(response) {
        if (response.value != null) {

            window.open("https://www.twitter.com" + response.value, "mywindow", "location=1,status=1,scrollbars=1, width=250,height=200");
            setTimeout("setPermCookieValue()", 1000);

        }
        else {
            GetTokens();
        }
    }

    //set a perminent cookie for the current logged in user
    function setPermCookieValue() {
        if (getCookies("TwitterAuth_temp") != null) {

            TwentyFour.Sites.News.UI.Site.TwitterAjax.CreatePermCookie(function (res) { CreatePermCookieCallback(res); });
        }
        else if (getCookies("TwitterAuth_temp") == null)
            setTimeout("setPermCookieValue()", 1000);
    }
    //request the user feed from twitter
    function CreatePermCookieCallback(res) {

        if (!res.error && res.value != 'error') {
            //ArticleName = stripCharacters(replaceCharacters());


            TwentyFour.Sites.News.UI.Site.TwitterAjax.TweetMessage($j("#taMessage").val(), function (res) { TweetMessageCallback(res); });
        }
    }
    function TweetMessageCallback(res) {
        $j("#taMessage").val("");
        var PageNo = $j("#PageNo").text(); //set page number;
        TwentyFour.Sites.News.UI.Site.TwitterAjax.GetUserImage(function (res) { GetUserImageCallback(res); });
        TwentyFour.Sites.News.UI.Site.TwitterAjax.GetUserFeedHtml(PageNo, function (res) { GetUserFeedHtmlCallback(res); });
        $j("#twitter_logo").removeClass("twithead");
        $j("#twitterBlock").removeClass("col299").addClass("col299 LoggedIn");
    }

    function GetUserImageCallback(res) {
        if (res.value != null) {

            $j("#UserProfilePic").html("<img height ='40' width='40'  src='" + res.value + "' />");
            $j("#twitter_logo").hide();

        }
        else {
            CheckStatus();
        }
    }
    function replaceCharacters() {
        var origString = returnDocument();
        var inChar = '-';
        var outChar = ' ';
        var newString = origString.split(inChar);
        newString = newString.join(outChar);
        return newString;
    }
    function stripCharacters(str) {
        str = str.replace('-', ' ');
        str = str.substring(0, str.length - 8);
        return str;
    }
    //writes user feed to page
    function GetUserFeedHtmlCallback(res) {
        //
        if (!res.error && res.value != 'error') {
            //response.value is the HTML built in the GetUserFeedHtml ajax method
            var headDiv = res.value;

            $j("#PageNo").text(NewPageNo);
            $j("#HomeTimeline").html(res.value);
            $j("#logout").show();
            $j("#twitter_logo").hide();
            $j("#twitter_logo").removeClass("twithead");
            $j("#twitterBlock").removeClass("col299").addClass("col299 LoggedIn");
            $j(".twithead").show();
            $j(".imgAjaxLoad").hide();
        }
    }
    // fieldname, warningname, remainingname, maxchars
    function CheckFieldLength(fn, wn, rn, mc) {
        var len = fn.value.length;
        if (len > mc) {
            fn.value = fn.value.substring(0, mc);
            len = mc;
        }
        document.getElementById(wn).innerHTML = len;
        document.getElementById(rn).innerHTML = mc - len;
    }
    function DeleteCookie(name, value, seconds) {

        if (typeof (seconds) != 'undefined') {
            var date = new Date();
            date.setTime(date.getTime() + (seconds * 1000));
            var expires = "; expires=" + date.toGMTString();
        }
        else {
            var expires = "";
        }
        document.cookie = name + "=" + value + expires + "; path=/";
    }

    // trims white spaces from text before using it in the tweetbox
    function trim(s) {
        s = s.replace(/(^\s*)|(\s*$)/gi, "");
        s = s.replace(/[ ]{2,}/gi, " ");
        s = s.replace(/\n /, "\n");
        return s;
    }

    //useed for setting the temp cookie
    function setCookie(c_name, exdays) {
        var exdate = new Date();
        exdate.setDate(exdate.getDate() + exdays);

        document.cookie = c_name;
    }

    //looks for the cookie
    function getCookies(name) {
        var h, z, u, ARRAYcookies = document.cookie.split(";");
        for (h = 0; h < ARRAYcookies.length; h++) {
            z = ARRAYcookies[h].substr(0, ARRAYcookies[h].indexOf("="));
            u = ARRAYcookies[h].substr(ARRAYcookies[h].indexOf("=") + 1);
            z = z.replace(/^\s+|\s+$/g, "");
            if (z == name) {
                return unescape(u);
            }
        }
    }


    //event that fires when 'prev' button is clicked
    function cmdPrev_Click() {
        var PageNo = $j("#PageNo").text();
        if (parseInt(PageNo) > 1) {
            NewPageNo = parseInt(PageNo) - 1;
            $j().geekGaTrackEvent('In-Page-Twitter', 'PrevPage', ArticleName);
            $j("#HomeTimeline").hide();
            $j(".imgAjaxLoad").show();
            TwentyFour.Sites.News.UI.Site.TwitterAjax.ChangePageNo(NewPageNo, function (res) { ChangePageNoCallback(res); });
        }
    }

    //event that fires when 'next' button is clicked
    function cmdNext_Click() {
        var PageNo = $j("#PageNo").text();
        if (parseInt(PageNo) < 10) {
            NewPageNo = parseInt(PageNo) + 1;
            $j().geekGaTrackEvent('In-Page-Twitter', 'NextPage', ArticleName);
            $j("#HomeTimeline").hide();
            $j(".imgAjaxLoad").show();
            TwentyFour.Sites.News.UI.Site.TwitterAjax.ChangePageNo(NewPageNo, function (res) { ChangePageNoCallback(res); });
        }
    }
    //generate feed after page index change
    function ChangePageNoCallback(res) {
        if (res.value != null) {
            $j(".imgAjaxLoad").hide();
            $j("#PageNo").text(NewPageNo);
            $j("#HomeTimeline").html(res.value);
            $j("#HomeTimeline").show();
        }
    }
</script>
