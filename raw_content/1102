    function RemoveSelections() { var a = document.getElementById("nav").children; for (var b = 0; b < a.length; b++) { if (a[b].className.indexOf('no_arrow') != -1) a[b].className = "nav_item no_arrow"; else a[b].className = "nav_item"; } } function SetSelections(a) { if (document.getElementById(a).className != "") document.getElementById(a).className += " selected"; else document.getElementById(a).className += "selected" } function testUrlAgainstMenuJsons(a) { if (a.substring(a.length - 1) == "/") a = a.substring(0, a.length - 1); for (i = 0; i < menuJsonArray.length; i++) { var b = menuJsonArray[i]; var c = b.Url; if (c.substring(c.length - 1) == "/") c = c.substring(0, c.length - 1); if (c.toUpperCase() == a.toUpperCase()) { defaultTabId = b.TabLinkToActivate; return i } } return -1 } function RunMenuScript() { var a = /(https?:\/\/[-A-Z0-9.]+(?:\/[-A-Z0-9+&@#\/%=~_|!:,.;]*)?)(\?[-A-Z0-9+&@#\/%=~_|!:,.;]*)?/i; var b = a.exec(document.location.href); if (b != null && b.length > 1) { var c = b[1]; var d = -1; d = testUrlAgainstMenuJsons(c); if (d == -1) { var e = c.lastIndexOf("/"); while (e != -1 && d == -1) { if (e != 0) { c = c.substring(0, e); d = testUrlAgainstMenuJsons(c); e = c.lastIndexOf("/") } else break } } } SetSelections(defaultTabId) }
