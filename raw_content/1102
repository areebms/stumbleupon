<script type="text/javascript">
    var user = { name: "", displayName: "", userid: "", avatarUrl: "", bio: "", referrer: "" };
    var hasProfile = '';

    $j(document).ready(function () {
        $j('#createProfileFire').fancybox({
            padding: 0,
            centerOnScroll: true
        });

        var btnUpload = $j('#btnUploadAvatar');
        var status = $j('#avatarError');

        new AjaxUpload(btnUpload, {
            action: '/AvatarUpload.axd',
            name: 'uploadfile',
            data: { userid: user.userid },
            onSubmit: function (file, ext) {
                if (!(ext && /^(jpg|png|jpeg|gif)$/.test(ext))) {
                    // extension is not allowed 
                    status.html('Only JPG, PNG or GIF files are allowed').fadeIn();
                    return false;
                }
                status.html('Uploading...').fadeIn();
            },
            onComplete: function (file, response) {
                //On completion clear the status
                status.html('');
                //Add uploaded file to list
                if (!response.error) {
                    var res = $j($j(response)[1]).html();

                    if (res != "error") {
                        user.avatarUrl = $j('#avatarImage').attr('src');
                        $j('#avatarImage').attr('src', res);
                        status.fadeOut().html('');
                        $j('#btnResetAvatar').fadeIn();
                    }
                    else
                        status.html('* The image you selected could not be uploaded.').fadeIn();
                }
                else
                    status.html('* The image you selected could not be uploaded.').fadeIn();
            }
        });

        if (hasProfile == 'true') {
            $j('#enterUsernameDiv').hide();
            $j('#spanNewUsername').html(unescape(user.name));
            $j('#txtDisplayName').val(unescape(user.displayName));
            $j('#txtAboutMe').val(unescape(user.bio));
            $j('#personaliseProfileDiv').show();
        }
    });

    function CheckUsernameAvailable() {
        var username = $j('#txtUsername').val();
        var pattern = new RegExp("^[a-zA-Z0-9\-\_]*$");

        if (!username == "") {
            if (!username.match(pattern))
                $j('#originalHeader').fadeOut("fast", function() { $j('#headerUsernameError').html("Your username may not contain any spaces or special characters.").fadeIn("fast"); });
            else
                TwentyFour.Sites.News.UI.Site.PersonalisationAjax.CheckUsernameAvailability(username, function(res) { CheckUsernameAvailableCallback(res); });
        }
        else
            $j('#originalHeader').fadeOut("fast", function () { $j('#headerUsernameError').html("Please enter a username and then click \"Register\"").fadeIn("fast"); });
    }

    function CheckUsernameAvailableCallback(res) {
        if (!res.error && res.value != "error") {
            if (res.value == true) {
                $j('#enterUsernameDiv').fadeOut("fast", function () {
                    $j('#spanNewUsername').html($j('#txtUsername').val());
                    $j('#txtDisplayName').val($j('#txtUsername').val());
                    $j('#personaliseProfileDiv').fadeIn("fast");
                });
            }
            else
                $j('#originalHeader').fadeOut("fast", function () { $j('#headerUsernameError').html("Sorry, that username is not available").fadeIn("fast"); });
        }
    }

    function ResetAvatar() {
        $j.ajax({
            url: '/AvatarRemove.axd?filename=' + $j('#avatarImage').attr('src'),
            type: 'POST'
        });

        $j('#avatarImage').attr('src', user.avatarUrl);
        $j('#btnResetAvatar').fadeOut();
    }

    function SaveUserProfile() {
        $j('#displayNameError').fadeOut("fast");
        $j('#profileSaveError').fadeOut("fast");

        var correct = true;
        var displayName = escape($j('#txtDisplayName').val());

        user.bio = escape($j('#txtAboutMe').val());
        user.avatarUrl = $j('#avatarImage').attr('src');
        user.name = escape($j('#txtUsername').val());
        if (displayName == "") {
            correct = false;
            $j('#displayNameError').fadeIn("fast");
        }
        else
            user.displayName = displayName;

        if (correct) {
            $j('#personaliseProfileDiv').fadeOut("fast");
            $j('.saveProfile').fadeIn("fast"); 
            TwentyFour.Sites.News.UI.Site.PersonalisationAjax.CreateTinaProfile(user, function (res) { SaveUserProfileCallback(res); });
        }
    }

    function SaveUserProfileCallback(res) {
        if (!res.error && res.value != "error") {
            if (res.value === "upload") {
                $j('.saveProfile').fadeOut("fast");
                $j('#personaliseProfileDiv').fadeIn("fast");
                $j('a.call2action').click();
                user.referrer = "";
            }
            else {
                $j('#fancybox-close').click();
                location.reload(true);
            }
        }
        else {
            $j('.saveProfile').fadeOut("fast");
            $j('#personaliseProfileDiv').fadeIn("fast");
            $j('#profileSaveError').fadeIn("fast");
        }
    }

    function CheckCharacterCount(event) {
        event = event || window.event;
        var keyCode = event.keyCode;

        if (keyCode == 8 || keyCode == 46)
            return true;
        else if ($j('#txtAboutMe').val().length < 1000)
            return true;
        else
            return false;
    }

</script>

