            function create_wing_ads(){ 
                                
                $('body').css('background','#000');

		// figure out how big the screen is .. and how wide our main div is
                
		var screen_width = $(document).width();
		var canvas_width = $('#canvas').width();
               
		// figure out how much space we have on each wing
                
		var wing_ad_space = parseInt( ( screen_width - canvas_width ) / 2 );

		// figure out how much to offset the right wing

                var right_ad_corner =  ( canvas_width + wing_ad_space );

		//alert( 	'wing ad space: ' + screen_width + ' - ' + canvas_width + ' / 2 ~= ' + wing_ad_space + '/n' + 'right ad corner: ' + canvas_width + ' + ' + wing_ad_space + ' ~= ' + right_ad_corner );
                
                // left ad is as wide as space round down and positioned absolutely at left: 0
                
                $('#left_wing_ad_div').css('height','613px')
					.css('width',wing_ad_space)
                                      .css('position','absolute')
                                      .css('top','10px')
                                      .css('left','0px')
                                      .css('background', "#000")               
                                      .css('cursor',"pointer");
                                      
                $('#right_wing_ad_div').css('height','613px')
                                      .css('width',wing_ad_space)
                                      .css('position','absolute')
                                      .css('top','10px')
                                      .css('left', right_ad_corner+'px')
                                      .css('background',"#000")
                                      .css('cursor',"pointer");
                
                if( wing_ad_space < 303 && wing_ad_space >= 75 ){
                
                    var ad_width = parseInt( wing_ad_space );
                    
                    $('#left_wing_ad_div').append("<img src='http://images.alternet.org/images/ads/keith_olbermann_wing2.png' width='"+ad_width+"' style='float: right;'>");
                    $('#right_wing_ad_div').append("<img src='http://images.alternet.org/images/ads/keith_olbermann_wing2.png' width='"+ad_width+"'' style='float: left;'>");
                    
                } else if( wing_ad_space < 75 ){
                    
                    $('#left_wing_ad_div').append("<img src='http://images.alternet.org/images/ads/keith_olbermann_wing2.png' width='75' style='float: right;'>");
                    $('#right_wing_ad_div').append("<img src='http://images.alternet.org/images/ads/keith_olbermann_wing2.png' width='75' style='float: left;'>");
                    
                } else {
                    
                    $('#left_wing_ad_div').append("<img src='http://images.alternet.org/images/ads/keith_olbermann_wing2.png' style='float: right;'>");
                    $('#right_wing_ad_div').append("<img src='http://images.alternet.org/images/ads/keith_olbermann_wing2.png' style='float: left;'>");
                    
                }
          
                var time = new Date().getTime();
          
                var rand = ( time * Math.floor( Math.random()*11) );
                
                $('#left_wing_ad_div').show().click( function() {
                    window.location="http://current.com/shows/countdown/";
                });
                
                $('#right_wing_ad_div').show().click( function() {
                    window.location="http://current.com/shows/countdown/";
                });
                
            }
         
