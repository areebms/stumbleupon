<script type="text/javascript">
(function(){
	GEL.thepage.initializer.addInitRoutine({ 
		name: "foulballRef", 
		priority: 0, 
		namespace: ['util.Cookie'],
		callback: foulball
	});
		GEL.thepage.initializer.addInitRoutine({
			name: "addAcquireSlot",
			namespace: ['firefly'],
			callback: bindFoulball, 
			priority: 20000           
		});
	//so it will run last
	function foulball(){
		var _continue= foulballFree();
		if(_continue){
			foulballRef()
		}
	}
	function foulballFree(){
		var _D= document, _W= window, _url, _ref, _temp;
		_temp= _url= _W.location.href.toLowerCase().replace(/[?&]+([^=&]+)=([^&]*)/gi, "");
		_url= _url.replace("viewart","article"); 
		if(_temp == _url){return true;}
		_ref= _D.referrer.toLowerCase().replace(/[?&]+([^=&]+)=([^&]*)/gi, "");
		if(_ref == _url){
			GEL.env.cookies.emetanclick.decrement();
			return false;
		}
		return true;
	}
	function getSlotFoulball()
	{
		//alert("Slot");
		fireflyJQuery(function(){
		if(typeof GEL.firefly.atyponAPI=='undefined'){GEL.firefly.initAtyponAPI();}
		//alert("Gel defined, getting slot");
		GEL.firefly.acquireSlot();
	});
	} 
	function bindFoulball()
	{
	var _D= document;
	var _check = GEL.env.cookies.emetanclick.checkReferrer(_D.referrer);
	if (_check)
		{
		//alert("foulballRef2");
			getSlotFoulball();
			//GEL.firefly.events.bind('topBarInitted', getSlotFoulball); 
			//alert("3");
			//GEL.firefly.events.bind('ssoComplete', getSlotFoulball); 
		//alert("8");
		} 
	//	alert("9");
	}
	function foulballRef(){
		var _D= document, _W= window.location.href.toLowerCase(), _check = GEL.env.cookies.emetanclick.checkReferrer(_D.referrer);
		if(_check && !GEL.thepage.pageinfo.firefly.alreadyVisited && (_W.indexOf("viewart") == -1 && _W.indexOf("viewint") == -1 && _W.indexOf("viewliv") == -1 )){
			GEL.env.cookies.emetanclick.increment();
		} 
	}
})();
</script>
