	{
		instance = 0;
		for(y in responseFromLocalDataIniSplitAtTheBar)
		{
			if(responseFromLocalDataIniSplitAtTheBar[x][0]==responseFromLocalDataIniSplitAtTheBar[y][0])
			{
				instance++;
			}
			if(instance>1)
