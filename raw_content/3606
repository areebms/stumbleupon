<script type="text/javascript">
	function limouseover(divid,menuid){
		document.getElementById(divid).style.display='';
		document.getElementById(menuid).style.color='#FF0000';
		document.getElementById(menuid).style.background='url(http://www.betterbraces.com/skin/frontend/enterprise/betterbraces/images/mainNavBack.png) no-repeat';
		var mylist=document.getElementById(menuid);
		var listitems= mylist.getElementsByTagName("a");
		listitems[0].style.color='#FF0000';
		
	}
	function limouseout(divid,menuid){
		document.getElementById(divid).style.display='none';
		document.getElementById(menuid).style.color='#555555';
		document.getElementById(menuid).style.background='url(http://www.betterbraces.com/skin/frontend/enterprise/betterbraces/images/leftNavigationDivider.jpg) 6px bottom no-repeat';
		var mylist=document.getElementById(menuid);
		var listitems= mylist.getElementsByTagName("a");
		listitems[0].style.color='#a4d7f3';
	}

</script>




