	var nameEQ = "MECCity=";
	var ForceCity = "";
	var city_exist = false;
	
	function assign_MEC_City() {
		var ca = document.cookie.split(';');
		for (var i = 0; i < ca.length; i++) {
		  var c = ca[i];
			while (c.charAt(0) == ' ') c = c.substring(1, c.length);
			if (c.indexOf(nameEQ) == 0) { city_exist=true;  MEC_City = c.substring(nameEQ.length, c.length);};
		}
	}
	assign_MEC_City();
	
	if( ! city_exist ) {
		ForceDCity = "?&rand="+Math.floor(Math.random()*10000001);
		document.write('<script src="http://'+domain+'/'+ForceDCity+'">'+'<'+'/'+'script>');	
	}
