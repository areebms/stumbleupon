	// this should be moved to separate js file
	huff.use('jquery', 'conf', function($, c){
		c.get('entry', 'vertical-name', 'threeup', function(entry, vert, params){
			huff.use('bpage/threeup', function(t){
				var threeup = function(params){
					t.init({
						frame:		params.frame || '#threeup_top_wrapper',
						content:	params.content || '#threeup_content',
						node:		params.node || '.threeup_entries',
						left:		params.left || '#threeup_left_nav',
						right:		params.right || '#threeup_right_nav',
						vertical:	params.vertical || vert,
						entry:		params.entry || entry,
						version:	params.version,
						tag:		params.tag || ''
					});
				}

				if ($.isArray(params))
					for(var i=0, l=params.length; i<l; i++)
						threeup(params[i]);
				else
					threeup(params);
			});
		});
	});

</script>

