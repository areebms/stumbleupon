				e.preventDefault();

				var link = $(this).attr('href');

				$.post( 'http://freshandfoodie.com/wp-admin/admin-ajax.php', {
					'action': 'wpl_record_stat',
					'stat_name': 'loggedout_signup_click'
				}, function() {
					location.href = link;
				} );
			});
		})
