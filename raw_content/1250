function deleteUser()
{
	var userIP = document.getElementById("userip").value;
	var userName = document.getElementById("username").value;
	if(userIP==""&&userName=="")
	{
		alert("Please enter user's name or IP");
		document.getElementById("username").focus();
		return;
	}
	var r=confirm("Are sure to delete this user and his posts?");
	if (r==true)
	  {
	  //post to delete user
		document.forms["frm_deleteuser"].submit();
	  }
	else
	  {
	  //cancel
	  return;
	  }
}

