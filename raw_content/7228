<div id="form-action-like-top">
    <form method="post" title="like this article" name="form-blox-like-top" id="form-blox-like-top" class="blox-like like" action="">
    <input type="hidden" name="action" value="asset:like" />
    <input type="hidden" name="act" value="like" />    
