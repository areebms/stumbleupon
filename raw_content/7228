    <script type="text/javascript">    
    // facebook responses trigger like/unlike
    $(window).load(function() {
        FB.Event.subscribe('edge.create', function(response) {
          // like clicked
          $("#form-blox-like-bottom").submit();
        });
        FB.Event.subscribe('edge.remove', function(response) {
          $("#form-blox-like-bottom").submit();
        });
    });
    $(document).ready(function(){
    // jQuery Form Plugin options
    var myFormOptions = {
      target:        '#form-action-like-bottom',
      beforeSubmit:  formBeforeSubmit,
      success:       formAfterSubmit
    };
    function formBeforeSubmit(){
      // PERFORM ACTIONS BEFORE FORM SUBMIT HERE
      // check current form behavior
      curBehavior = $("#form-blox-like-bottom input[name='act']").val();
      return true;
    }
    function formAfterSubmit(){
    // PERFORM ACTIONS AFTER FORM SUBMIT HERE
     // get like count
     var curLikeCount = 0;
     
     // check form's current behavior
      if(curBehavior == 'like'){
      // change form action to dislike
        formAct = 'dislike';
        buttonText = 'unlike';
        curLikeCount++;
      }else{
      // change form action to like
        formAct = 'like';
        buttonText = formAct;
        curLikeCount = 0;
      }
      // create new form
      var newForm = ''+
      '<form method="post" title="'+formAct+' this article" name="form-blox-like-bottom" id="form-blox-like-bottom" class="blox-like '+formAct+'" action="">'+
      '<input type="hidden" name="action" value="asset:'+formAct+'" />'+
      '<input type="hidden" name="act" value="'+formAct+'" />'+    
      '</form>';
     // updated count
     // place new form in response
      $("#form-action-like-bottom").prepend(newForm);     
      // call ajax for new form
      $('#form-blox-like-bottom').ajaxForm(myFormOptions);
    }
    // Make orginal form use jQuery ajax form plugin
    $('#form-blox-like-bottom').ajaxForm(myFormOptions);
});
    
    </script>
