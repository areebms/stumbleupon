    <script>
        var agent = navigator.userAgent.toLowerCase();
        // if userAgent matches 
        if (!!(agent.match(/(iPad|Android|SCH-I800|xoom|kindle|silk)/i))){
            // assign cookies
            var noMobile = null;
            var skipMobileNote = null;
            var mobileNoteDisplayed = null;
            noMobile = $.cookie('NoAdvMobile');
            skipMobileNote = $.cookie('noMobileNote'); 
            mobileNoteDisplayed = $.cookie('diplayedMobileNote');   
            if (noMobile == null){
            
                window.location = '/blogs/type-a-kitchen/baked-stuffed-shells/article_06d7b0d0-4bb5-566a-90ef-b38df432f5e7.html?mode=jqm';
               
            }
            else{
                if ((skipMobileNote == null)&&(mobileNoteDisplayed == null)){
                    // create session cookie to prevent mobile notice
                    $.cookie("diplayedMobileNote", 1);
                    // write a message to recommend mobile        
                    $(document).ready(function(){
                        $("#blox-html-container").prepend('<div id="show-mobile-notice" class="ui-state-highlight" title="Switch to the mobile view"><p>This site is optimized for mobile devices with no download required.</p></div>');
                        $("#show-mobile-notice").dialog({
                            autoOpen: true,
                            modal: true,
                            buttons: {
                                "Go to mobile view": function() {
                                    $.cookie("NoAdvMobile", null);
                                    window.location = '/mobile_adv/';
                                    $(this).dialog("close");
                                },
                                "Don't show this message": function() {
                                    // create long term cookie to disable mobile notice
                                    $.cookie("noMobileNote", 1, {expires: 30});
                                    $(this).dialog("close");
                                }
                            }
                        });
                    });    
                }        
            }
        } 
    </script>    

