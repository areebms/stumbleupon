		jQuery(this).addClass('jcarousel-item-'+(i+1));
	});
    if(false) { // working out wrong on ajax update of 'featured blogs' column
        if(jQuery.browser.webkit) {
            setTimeout(function(){
                jQuery('#carousel_1355203336b819b66289').animate({
                    height: jQuery('#carousel_1355203336b819b66289 .jcarousel-item-1').outerHeight(true)
                });
           },1000);
        } else {
            jQuery('#carousel_1355203336b819b66289').animate({
                height: jQuery('#carousel_1355203336b819b66289 .jcarousel-item-1').outerHeight(true)
            });
        }
    }
	jQuery('#jcarousel-next-horizontal_1355203336b819b66289').live('click', function(){
		var slide = parseInt(jQuery('#slide_1355203336b819b66289').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1355203336b819b66289').html(slide);
		if(jQuery('#carousel_1355203336b819b66289').outerHeight(true) < jQuery('#carousel_1355203336b819b66289 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1355203336b819b66289').animate({
				height: jQuery('#carousel_1355203336b819b66289 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1355203336b819b66289').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1355203336b819b66289').animate({
			height: jQuery('#carousel_1355203336b819b66289 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1355203336b819b66289').live('click', function(){
		var slide = parseInt(jQuery('#slide_1355203336b819b66289').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1355203336b819b66289').html(slide);
		if(jQuery('#carousel_1355203336b819b66289').outerHeight(true) < jQuery('#carousel_1355203336b819b66289 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1355203336b819b66289').animate({
				height: jQuery('#carousel_1355203336b819b66289 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1355203336b819b66289').children('li:last').detach();
		jQuery('#carousel_1355203336b819b66289').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1355203336b819b66289').animate({
			height: jQuery('#carousel_1355203336b819b66289 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
