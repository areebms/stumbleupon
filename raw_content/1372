function closeiframe() {
	try {
		var iframeArray = document.getElementsByTagName('iframe');
		for(i=0;i<iframeArray.length;i++)
		{
			iframeArray[i].className+=" hide";
		}
	}
	catch (e){
	}
}
//Call this function when the Ad contracts 
function openiframe() {
	try {
		var iframeArray = document.getElementsByTagName('iframe');
		for(i=0;i<iframeArray.length;i++)
		{
			 iframeArray[i].className=iframeArray[i].className.replace(" hide","");
		}
	}
	catch (e){
	}
}
