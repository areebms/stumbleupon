<SCRIPT LANGUAGE="JavaScript">
function Search_newfn_1(typesearch) {
    if (typesearch == 1) {
        var hidVal = document.frm_searchweb.SearchboxChoosedVal.value;
        if (hidVal == 0) {
            searchParam = "search"
        } else if (hidVal == 1) {
            searchParam = "Searchbox_Google"
        }
        dd = eval("document.frm_searchweb." + searchParam);
        if (dd.value == "") {
            alert("Please enter any valid search parameter.");
            dd.focus()
        } else {
		if(document.getElementById('search_sify').checked)
		{
		//alert('inside');
		var searchstr= dd.value;
		var searchstrArray = searchstr.split(" ");
		for(var i=0;i<searchstr.length;i++){
		searchstr = searchstr.replace(" ","_");
		}
		window.location.href='http://www.sify.com/topics/'+searchstr+'.html';
		}
		else if(document.getElementById('search_web').checked)
		{
            sitesearch = document.getElementById("sitesearch").value;
            ss = "32&rd=" + (sitesearch == "1" ? "sify" : "web");
            document.frm_searchweb.action = "http://search.sify.com/search.php?q=" + encodeURIComponent(dd.value) + "&ss=" + ss;
            document.frm_searchweb.search.value = "";
            document.frm_searchweb.submit()
		}
        }
    }
}
