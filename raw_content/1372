function loadNext(){

	if (pages <= (totalpages-1)) {
		pages++;
		startVal = 4 * pages;
	}
	for (i=0; i<(startVal+4); i++){
		if (i<startVal){
			$('#li_'+i).fadeOut("slow");
		}	
		else {
			$('#li_'+i).css("left", "500px");
			for (var j=parseInt($('#li_'+i).css("left")); j>=0; j=j-100)
				$('#li_'+i).animate({"left": j+"px"}, "50");
			$('#li_'+i).animate({"left": "0"}, "50");
			$('#li_'+i).show();
		}
	}
	if (pages < (totalpages-1)){
		if (i <= totalObjects){
			$('#startVal').html(i + " ");
			var tmpStartVal = (i-4); 
		}
		else {
			$('#startVal').html(totalObjects + " ");
			var tmpStartVal = pages * 4; 
		} 
		$('#li_'+tmpStartVal).trigger('click');
		$("#tdNext").show();
		$("#tdPrev").show();
	}
	else {
		var tmpStartVal = pages * 4; 
		$('#startVal').html(totalObjects + " ");
		$('#li_'+tmpStartVal).trigger('click');
		$("#tdNext").hide();
		$("#tdPrev").show();
	}
}
function loadPrev(){
	if (pages > 0) {
		startVal = 4 * pages;
		pages--;
	}
	for (i=(startVal+4); i>=(startVal-4); i--){
		if (i>=startVal){
			$('#li_'+i).fadeOut("slow");
		}	
		else {
			for (var j=-500; j<=0; j=j+100)
				$('#li_'+i).animate({"left": j+"px"}, "50", function() {$(this).show();});
			
		}
	}
	if (pages > 0){
		if (i > 0) {
			$('#startVal').html(startVal + " ");
			var tmpStartVal = (startVal-1);  
		}
		else {
			$('#startVal').html(4 + " ");
			var tmpStartVal = 3;
		}			
		$("#li_"+tmpStartVal).trigger('click'); 
		$("#tdNext").show();
		$("#tdPrev").show();
	}
	else {
		var tmpStartVal = 3;
		$('#startVal').html(4 + " ");
		$("#li_"+tmpStartVal).trigger('click'); 
		$("#tdNext").show();
		$("#tdPrev").hide();
	}
}

function getref_searchAds() {
			r = document.referrer; 
			if(r !="")
			{ 
				var d = r.split("//"); 
				var d1 = d[1].split("/"); 
				var d2 = d1[0].split("."); 
				var dname =''; 
				if(d2.find('google')!=false)
				{
					document.getElementById('searchad').style.display = 'block';
				}
			}
		}
		Array.prototype.find = function(searchStr) {
		  var returnArray = false;
		  for (i=0; i<this.length; i++) {
			if (typeof(searchStr) == 'function') {
			  if (searchStr.test(this[i])) {
				if (!returnArray) { returnArray = [] }
				returnArray.push(i);
			  }
			} else {
			  if (this[i]===searchStr) {
				if (!returnArray) { returnArray = [] }
				returnArray.push(i);
			  }
			}
		  }
		  return returnArray;
		}
		getref_searchAds();
