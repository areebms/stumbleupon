	jQuery('#jcarousel-next-horizontal_1363122373255a11d061').live('click', function(){
		var slide = parseInt(jQuery('#slide_1363122373255a11d061').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1363122373255a11d061').html(slide);
