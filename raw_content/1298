		$('.lightbox').fancybox();
		function formatTitle(title, currentArray, currentIndex, currentOpts) {
			var printTitle = title.replace("--|", "</b><i>");
			var printTitle = printTitle.replace("|--", "</i><b>");
			return '<div id="pic-title"><span><a href="javascript:;" onclick="$.fancybox.close();"><img src="/scripts/jQuery/fancybox/v134/fancy_close.png" /></a></span>' + (printTitle && printTitle.length ? '<b>' + printTitle + '</b>' : '' ) + '<h4>Image ' + (currentIndex + 1) + ' of ' + currentArray.length + '</h4></div>';
		}
		$("a.grouped_elements").fancybox({
		'showCloseButton'	: false,
		'titlePosition' 	: 'inside',
		'titleFormat'		: formatTitle
		});
	</script>
