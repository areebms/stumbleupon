		(function($){
			var nav = $("#TopNav");
			nav.find("li").each(function() {
				if ($(this).find("ul").length > 0) {
					$(this).mouseenter(function() {
						$(this).find("ul").stop(true, true).show();
					});
					$(this).mouseleave(function() {
						$(this).find("ul").stop(true, true).hide();
					});
				}
			});
		})(jQuery);
	</script>
