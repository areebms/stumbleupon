<script type="text/javascript" language="Javascript">		
var deviceIphone = "iphone";
var deviceIpod = "ipod";
var deviceS60 = "series60";
var deviceSymbian = "symbian";
var engineWebKit = "webkit";
var deviceAndroid = "android";
var deviceWinMob = "windows ce";
var deviceBB = "blackberry";
var devicePalm = "palm";
var deviceOpera = "Opera M";
var usragent = navigator.userAgent.toLowerCase();
function Iphone() {if (usragent.search(deviceIphone) > -1) {return true;} else {return false;};};
function Ipod() { if (usragent.search(deviceIpod) > -1) { return true; } else { return false; };};
function IphoneOrIpod() { if (Iphone()) { return true; } else if (Ipod()) { return true; } else { return false; }; };
function S60OssBrowser() { if (usragent.search(engineWebKit) > -1) { if ((usragent.search(deviceS60) > -1 || usragent.search(deviceSymbian) > -1)) { return true; } else { return false; }; } else { return false; };};
function Android() {if (usragent.search(deviceAndroid) > -1) {return true;} else {return false;};}; 
function AndroidWebKit() { if (Android()) { if (Webkit()) { return true; } else { return false; }; } else {return false;}; };
function WindowsMobile() {if (usragent.search(deviceWinMob) > -1) { return true; } else { return false;};};
function BlackBerry() {if (usragent.search(deviceBB) > -1) {return true;} else {return false;};};
function PalmOS() {if (usragent.search(devicePalm) > -1) {return true;} else {return false;};}; 
function Opera() {if (usragent.search(deviceOpera) > -1) {return true;} else {return false;};}; 
function DetectMobile() {
   if (IphoneOrIpod())
      return true;
   if (Android())
      return true;
   if (AndroidWebKit())
      return true;
   if (PalmOS())
      return true;
   if (BlackBerry())
	  return true;
   if(Opera())
	  return true;
   else
      return false;
};
isMobile = DetectMobile();

	

</script>


	
	
