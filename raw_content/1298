		$('.MobileSiteLink').click(function() {
			var COOKIE_NAME = 'MobilePreference';
			var options = { path: '/', expires: 300 };
			$.cookie(COOKIE_NAME, 'yes', options);
			var CurrentURL = $(this).attr('href');
			window.location=CurrentURL;
			return false;
		});
	</script>
