<script type="text/javascript">
$(document).ready(function(){
	valueWidth();
	var realHeight = $(".video-info-content").height();
	$("#object-show-more").toggle(
	function(){
		get_short_url(function(short_url) {
		    $("#link-url").val(short_url);
		});
		$("#description-box").animate({
			height: realHeight+140
		}, 200);
		$(".show-more-text").html("Show Less");
		$(".show-more-arrow").addClass("less");
	}, function() {
				$("#description-box").animate({
			height: 40
		}, 200);
		$(".show-more-text").html("Show More");
		$(".show-more-arrow").removeClass("less");
	})
	$('#embed-btn').click(function(){
		embedDown();
	});
	$('.close-btn').click(function(){
		$('#embed-box').animate({top: '+98'});
	});
	$('#flag-video').click(function(){
		$('#flag-box').slideDown('fast');
	});
	$('.close-btn-flag').click(function(){
		$('#flag-box').slideUp('fast');
	});
	
				$("#favorite a").click(function(){
												openGigyaLogin();
							});
			
});

function embedDown(){
	$('#embed-box').show();
	$('#embed-box').animate({top: '-98'});
	return false;
}
function playerEmbed(){
	shareDown();
	embedDown();
}

function flagVideo() {
	var option = $('input:radio[name=flag_option]:checked').val();
	var details = $("#flag_details").val();
	
	$.post(
		'http://www.videobash.com/flagitem',
		{'type':'video', 'id':3664 , 'option':option, 'details':details},
		function(response) {
			$("#flaggedMsg").html(response);
		}
	);	
	$("#flaggedMsg").show();	
	setTimeout(function () {
		$('#flag-box').slideUp('fast');	
	}, 3000);
}

$(window).load(function(){	
	$('.icon').mouseenter(
		  function(){
			$(this).children('.tooltip').show();
			var ttw = $(this).children('.tooltip').width()/2;
			$(this).children('.tooltip').css('margin-left', -ttw)
		  });
	$('.icon').mouseleave(
		  function(){
			 $(this).children('.tooltip').hide();
		  });
});

function addFavorite(id){
	showHide('div.login-box-wrapper');
}

var t_up = 884;
var t_down = 266;
function rateItem(id, type)
{
	$.post(
		'http://www.videobash.com/video/rateVideo',
		{'id':id, 'type':type},
		function(response) {
			if(response){
				if(type){
					t_up = parseInt(response);
					var t_percent = (t_up * 100) / (t_up + t_down);
					$("#thumbs_up").text(response);
					$("#thumbs_width").css("width", t_percent + "%");
					$(".hype-btn > strong").text(t_up);
				}
				else{
					t_down = parseInt(response);
					var t_percent = (t_up * 100) / (t_up + t_down);
					$("#thumbs_down").text(response);
					$("#thumbs_width").css("width", t_percent + "%");
					$(".bash-btn > strong").text(t_down);
				}
				$("#thumbs_percent").html('Thanks!');
				$("#thumbs_percent").css("padding", "5px 0");
				$("#thumbs_percent").css("width", "100%");
				$("#thumbs_percent").css("font-size", "10px");
				
			}
			else{
				$("#thumbs_percent").html('Already Rated');
				$("#thumbs_percent").css("padding", "5px 0");
				$("#thumbs_percent").css("width", "100%");
				$("#thumbs_percent").css("font-size", "10px");
			}
		}
	);
}
var sb = $('.sidebar-share')
var sboffset = sb.offset().top;

$(window).scroll(function() {
	var wheight = $(window).scrollTop();
	if (wheight > sboffset) {
		sb.addClass("sidebar-scroll");
	}else {
		sb.removeClass("sidebar-scroll");
	}
});
	
function showGigShare(mode, provider){
	if(gigya_enable == false){
		return false;
	}
	
	var act_share = new gigya.services.socialize.UserAction();
	var meta_img_share = $('meta[property=og:image]').attr("content");
	var linkBack = 'http://www.videobash.com/video_show/crazy-biker-3664';
	var mode = typeof(mode) != 'undefined' ? mode : 'multiSelect';
	var title = 'Crazy biker';
	var description = "Another crazy russian biker on Moscow\\\'s highway.";
	var provider = typeof(provider) != 'undefined' ? provider : 'facebook';
	var feedID = 'live';
	var initView = provider == 'email' ? 'email' : 'share';
	var title_ext = provider == 'twitter' ? ' via @VideoBash #Funny_Videos' : '';
	
	if(mode == 'bookmark'){
		var params_share =
		{
			provider: provider
			,url: linkBack
			,title: title + title_ext
			,description: description
			,shortURLs: 'never'
		}
		gigya.services.socialize.postBookmark(conf,params_share);
		if(gigyaUser){
			publishShare({title: title, feedID: feedID});
		}
		return false;
	}
	 
	act_share.setTitle(title);  
	act_share.setLinkBack(linkBack);
	act_share.setDescription(description);
	act_share.addActionLink("Read More", linkBack);   
	if(meta_img_share){
		act_share.addMediaItem({type: 'image', src: meta_img_share, href: linkBack});
	}               
	var params_share =   
	{  
		userAction:act_share
						,operationMode: mode   
		,showEmailButton: 'true'
		,shortURLs: 'never'
		,onSendDone: publishLocalAction
		,context: {title: title, feedID: feedID}
		,initialView: initView
	};
	
	gigya.services.socialize.showShareUI(conf,params_share);
	return false;  
}

function showStatsBlock(){
	
	if($("#item_stats_block").hasClass("active")){
		$('#item_stats_block').slideUp();
		$('#item_stats_block').removeClass("active");
		return false;
	}
	$.post(
		'http://www.videobash.com/stats_block',
		{'type':'video', 'id':'3664'},
		function(response) {
			if(response){
				$('#item_stats_block').html(response);
				$('#item_stats_block').addClass("active");
				$('#item_stats_block').slideDown();
			}
		}
	);		

	return false;
}	
function get_short_url(func)
{
	$.getJSON(
	    "http://api.bitly.com/v3/shorten?callback=?", 
	    { 
	        "format": "json",
	        "apiKey": "R_84acf6173d1a9484ccb46bc1e55a9321",
	        "login": "videobash",
	        "longUrl": "http://www.videobash.com/video_show/crazy-biker-3664"
	    },
	    function(response)
	    {
	        func(response.data.url);
	    }
	);
}	
