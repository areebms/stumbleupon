		<script type="text/javascript" src="http://cdn1.static.videobash.com/js/swfobject.js" language="javascript"></script>
	<script type="text/javascript">
	var playCheck = false;
	var adsStarted = false;
	var adaptvCompanionAdTag = '<iframe marginheight="0" marginwidth="0" scrolling="no" frameborder="0" width="300" height="250" src="http://ads.videobash.com/ads?zone_id=764&client=videobash"></iframe>';
		
	    function displayPlayer(autoplay) {
	    	    			playCheck = true;
				var playerVersion = swfobject.getFlashPlayerVersion();
				var playerSWFname;
				var playerVersionNumber = playerVersion.major + playerVersion.minor / 10;
				if (playerVersionNumber < 10 ) {
					playerSWFname = "player.swf";
				} else if (playerVersionNumber >= 10 && playerVersionNumber < 10.2) {
					playerSWFname = "player_10_0_mini.swf";
				} else {
					playerSWFname = "player_10_2_mini.swf";
				}
				var CDNPath = "http://resources.videobash.com/flash/" + playerSWFname;
				var htmlStr = 
					'<object height="386" ' +
					'width="636"' +
					'type="application/x-shockwave-flash"' +
					'id="vb_vid"' +
					'style="visibility: visible;"' +
					'data="' + CDNPath + '">' +
					'<param name="movie" value="' + CDNPath + '" />' +
					'<param name="quality" value="high" />' +
					'<param name="allowfullscreen" value="true"/>' +
					'<param name="allowScriptAccess" value="always"/>' +
					'<param name="wmode" value="transparent"/>' +
					'<param name="flashvars" value="autoplay=true&amp;autoreplay=false&amp;video_url=http://cdn1.videos.videobash.com/201009/22/3664/480_3664.mp4?ir%3D-1%26sr%3D2000%26int%3D20%25"/>' +
					'</object>';
					$("#player").html(htmlStr);
						}
				</script>
<script>displayPlayer();</script>
<script type="text/javascript">
	var loadCounter = 3;
	function nextVideo(){
		var autoplay = $("#nextvideo").attr('checked');
		if(autoplay){
			if(loadCounter <= 0){
				nextVideoGo();
			}
			else{
				$("#loadCounter").html('&nbsp;&nbsp; Loading in .. ' + loadCounter);
				loadCounter--;
				setTimeout(nextVideo, 1000);
			}
		}
	}
	function nextVideoGo(){
				window.location = "http://www.videobash.com/video_show/motorbike-crashes-into-gate-819";
			}
	function setNextVideo(){
		var autoplay = $("#nextvideo").attr('checked') ? 1 : 0;
		$.post('http://www.videobash.com/video/autoplay_click',{'autoplay':autoplay},function(response) {});		
	}
	
	function rateItemPlayer(){
		rateItem(3664, 1);
	}
</script>	</div>
	
	
