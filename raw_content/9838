<script type="text/javascript">
	function addComment(is_social)
	{	
		getBlock(1);
		var type = typeof(is_social) != 'undefined' ? 1 : 0;
		var comment = $("#comment_content").val();
		var username = $("#comment_username").val();
		var thumb = $("#comment_thumb").val();
		$.post(
			'http://www.videobash.com/comment/addComment',
			{'commentvars[id]':3664,'commentvars[type]':'video','commentvars[uid]':0,'commentvars[comment]':comment,'commentvars[social]':type,'commentvars[username]':username, 'commentvars[thumb]':thumb},
			function(response) {
				if(type == 0){
					if(response.indexOf("<div") > -1){
						$("#comment_content").val('');
						$("#comment_new").html(response);
						$("#comment_response").text('Comment Added!');
						$("#comment_response").removeClass('red');
					} else {
					    $("#comment_response").addClass('red');
					    $("#comment_response").text(response);
					}
				}
			}
		);
	}
	function deleteComment(id)
	{
		$.post(
			'http://www.videobash.com/comment/deleteComment',
			{'commentvars[id]':id,'commentvars[uid]':0},
			function(response) {
				$("#comment_block_" + id).text(response);
				$("#comment_block_" + id).attr('style', 'font-weight:bold');
			}
		);
	}
	function reportComment(id)
	{
		$.post(
			'http://www.videobash.com/comment/reportComment',
			{'commentvars[id]':id},
			function(response) {
				$("#comment_block_" + id).text(response);
				$("#comment_block_" + id).attr('style', 'font-weight:bold');
			}
		);
	}
	var selorder = 0;
	function getComments(id, type, order)
	{
		if(selorder != order){
			$.post(
				'http://www.videobash.com/comment/getCommentsBlock',
				{'id':id,'type':type,'order':order},
				function(response) {
					if(order == 1){
						$("#comments_newest").removeClass('active');
						$("#comments_oldest").addClass('active');
					}
					else{
						$("#comments_newest").addClass('active');
						$("#comments_oldest").removeClass('active');
					}
					selorder = order
					$("#comments_block").html(response);
					getBlock(1);
				}
			);
		}
	}

	var pages = 2;	var page = 1;
	function getBlock(block) {
		$('#page_block').html($('#page_block_' + block).html());
		if(block <= 10){
			$('#paginator_' + page).removeClass('active');
			$('#paginator_' + block).addClass('active');
		}else{
			$('#paginator_10').removeClass('active');
		}
		page = block;
		$('#comment_page').text('Page ' + page);
	}
	function getNextPage() {
		if(page < pages){
			getBlock(page + 1);
		}
		else{
			getBlock(1);
		}
	}
		$('#page_block_1').html($('#page_block').html());
	</script>
		</div>
