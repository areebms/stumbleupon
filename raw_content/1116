<script>
(function(){
	GEL.thepage.initializer.addInitRoutine({ 
		name: "foulballRef", 
		priority: 0, 
		namespace: ['util.Cookie'],
		callback: foulball
	});
	function foulball(){
		var _continue= foulballFree();
		if(_continue){
			foulballRef()
		}
	}
	function foulballFree(){
		var _D= document, _W= window, _url, _ref, _temp;
		_temp= _url= _W.location.href.toLowerCase().replace(/[?&]+([^=&]+)=([^&]*)/gi, "");
		_url= _url.replace("viewart","article"); 
		if(_temp == _url){return true;}
		_ref= _D.referrer.toLowerCase().replace(/[?&]+([^=&]+)=([^&]*)/gi, "");
		if(_ref == _url){
			GEL.env.cookies.emetanclick.decrement();
			return false;
		}
		return true;
	}
	function foulballRef(){
		var _D= document, _W= window.location.href.toLowerCase(), _check = GEL.env.cookies.emetanclick.checkReferrer(_D.referrer);
		if(_check && (_W.indexOf("viewart") == -1 && _W.indexOf("viewint") == -1 && _W.indexOf("viewliv") == -1 || GEL.thepage.pageinfo.firefly.alreadyVisited )){
			GEL.env.cookies.emetanclick.increment();
		}
	}
})();
</script>
