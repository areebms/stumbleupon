			<script type="text/javascript">
				GEL.thepage.initializer.addInitRoutine({
					name: "widget.ArticleBreakout",
					priority: 35,
					namespace: ['widget.ArticleBreakout'],
					callback: function(){artBreakout=new ArticleBreakout(true);}
				});
