});
function initFirefly(){
	fireflyJQuery(function(){
		if(typeof GEL.firefly == 'undefined') GEL.firefly = new GEL.ody_firefly(GEL.thepage.pageinfo.firefly);
		if(!GEL.firefly.initted)GEL.firefly.init();
	});
}
