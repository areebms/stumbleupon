function getLocation () {
var theUrl = escape(getClickURL());
var theTitle = escape(getClickTitle());
theUrl = appendAndEscape(getClickURL(),"")
var theLoc = "&fb=Y&url="+theUrl+"&title="+theTitle+"&random="+Math.random()+"&partnerID="+partnerID+"&expire="+escape(getClickExpire());
return theLoc;
}
