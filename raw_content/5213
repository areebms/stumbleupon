            appendShare.listener = function(T){
                if(!T) T = 0;
                if(T > 0){
                    setTimeout(appendShare.listener,T);
                    return false;
                    }

                if($('.pds-box .morePolls').length > 0){
                    return appendShare.listener(1000);
                    }
                if($('.pds-box .pds-share').length > 0){
                    $('.pds-box .pds-share').remove();
                    }
        $('a.pds-view-results').after( '<span><a href="'+appendShare.url+'" class="morePolls">More Polls</a></span>' );
        $('a.pds-return-poll').after( '<span><a href="'+appendShare.url+'" class="morePolls">More Polls</a></span>' );
        return appendShare.listener(1000);
      }
      appendShare.listener(5000);
