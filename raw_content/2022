        } else {
            var federated_media_section = '';
            document.write ("<scr"+"ipt type='text/javascript' src='http://static.fmpub.net/zone/935");
            document.write ("'><\/scr"+"ipt>");
        }
    </script> 
  </div>
  
</div>

  </div> <!-- #project-sidebar -->
</div> <!-- .flex-box-1 -->    
</div> <!-- .flex-container -->


        </div><!-- #primary-content -->

