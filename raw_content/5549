<script type="text/javascript">
	jQuery('#carousel_1355529970b819b66264').children('li').css('float', 'left').each(function(i){
		jQuery(this).addClass('jcarousel-item-'+(i+1));
	});
    if(false) { // working out wrong on ajax update of 'featured blogs' column
        if(jQuery.browser.webkit) {
            setTimeout(function(){
                jQuery('#carousel_1355529970b819b66264').animate({
                    height: jQuery('#carousel_1355529970b819b66264 .jcarousel-item-1').outerHeight(true)
                });
           },1000);
        } else {
            jQuery('#carousel_1355529970b819b66264').animate({
                height: jQuery('#carousel_1355529970b819b66264 .jcarousel-item-1').outerHeight(true)
            });
        }
    }
	jQuery('#jcarousel-next-horizontal_1355529970b819b66264').live('click', function(){
		var slide = parseInt(jQuery('#slide_1355529970b819b66264').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1355529970b819b66264').html(slide);
		if(jQuery('#carousel_1355529970b819b66264').outerHeight(true) < jQuery('#carousel_1355529970b819b66264 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1355529970b819b66264').animate({
				height: jQuery('#carousel_1355529970b819b66264 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1355529970b819b66264').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1355529970b819b66264').animate({
			height: jQuery('#carousel_1355529970b819b66264 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1355529970b819b66264').live('click', function(){
		var slide = parseInt(jQuery('#slide_1355529970b819b66264').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1355529970b819b66264').html(slide);
		if(jQuery('#carousel_1355529970b819b66264').outerHeight(true) < jQuery('#carousel_1355529970b819b66264 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1355529970b819b66264').animate({
				height: jQuery('#carousel_1355529970b819b66264 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1355529970b819b66264').children('li:last').detach();
		jQuery('#carousel_1355529970b819b66264').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1355529970b819b66264').animate({
			height: jQuery('#carousel_1355529970b819b66264 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
