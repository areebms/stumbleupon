<script type="text/javascript">
	var narrowSiteBool = 'False'.toLowerCase();
	var category = 'news'.toLowerCase();
    var script = document.createElement('script');
    script.type = 'text/javascript';
    switch (category) {
    	case "news":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204330&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	case "sport":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204331&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	case "entertainment":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204334&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	case "healthlifestyle":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204332&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	case "thefix":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204333&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	case "travel":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204335&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	case "finance":
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204427&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    	default:
    		script.src = "http://data.ninemsn.com.au/Services/Service.axd?ServiceName=Highlight&ServiceAction=GetThirtyMinuteCache&ServiceFormat=JSONAUTO&SiteID=4375&SectionID=75339&SubSectionID=204330&GroupID=141575&callback=NFP_jsonCallback";
    		break;
    }
    document.getElementsByTagName('head')[0].appendChild(script);

    function NFP_jsonCallback(data) {
        if (data && data.HighlightList && data.HighlightList.Highlight && data.HighlightList.Highlight.LinkList && data.HighlightList.Highlight.LinkList.Link) {
            var PARTNERS;
            var hl_links_counter = 0;
            var total_links_count;
            var nw_ft_partners_lks_div_html = "";
            var hl_data_object = data.HighlightList.Highlight;

            if (hl_data_object.Title != "" || hl_data_object.ImageUrl != "")
                nw_ft_partners_lks_div_html += '<div class="nw_ft_partners_head"><div class="nw_ft_more_title">' + hl_data_object.Title + '</div></div>';
            hl_data_object = hl_data_object.LinkList.Link;
            total_links_count = hl_data_object.length
            nw_ft_partners_lks_div_html += '<div class="nw_ft_all_partners">';

            if (narrowSiteBool == 'true') {
                PARTNERS = 5;
                document.getElementById("nw_ft_wrap").style.width = "780px";
            }
            else
                PARTNERS = 6;

            for (var i = 0; i < PARTNERS; i++) {
                if (hl_data_object[hl_links_counter].Indent == 1) {
                    nw_ft_partners_lks_div_html += '<div class="nw_ft_partner nw_ft_partner_' + i + '">';
                    if (hl_data_object[hl_links_counter].Title != "")
                        nw_ft_partners_lks_div_html += '<div class="nw_ft_title_section"><a href="' + hl_data_object[hl_links_counter].Url + '">' + hl_data_object[hl_links_counter].Title + '</a></div>';
                    if (hl_links_counter != total_links_count - 1)
                        hl_links_counter++;
                    else
                        break;
                    if (hl_data_object[hl_links_counter].Indent == 2) {
                        nw_ft_partners_lks_div_html += '<div id="nw_ft_section_image"><a href="' + hl_data_object[hl_links_counter].Url + '"><img class="nw_ft_partnerimage" src="' + hl_data_object[hl_links_counter].ImageUrl + '" /></a></div>';
                        if (hl_data_object[hl_links_counter].Title != "")
                            nw_ft_partners_lks_div_html += '<div class="nw_ft_bottom_title"><a href="' + hl_data_object[hl_links_counter].Url + '">' + hl_data_object[hl_links_counter].Title + '</a></div>';
                        while (hl_data_object[hl_links_counter].Indent == 2) {
                            if (hl_links_counter != total_links_count - 1)
                                hl_links_counter++;
                            else
                                break;
                        }
                    }
                    while (hl_data_object[hl_links_counter].Indent == 3) {
                        if (hl_data_object[hl_links_counter].Title != "")
                            nw_ft_partners_lks_div_html += '<div class="nw_ft_subtitle"><a href="' + hl_data_object[hl_links_counter].Url + '">' + hl_data_object[hl_links_counter].Title + '</a></div>';
                        if (hl_links_counter != total_links_count - 1)
                            hl_links_counter++;
                        else
                            break;
                    }
                    nw_ft_partners_lks_div_html += '</div>';
                }
                else {
                    while (hl_data_object[hl_links_counter].Indent != 1) {
                        if (hl_links_counter != total_links_count - 1)
                            hl_links_counter++;
                        else
                            break;
                    }
                }
            }
            nw_ft_partners_lks_div_html += '</div>';
            var div = document.createElement("div");
            div.id = "nw_ft_partners_lks";
            div.innerHTML = nw_ft_partners_lks_div_html;
            var footer = document.getElementById("nw_ft_wrap");
            var child = footer.firstChild;
            footer.insertBefore(div, child);
            if (narrowSiteBool == 'true') {
                footer.innerHTML = '<div id="nw_ft_narrow">' + footer.innerHTML + '<div>';
            }
            document.getElementById("nw_ft_partners_lks").style.display = "block";
        }
    }
</script>
