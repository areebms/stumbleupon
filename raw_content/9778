SERVICE_BASE_URL = 'http://data.ninemsn.com.au/Services/Service.axd';


HighlightGetCallback = function(data) {
    var dataArr = data.HighlightList.Highlight.LinkList.Link;
    var promo = $("#article_hotsearch");
    promo.append('<div class="linkHead">Hot Searches:</div>') 
    for (i = 0; i < dataArr.length; i++) {
        promo.append('<div class="linkItem">' + '<a class="linkUrl" href="' + dataArr[i].Url + '">' + dataArr[i].Title + '</a></div>')
        var tabNum = i + 1;

    }
}

    var url = [];
    var i = 0;

    url[i++] = SERVICE_BASE_URL;
    url[i++] = '?ServiceName=Highlight';
    url[i++] = '&ServiceAction=Get';
    url[i++] = '&ServiceFormat=JSONAUTO';
    url[i++] = '&SiteID=2339';
    url[i++] = '&SectionID=0';
    url[i++] = '&SubSectionID=0';
    url[i++] = '&GroupID=142088';
    url[i++] = "&responseEncoding=json&callback=?";

    //&SiteID=2339&SectionID=0&SubsectionID=0&GroupID=72594
    $.getJSON(url.join(""), HighlightGetCallback);

