			        for (var i = 0; i < sfEls92295.length; i++) 
			        {
				        sfEls92295[i].onmouseover = function() 
				        {
					        this.className += " sfhover";
		                    return true;
				        }
				        sfEls92295[i].onmouseout = function() 
				        {
					        this.className = this.className.replace(new RegExp(" sfhover\\b"), "");
					        return true;
				        }
			        }
		            return true;
		        }
		        attach( window , "onload" , sfHover92295);
	        } 
        </script>
