  if (!isNaN(JS_NIELSEN_SURVEY_SAMPLERATE)) {
       _rsEnableSurvey = 1;
       _rsSampleRate = JS_NIELSEN_SURVEY_SAMPLERATE;
  }
  var feat={surveys_enabled:_rsEnableSurvey, sample_rate:_rsSampleRate};
  var trac=nol_t(pvar,feat);
  var _rsND=trac.getSchemeHost(); // For compatibility with v53 surveys
  var _rsCI="ninemsn";// For compatibility with v53 surveys
  trac.record().post().do_sample();
  _rsPageEvent = function (_rsClickContent, _rsCG) { trac.pageEvent(_rsClickContent, _rsCG); };
