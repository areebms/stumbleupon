		<script type="text/javascript">
			// determine news feed URL
		    var feed_domain = "http://content.ninemsn.com.au/";
			var JS_HD_VER;
			
			var url = window.location.href;
		    var firsDotPos = feed_domain.indexOf(".");
			var pre = feed_domain.substring(0, firsDotPos + 1);
			var post = feed_domain.substring(firsDotPos, feed_domain.length);
			
			if(url.indexOf("devprev01.ninemsn") > -1)
			{
			    feed_domain = pre + "devprev01" + post;
			}
			else if(url.indexOf("prev01.ninemsn") > -1 || url.indexOf("syd.ninemsn") > -1)
			{
			    feed_domain = pre + "prev01" + post;
			}
			
			var qs;
			if(url.indexOf("mode=preview")>-1&&url.indexOf("?")>-1)
			{
			    qs = "?" + url.split("?")[1];
			}
			else
			{
				qs = "?cache=" + parseInt((new Date()).getTime()/1000/60/60/24/365*52);
			}
			var newsFeed = feed_domain + "newsfeed.aspx" + qs;
			var moreFeed = feed_domain + "morefeed.aspx" + qs;
			
			// set for sbk-driven sites and must be set prior to ad call
			var spac_has_sbk_header = true;
			
			// omniture-related code
			if(JS_OMNITURE_COMPANY == '')
			{
    			JS_OMNITURE_COMPANY = "ninemsn";
			}

			function OCLTracking(obj, CustomLinkName)
			{
	    		document.getElementById("cl_tracking").src = "http://" + JS_OMNITURE_CATEGORY + ".112.2o7.net/b/ss/" + JS_OMNITURE_CATEGORY + "/1/H.1-pdv-2/s3589868987415?[AQB]&ndh=1&ns=msnportal&cdp=3&cc=USD&c1=" + JS_SITE + "-" + JS_SECTION + "&c2=en-au&c4=" + JS_OMNITURE_CATEGORY + "&c5=" + JS_OMNITURE_COMPANY + "&c6=" + JS_PAGE_COBRAND + "&pe=lnk_o&pev1=" + window.location.href + "&pev2=" + CustomLinkName + "&s=1280x1024&c=32&j=1.3&v=Y&k=Y&bw=1280&bh=771&ct=lan&hp=N&[AQE]";
			}
			
			// other helper classes
			Array.Clone = function(iterable)
			{
				if (!iterable) return [];
				if (iterable.toArray)
				{
					return iterable.toArray();
				}
				else
				{
					var results = [];
					for (var i=0; i<iterable.length; i++)
					{
						results.push(iterable[i]);
					}
					return results;
				}
			}

			Function.prototype.Bind = function()
			{
				var method = this, args = Array.Clone(arguments), context = args.shift();
				return function()
				{
					method.apply(context, args);
				}
			}
		</script>

