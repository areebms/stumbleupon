			        <div class="headerimage"><a href="/glance/"></a><div class="headertitle"><a href="/glance/"><b>Glance:</b> The news in pictures</a></div></div><div class="headertext"><script type="text/javascript">
trackNavClicks = function(name)
{
	try
	{
		if(name!=null && name != '')
		{
			var nameStr = 'NEWS_SHARE_' + name + '_' + JS_ARTICLE_ID;
			OMNTRLinkTracking(true,nameStr,'',null,null,null);
		}
	}
	catch(e)
	{}
}

$(document).ready(function()
{
	trackNavClicks('LOAD');
	
	if($('#article_sharing_footer').length)
	{
		$('#asf_prt_wrap').click(pClick);
		$('#asf_eml_wrap').click(eClick);
		$('#asf_mess_wrap').click(messClick);
		$('#asf_fb_wrap').click(fClick);
		$('#asf_dd').children().click(ddIClick);
	}	
});

pClick = function(event)
{
	trackNavClicks('PRINT');
};
eClick = function(event)
{
	trackNavClicks('EMAIL');
};
messClick = function(event)
{
	trackNavClicks('MESS');
};
fClick = function(event)
{
	trackNavClicks('FB');	
};
ddIClick = function(event)
{
	var tit = $(this).attr('title');
	switch(tit)
	{
		case 'Blog this article on Spaces':	
			trackNavClicks('SPACES');
			break;
		case 'Share this article on delicious':	
			trackNavClicks('DEL');
			break;
		case 'Share this article on Digg':	
			trackNavClicks('DIGG');
			break;
		case 'Share this article on MySpace':	
			trackNavClicks('MYSPACE');
			break;
		default:
			break;
	}
};

</script></div>
