        <h3><a href="/flock.aspx">The Flock</a></h3>
        <div class="whatsthis" title="offsety=[-115] offsetx=[-165] header=[a] cssheader=[tooltipheader] cssbody=[tooltipbody] body=[<span class=tipcontent>&nbsp;&nbsp;You get to shape the news mix at The<br />&nbsp;&nbsp;Flock, rating articles by clicking on the <br />&nbsp;&nbsp;Flock It button at the bottom of every <br />&nbsp;&nbsp;article</span>]">
            <img src="/img/icons/icn_whatisthis.gif">
        </div>
    </div>
    <a name="anchor100"></a>
    <div id="block100">
        <div id="inner_block100">
        <script type="text/javascript">
            var uid = new Date().getTime();
            var flashProxy = new FlashProxy(uid, '/media/flash/flock/JavaScriptFlashGateway.swf');
            var tag = new FlashTag('/media/flash/flock/flock_rhc.swf', '310', '335'); 
            tag.setFlashvars('lcId='+uid);
            tag.setFlashvars('currentPageTitle', '');

            tag.write(document);
                
            //var f_article_id = $.query.get("id");                
            var f_article_id = JS_ARTICLE_ID;
            function flashHighlightComment()
            {
                var f_title = $("#title").text();    
                var f_comment = $("#ugc_flock_form_comment").val();
                flashProxy.call('forceFlockComment', 'commented', f_article_id, f_title, f_comment,flockUserProfile.flockerName);
                var anchorDiv = $('#block100');
                if(anchorDiv.length)
{
    window.scroll(0, anchorDiv.offset().top); 
}
   }
            function flashHighlightBasic()
            {
                var f_title = $("#title").text(); 
                flashProxy.call('forceFlockComment', 'forcedBasic', f_article_id, f_title);
                                var anchorDiv = $('#block100');
                if(anchorDiv.length)
{
    window.scroll(0, anchorDiv.offset().top); 
}
            }
        </script>
        </div>
    </div>
</div>

