<script type="text/javascript">
var timeStampForOptusUser = "";
var OptusBrandCookieExists = fncCheckForCookie("brand", "optus");
if(OptusBrandCookieExists) 
    fncKillOptusBrandCookie();

// detects cookieName=cookieValue pair
function fncCheckForCookie(cookieName, cookieValue) {
   try {
      var search = cookieName + "=";
      var returnvalue = "";
      if (document.cookie.length > 0) {
         offset = document.cookie.indexOf(search);
         if (offset != - 1) {
            offset += search.length;
            var end = document.cookie.indexOf(";", offset);
            if (end == - 1) 
                end = document.cookie.length;
            returnvalue = unescape(document.cookie.substring(offset, end)) }
         }
     if(returnvalue == cookieValue)
        return true;
     else
        return false;            
      }
   catch(ex) {
      }
};

// kills optus=brand cookie, when detected
function fncKillOptusBrandCookie () {
    var expiresAt = new Date();
    expiresAt.setDate(expiresAt.getDate() - 10);
    document.cookie = "brand=optus;expires="+ expiresAt.toLocaleTimeString() +";domain =.ninemsn.com.au;path=/";
};

// collects all links on the page, appends a check function on-click
function fncAppendCheckToLinks() {
    try {
        var allAnchors = document.getElementsByTagName("a");
        for(i = 0; i < allAnchors.length; i++) {
             var currentAnchor = allAnchors[i];
             var me = currentAnchor;
             me.onclick = function(evt) {
                fncAppendCoBrandIfOptusSession(this, evt);
            }
             
         }
      }
    catch(ex) {
    }
};

// the co-brand gets attached and cascades on-click, if the user navigates with-in the first 15 minutes
function fncAppendCoBrandIfOptusSession(obj, evt) {
    var timeStampNow = new Date();
    var timeDifference =  timeStampNow.getMinutes() - timeStampForOptusUser.getMinutes();
    if(timeDifference <= 15) {
         var stringToCheck = obj.href;
         if(stringToCheck.indexOf("?") > ( - 1))  
            obj.href = obj.href + "&_cobr=optus";
         else if (stringToCheck.indexOf("&") > ( - 1))
            obj.href = obj.href + "?_cobr=optus";
         else 
            obj.href = obj.href + "?_cobr=optus";
    document.location = obj.href;
    }
};

// does the optus co-brand string exist?
var OptusCoBrandString = "_cobr=optus";
var documentURL = document.URL.toLowerCase();
var OptusCoBrandStringExists = documentURL.indexOf(OptusCoBrandString) > ( - 1) ? true : false;

// if co-brand exists, track as optus user
if(OptusCoBrandStringExists) {
    timeStampForOptusUser = new Date();
    JS_PAGE_COBRAND = "optus";
    attach(window, "onload", fncAppendCheckToLinks);
}
</script>
