<script type="text/javascript">

var div=document.getElementById('cat_hl_222396');
var item=div.childNodes[0];

var url = 'http://data.ninemsn.com.au/Services/Service.axd?ServiceFormat=JSONAUTO&ServiceName=Highlight&ServiceAction=Get&SiteID=2339&SectionID=0&SubSectionID=0&GroupID=72612&callback=getLinks';

    var script = document.createElement('script');
    script.type = 'text/javascript';
    script.src = url;
    document.getElementsByTagName('head')[0].appendChild(script);

    function getLinks(data) {
    var totallinks;
    var hl_data_object = data.HighlightList.Highlight.LinkList.Link;

totallinks = hl_data_object.length;
      if(totallinks > 2){

      // get two different random numbers less than the totallinks
      var rnd2,rnd1=Math.floor(Math.random()*totallinks);

      while((rnd2=Math.floor(Math.random()*totallinks))==rnd1){
              rnd2=Math.floor(Math.random()*totallinks);
      }
      //alert("random numbers are " + rnd2 + "     -    " + rnd1);
      for (var i = 0; i < totallinks; i++) {
           $("#item_" + i).css("display", "none");
          if (i == rnd1 || i == rnd2) {
              $("#item_" + i).css("display", "block");
          }
        
      }
      }
}

</script>

