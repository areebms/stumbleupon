var JS_PAGE_COBRAND;

function GetCobrandAdStr_HEADER_633609789393515753()
{
if(JS_PAGE_COBRAND != undefined && JS_PAGE_COBRAND != ''){return '/COBRAND='+JS_PAGE_COBRAND;}else{return '';}}

function RenderHeaderAd(adsize)
{
var memTag;
if(JS_AD_MEM_TAG==undefined)
{
    memTag="";
}
else if(JS_MEMBERSHIP_IS_AUTHENTICATED==undefined)
{
    memTag="";
}
else if(JS_MEMBERSHIP_IS_AUTHENTICATED.toLowerCase()=="true")
{
    memTag=JS_AD_MEM_TAG;
}
else
{
    memTag="";
}

spac_writeAd("/SITE=" + JS_SITE + "/AREA=" + JS_SECTION + "/SUBSECTION=" + JS_SUB_SECTION + "/LOC=TOP/AAMSZ="+adsize+memTag+GetCobrandAdStr_HEADER_633609789393515753());

}
</script>

