    <script type="text/javascript">
    
    $(document).ready(function()
    {
    var alerts = $('#art_rhc .alert');
    var alertArr = [];
    var alertLen = null;
    var rand = null;
    var index = null;

    if (alerts.length)
    {

        for (i = 0; i < alerts.length; i++)
        {
            if (alerts.eq(i).children().length)
            {
                alertArr.push(alerts.eq(i));
            }
        }
    }
    
    alertLen = alertArr.length;
    
    if (alertLen)
    {
        rand = Math.ceil(Math.random()*100+6)
        index = rand % alertLen;
        $(alertArr[index]).css('display', 'block');
    }
    });
    
    

    </script>    
    
    
  
