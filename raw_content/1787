  var indexn = new Image();
  indexn.src = "http://picky-palate.com/wp-content/themes/picky-new/images/recipe-indexn.gif";
  var indexa = new Image();
  indexa.src = "http://picky-palate.com/wp-content/themes/picky-new/images/recipe-indexa.gif";
  var giveawaysn = new Image();
