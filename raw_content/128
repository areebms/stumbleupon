<script type="text/javascript">

// public product_load() {{{ 
/**
 * product_load
 * 
 * @access public
 * @return void
 */

var products = {
	0: { 
		id: 'http://www.footballfanatics.com/NFL_New_England_Patriots/browse/partnerid/7898/featuredproduct/37722/source/team'
	},
	1: {
		id: 'http://www.footballfanatics.com/MLB_New_York_Yankees/browse/partnerid/7898/featuredproduct/141327/source/team'
	},
	2: {
		id: 'http://www.footballfanatics.com/NBA_Los_Angeles_Lakers/browse/partnerid/7898/featuredproduct/194199/source/team'
	}
};

window.productPosition = 0;

var wc = '31337';

function product_load(search) {

	
	var page = 'blog';
	
	if (typeof(search) == 'string') {
		var keyword = search;
		var searching = 1;
	} else {
		
		var searching = 0;
		var sport = 'nfl';
		var keyword = '';
		var gender = '';
		
	}

	new Ajax.Request('ajax/services.php', {
		method: 'get',
		parameters: { action: 'tfs_productGet', page: page, sport: sport, keyword: keyword, gender: gender },
		onSuccess: function(transport) {

			if (transport.responseText == 'null') {
				if (searching == 1) {
					$('productLoading' + wc).update('No gear found, try another search');
				} else {
					$('productLoading' + wc).update('No gear found, try searching');
				}
				return true;
			}

			var response = transport.responseText.evalJSON();


			window.products = response;
			window.productPosition = 0;
			var product = { 
				A:window.products[0], 
				B:window.products[1], 
				C:window.products[2] 
			}

			if (response.errors) {
				alert(response.errors);
			}

			product_display(product,true);
			kill_auto();
			window.cycler = setInterval(function() { product_cycle(true, true) }, 7000);
		},

		onFailure: function(transport) {
			alert('failure : product_load()');
			$('productLoading' + wc).update('Gear currently unavailable.<br />Please check back later');
		}
	});

}
// }}}
// public product_click() {{{ 
/**
 * product_click
 * 
 * @access public
 * @return void
 */
function product_click(position) {

	if (position == 'SHOP') {

		$('productId' + wc).value = 'SHOP';

	} else {

		var product = {
			A:window.products[window.productPosition],
			B:window.products[window.productPosition + 1],
			C:window.products[window.productPosition + 2]
		};

		$('productId' + wc).value = product[position].id;

	}

	document['productForm' + wc].submit();

}
// }}}
// public product_display(product,first) {{{ 
/**
 * product_display
 * 
 * @param product $product 
 * @param first $first 
 * @access public
 * @return void
 */
function product_display(product,first) {

	$('productLoading' + wc).hide();

	if (first == true) {
	//	var productLabel = 'productLoading';
		var productLabel = 'productInfo';
	} else {
		var productLabel = 'productInfo';
	}

	Effect.Fade(productLabel + wc, {
		duration: 0.5,
		afterFinish: function() {
			product_update(product);
			Effect.Appear('productInfo' + wc, {duration: 0.5})
			if (first == true) {
				Effect.Appear('productMoreRight' + wc, {duration: 0.5})
			}
		}
	});

}
// }}}
// public product_update(product) {{{ 
/**
 * product_update
 * 
 * @param product $product 
 * @access public
 * @return void
 */
function product_update(product) {

	var maxlen = 26;

	for (key in product) {
		if (!product[key]) {
			continue;
		}
		product[key].titleOrig = product[key].title;
		if (product[key].title.length > maxlen) {
			product[key].title = product[key].title.substring(0, maxlen) + '..';
		}
		$('product' + key + 'Title' + wc).update(product[key].title);
		$('product' + key + 'Image' + wc).title = product[key].titleOrig;
		if (product[key].thumbnail == '') {
			$('product' + key + 'Image' + wc).update('Click to view');
		} else {
			$('product' + key + 'Image' + wc).update('<img src="' + product[key].thumbnail + '" height="75" />');
			$('product' + key + 'Z' + wc).update( '<img src="' + product[key].thumbnail + '" />' + 
			'<div>' + product[key].titleOrig + '</div>');
		}
		$('product' + key + 'Price' + wc).update(product[key].price);
	}

}
// }}}
// public product_cycle(forward) {{{ 
/**
 * product_cycle
 * 
 * @param forward $forward 
 * @access public
 * @return void
 */
function product_cycle(forward, auto) {

	if (!auto) {
		kill_auto();
	}

	if (forward == true) {

		window.productPosition += 3

		if (window.products[window.productPosition]) {

			var product = {
					A:window.products[window.productPosition],
					B:window.products[window.productPosition + 1],
					C:window.products[window.productPosition + 2]
			};

			product_display(product, false);
		} 

		if (!window.products[window.productPosition + 6]) {
			$('productMoreRight' + wc).fade({duration: 0.3});
		}

		if ($('productMoreLeft' + wc).style.display == 'none') {
			$('productMoreLeft' + wc).appear({duration: 0.3});
		}

	} else {

	window.productPosition -= 3

		var product = {
			A:window.products[window.productPosition],
			B:window.products[window.productPosition + 1],
			C:window.products[window.productPosition + 2]
		}

		product_display(product, false);

		if (window.productPosition == 0) {
			$('productMoreLeft' + wc).fade({duration: 0.3});
		}

		if ($('productMoreRight' + wc).style.display == 'none') {
			$('productMoreRight' + wc).appear({duration: 0.3});
		}

	}

}
// }}}

function kill_auto() {
	clearInterval(window.cycler);
}

function product_searchFocus(wc) {

	$('productSearch' + wc).className = '';
	if ($('productSearch' + wc).value == 'Search Gear...') {
		$('productSearch' + wc).value = '';
	}

	kill_auto();
}

function product_searchBlur(wc) {

	if ($('productSearch' + wc).value == '') {
		$('productSearch' + wc).className = 'cGrey4';
		$('productSearch' + wc).value = 'Search Gear...';
	}

}

function product_searchCheck(wc, e) {

	if (window.event) {
		var keynum = window.event.keyCode;
	} else {
		var keynum = e.which;
	}

	if (keynum == 13) {
		product_search(wc);
	}
}

function product_search(wc) {

	$('productInfo' + wc).hide();
	$('productLoading' + wc).update('Searching for "'+$('productSearch' + wc).value + '"')
	$('productLoading' + wc).show()

	product_load($('productSearch' + wc).value);
}

addLoadEvent(product_load);

</script>


