<script type="text/javascript">

function ticket_loadTickets() {

	
	var keyword = '';
	var category = 'nfl';
	var campaign = 'blog';
	var creative = 'cr_0';
	window.ticketKeyword = '';
	window.ticketCategory = 'nfl';
	window.ticketCampaign = 'blog';
	

	$('ticketLoading').pulsate({ pulses: 5, duration: 30.0, from: 0.1});

	new Ajax.Request('ajax/services.php', {
		method: 'get',
		parameters: { action: 'ticketsGet', keyword: keyword, category: category, campaign : campaign, creative : creative },
		onSuccess: function(transport) {
			var response = transport.responseText.evalJSON();
			window.tickets = response.results.searchResults.searchEvents;
			if (!window.tickets[0]) {
				return ticket_none();
			}
			window.ticketPosition = 0;

			var ticket = window.tickets[0];

			if (response.errors) {
				//alert(response.errors);
			}

			ticket_displayTicket(ticket,true);
			kill_tauto();
			window.tcycler = setInterval(function() { ticket_cycle(true) }, 7000);
		},

		onFailure: function(transport) {
			alert('failure : ticket_loadTickets()');
		}
	});

}

function kill_tauto() {
	clearInterval(window.tcycler);
}


function ticket_click() {

	new Ajax.Request('ajax/services.php', {
		method: 'post',
		parameters: { 
			action: 'ticketsClick', 
			keyword: window.ticketKeyword, 
			category: window.ticketCategory, 
			campaign: window.ticketCampaign
		},
		onSuccess: function (transport) {
			window.open(window.target);
		}
	});

}

function ticket_none() {
	new Ajax.Request('ajax/services.php', {
		method: 'post',
		parameters: {
			action: 'ticketsNone',
			keyword: window.ticketKeyword, 
			category: window.ticketCategory, 
			campaign: window.ticketCampaign
		},
		onSuccess: function (transport) {
			$('ticketLoading').update('No related tickets found..');
			$('ticketLoading').pulsate({pulses: 1, duration: 0.1});
		}
	});
}

function ticket_displayTicket(ticket,first) {

	if (first == true) {
		var ticketLabel = 'ticketLoading';
	} else {
		var ticketLabel = 'ticketInfo';
	}

	Effect.Fade(ticketLabel, {
		duration: 0.5,
		afterFinish: function() {
			ticket_update(ticket);
			Effect.Appear('ticketInfo', {duration: 0.5})
			if (first == true) {
				Effect.Appear('ticketMoreRight', {duration: 0.5})
			}
		}
	});

}

function ticket_update(ticket) {

	// : populate form fields etc
	window.target = ticket.url;
	$('ticketURL').value = ticket.url;
	$('ticketDetail').value = ticket.name;
	$('ticketInteracted').value = window.ticketInteracted;

	if (window.ticketInteracted) {
		$('ticketInteracted').value = 1;
	}

	// : truncate ticket names too long
	var maxlen = 48;
	var nameOrig = ticket.name;

	if (ticket.name.length > maxlen) {
		ticket.name = ticket.name.substring(0, maxlen) + '..';
	}


	var url = '<a target="_new" onClick="document.ticketForm.submit(); return false;" title="' + nameOrig + '" href="' + ticket.url + '">' + ticket.name + '</a>';
	$('ticketName').update(url);
	$('ticketLowestPrice').update('$' + ticket.minPrice);
	$('ticketDate').update(ticket.date);
	$('ticketVenue').update(ticket.venue);
	$('ticketTracker').update('<img src="' + ticket.assets.priceTrend.small + '" border="0"/>');

}

function ticket_cycle(forward) {

	window.ticketInteracted = 1;

	if (forward == true) {

		window.ticketPosition++

		if (window.tickets[window.ticketPosition]) {
			var ticket = window.tickets[window.ticketPosition];
			ticket_displayTicket(ticket, false);
		} else {
			return false;
		}

		if (!window.tickets[window.ticketPosition + 1]) {
			$('ticketMoreRight').fade({duration: 0.3});
		}

		if ($('ticketMoreLeft').style.display == 'none') {
			$('ticketMoreLeft').appear({duration: 0.6});
		}

	} else {

	window.ticketPosition--
		var ticket = window.tickets[window.ticketPosition];
		ticket_displayTicket(ticket, false);
		if (window.ticketPosition == 0) {
			$('ticketMoreLeft').fade({duration: 0.3});
		}

		if ($('ticketMoreRight').style.display == 'none') {
			$('ticketMoreRight').appear({duration: 0.3});
		}

	}

}

addLoadEvent(ticket_loadTickets);

</script>


