  <script type="text/javascript">
        /* &lt;![CDATA[ */
            var interval;
            var panelOpacity;

            $(document).ready(function(){

                if ($("body").hasClass("HomePage")) {
                    /*--- Opacity Fix For Homepage ----*/
                    panelOpacity = -1;
                } else {
                    panelOpacity = 0.1;
                }

                /*---- Default Variation ----*/
                $(".roundaboutDefault #roundabout").roundabout({
                    easing: 'easeInOutQuad',
                    duration: 500,
                    btnNext: '.roundaboutNext',
                    btnPrev: '.roundaboutPrev',
                    reflect: true,
                    minZ: 10,
	            	maxZ: 20,
                    minOpacity: panelOpacity,
                    minScale: 1.0
                }).hover(
                    function() {
                        /*---- pause ----*/
                        clearInterval(interval);
                    },
                    function() {
                        /*---- play ----*/
                        startAutoPlayDefault();
                    }
                );

                /*---- Text Overlay ----*/
                $(".roundaboutOverlay #roundabout").roundabout({
                    easing: 'easeInOutQuad',
                    duration: 500,
                    btnNext: '.roundaboutNext',
                    btnPrev: '.roundaboutPrev',
                    reflect: true,
                    minZ: 10,
	            	maxZ: 20,
                    minOpacity: panelOpacity,
                    minScale: 1.0
                }).hover(
                    function() {
                        /*---- pause ----*/
                        clearInterval(interval);
                    },
                    function() {
                        /*---- play ----*/
                        startAutoPlayOverlay();
                    }
                );

                if ($("#roundabout").parent("div").hasClass("roundaboutDefault")) {
                    /*--- Default Auto Start ----*/
                    startAutoPlayDefault();
                } else {
                    /*--- Text Overlay Auto Start ----*/
                    startAutoPlayDefault();
                }
            });

            /*---- Default Button Pause/Play ----*/
            $(".roundaboutDefault .roundaboutBtn").hover(
                function() {
                    /*---- pause ----*/
                    clearInterval(interval);
                },
                function() {
                    /*---- play ----*/
                    startAutoPlayDefault();
                }
            );

            /*---- Text Overlay Button Pause/Play ----*/
            $(".roundaboutOverlay .roundaboutBtn").hover(
                function() {
                    /*---- pause ----*/
                    clearInterval(interval);
                },
                function() {
                    /*---- play ----*/
                    startAutoPlayOverlay();
                }
            );

            /*---- Default Auto Play ----*/
            function startAutoPlayDefault() {
                interval =  setInterval(function() {
                    $("#roundabout").roundabout_animateToNextChild(700,"easeInOutQuad");
                }, 4500);
            }

            /*---- Text Overlay Auto Play ----*/            
            function startAutoPlayOverlay() {
                interval =  setInterval(function() {
                    $("#roundabout").roundabout_animateToNextChild(1500,"easeInOutQuad");
                }, 4500);
            }
        /* ]]&gt; */
        </script>
