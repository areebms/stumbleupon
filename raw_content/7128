<script type="text/javascript">
$(function () {
	"use strict";
	var images, pinURL, post_h4, url, media, description, imgWidth, imgAlign, winTop, winLeft;
	images = $(".post img").map(function () {
		return $(this);
	});
	$.each(images, function () {
		post_h4 = $(this).parents(".post-wrapper").children("h4").children();
		url = $(post_h4).attr("href");
		media = $(this).attr("src");
		description = encodeURIComponent($(post_h4).text());
		pinURL = "http://pinterest.com/pin/create/button/?";
		pinURL += "url=" + url;
		pinURL += "&media=" + media;
		pinURL += "&description=" + description;
		imgWidth = $(this).width() + 20; // add padding width
		if ($(this).hasClass("alignleft")) {
			imgAlign = " alignleft";
		} else if ($(this).hasClass("alignright")) {
			imgAlign = " alignright";
		} else if ($(this).hasClass("aligncenter")) {
			imgAlign = " aligncenter";
		} else {
			imgAlign = "";
			imgWidth = imgWidth - 20; // remove padding width
		}
		$(this).parent().wrap(function () {
			return "<div class='pin-it-wrapper" + imgAlign + "' style='width:" + imgWidth + "px;' />";
		});
		$(this).parent().before("<a href='" + pinURL + "' class='pin-it'><img src='http://images.meredith.com/bhg/images/blog/centsational-style/pinit-medallion.png' alt='Pin It!' width='100' height='100' /></a>");
	});
	$(".pin-it").css("display", "none");
	$(".pin-it-wrapper").hover(function () {
		$(this).find(".pin-it").css("display", "block");
		$(this).find("img[alt!='Pin It!']").css({"opacity": "0.25", "filter": "alpha(opacity=25)"});
	}, function () {
		$(this).find(".pin-it").css("display", "none");
		$(this).find("img[alt!='Pin It!']").css({"opacity": "1", "filter": "alpha(opacity=100"});
	});
	$(".pin-it").click(function (event) {
		event.preventDefault();
		winTop = window.outerHeight / 2 - 125;
		winLeft = window.outerWidth / 2 - 300;
		window.open(($(this).attr("href")), '_blank', 'top=' + winTop + ',left=' + winLeft + ',width=600,height=250,location=no,menubar=no,scrollbars=no,status=no,toolbar=no');
	});
});
</script>
