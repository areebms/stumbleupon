<script language="javascript">

function validateSignUpForm(theForm){
	if(isSelected(theForm.title1,       "title"))     {return (false)};
	if(isFieldBlank(theForm.first_name,"first name")) {return (false)};
	if(isString(theForm.first_name,    "first name")) {return (false)};	
	if (theForm.first_name.value == "Enter first name")     
	{
		alert("Please enter your first name.");
		return (false);
	}
	
	if(isFieldBlank(theForm.last_name,   "last name")) {return (false)};
	if(isString(theForm.last_name, "last name"))       {return (false)};
	if (theForm.last_name.value == "Enter surname")     
	{
		alert("Please enter your surname.");
		return (false);
	}
	
	if(isFieldBlank(theForm.e_mail,    "e-mail")) {return (false)};
	if(isEmail(theForm.e_mail,         "e-mail")) {return (false)};
}
</script>

	
