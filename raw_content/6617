var Timer;
var stop_Timer=0;

function stop_inInterval(){
	clearInterval(Timer);
	stop_Timer=1;
}

clearInterval(Timer);
	
function start_rotate_div(){
	if(stop_Timer==0)
	Timer=setInterval("change_div()", 5000);
}
start_rotate_div();	
</script>
