<script type="text/javascript">
    jQuery(document).ready(function() {
        jQuery("body").append("<div id='navArrow'></div>");

        function addMega() {
            jQuery("li.mega").removeClass("hovering");
            jQuery(this).addClass("hovering");


        }
        function removeMega() {

            jQuery(this).removeClass("hovering");

        }

        var megaConfig = {
            interval: 100,
            sensitivity: 6,
            over: addMega,
            timeout: 500,
            out: removeMega
        };

        jQuery("li.mega").hoverIntent(megaConfig);



    });
    var countId = 'WidgetRecipeBoxCount'
    function updateRecipeBoxCountWithValue(count) {
        jQuery("#" + countId).html(count);
    }
    function updateRecipeBoxCount() {
        GeneralMills.BettyCrocker.RecipeBox.GetRecipeBoxCount(getCountResults);
    }

    function getCountResults(result) {

        updateRecipeBoxCountWithValue(result);
    }
</script>




