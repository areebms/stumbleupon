<script type="text/javascript">
    var recipeBoxServiceTimeout;
       
    var memberGeneratedPrefix = '/usergen';
    var kitchenGeneratedPrefix = '/recipes';

    var recipeBoxAutoOpen = 'False';

    jQuery(document).ready(function () {
      if (recipeBoxAutoOpen == 'True') {
            jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRecipeId").val(recipeId);
            GeneralMills.BettyCrocker.RecipeBox.GetRecipeBoxCategories(GetCategoryResults);     
            dcsMultiTrack('DCS.dcsuri', '/recipes/SaveRecipeBox.aspx', 'DCS.dcsqry', 'RecipeId=' + jQuery('#System.Web.UI.HtmlControls.HtmlInputHidden').val(), 'WT.ti', 'Saving a Recipe to your BettyCrocker.com recipe box.');
      } 
       jQuery('div.SaveToRecipeBoxContainer').keydown(function(event) {
        if (event.which == 13) {
            event.preventDefault();
            jQuery('#SaveRecipeToBoxLink', this).click();
        }
        });
        jQuery('#SaveRecipeToBoxLink').click(function(event) {        
            event.preventDefault();
            AddToRecipeBox();
        });
       jQuery("#ErrorRetryLink").click(function() {
        jQuery("#RecipeBoxError").hide();
        jQuery("#RecipeBoxDisplay").show();
        GeneralMills.BettyCrocker.RecipeBox.GetRecipeBoxCategories(GetCategoryResults,ErrorLoadingResults);
        });

         if (typeof (urlParams) != "undefined" && urlParams.rbt != null) {
                recipeBoxServiceTimeout = parseInt(urlParams.rbt);
           };
         if (recipeBoxServiceTimeout != null) {
            GeneralMills.BettyCrocker.RecipeBox.set_timeout(recipeBoxServiceTimeout);
         };
         
    }); 

    function openSaveToRecipeBox(recipeId, categoryId) {
        jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRecipeId").val(recipeId);
        if (categoryId != null) {
               jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRBDCategoryID").val(categoryId);
        }
        
        GeneralMills.BettyCrocker.RecipeBox.GetRecipeBoxCategories(GetCategoryResults);		   
        OpenVariableDialog('main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_SaveToRecipeBoxDialog','/popup/saveToRecipeBox','Open Save To Recipe Box Popup',false);;
    }
        
    function openSaveToRecipeBox(recipeId, memberGenerated) {
       jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRecipeId").val(recipeId);
       jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnMemberGenerated").val(memberGenerated);

        GeneralMills.BettyCrocker.RecipeBox.GetRecipeBoxCategories(GetCategoryResults,ErrorLoadingResults);		   		   

        if ('True' == 'True'){
            if (jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnMemberGenerated").val() == 'true') {
                dcsMultiTrack('DCS.dcsuri', memberGeneratedPrefix + '/SaveRecipeBox.aspx', 'DCS.dcsqry', 'RecipeId=' + recipeId, 'WT.ti', 'Saving a Recipe to your BettyCrocker.com recipe box.');
                }
                else
                {
                 dcsMultiTrack('DCS.dcsuri', kitchenGeneratedPrefix + '/SaveRecipeBox.aspx', 'DCS.dcsqry', 'RecipeId=' + recipeId, 'WT.ti', 'Saving a Recipe to your BettyCrocker.com recipe box.');
                }
           }
        OpenVariableDialog('main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_SaveToRecipeBoxDialog','/popup/saveToRecipeBox','Open Save To Recipe Box Popup',false);;
    }
    
    function GetCategoryResults(result)
	{
		var excludeCategory = "";
		var mode = "0";

		jQuery("#ddlFolders").get(0).options.length = 0;
		
		if (mode == 0)
		{
			jQuery("#ddlFolders").get(0).options[0] = new Option("Recipe Box", "ROOT"); 
		}
		else
		{
			jQuery("#ddlFolders").get(0).options[0] = new Option("Select One", "SELECT ONE");
		}

	    jQuery.each(result, function(index, item) {
			if (mode == 0 || item.ID != excludeCategory)
				jQuery("#ddlFolders").get(0).options[jQuery("#ddlFolders").get(0).options.length] = new Option(item.Name, item.Id);
		});
		
		jQuery("#ddlFolders").get(0).options[jQuery("#ddlFolders").get(0).options.length] = new Option("----OR----", "SPACER"); 	
		jQuery("#ddlFolders").get(0).options[jQuery("#ddlFolders").get(0).options.length] = new Option("Create New Folder", "NEW"); 	

        jQuery("#LoadingText").hide();
        jQuery("#ddlFolders").show();
	}

    function ErrorLoadingResults(result, method)
    {        
        jQuery("#RecipeBoxDisplay").hide();
        jQuery("#RecipeBoxError").show();
    }

	function RecipeBoxWebTrendsSave(recipeId, memberGenerated) {
	    if ('True' == 'True'){
            if (memberGenerated == 'true') {
                dcsMultiTrack('DCS.dcsuri', memberGeneratedPrefix + '/RecipeBoxThankYou.aspx', 'DCS.dcsqry', 'RecipeId=' + recipeId, 'WT.ti', 'Thank You For Adding a Recipe to you BettyCrocker.com Recipe Box');
                }
                else
                {
                 dcsMultiTrack('DCS.dcsuri', kitchenGeneratedPrefix + '/RecipeBoxThankYou.aspx', 'DCS.dcsqry', 'RecipeId=' + recipeId, 'WT.ti', 'Thank You For Adding a Recipe to you BettyCrocker.com Recipe Box');
                }
            }       

	}

	function ResetSaveRecipeDialog(event, ui) {
		jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_txtNewFolderName").val("");
		jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_txtNewFolderName").hide();
        jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_SaveRecipePanel").show();
        jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_ConfirmationPanel").hide();
        jQuery("#RecipeBoxError").hide();
        jQuery("#RecipeBoxDisplay").show();
        updateRecipeBoxCount();
    }
    
    jQuery("#ddlFolders").change(function() {
        var category = jQuery('option:selected', this).val();

		if (category.toUpperCase() == "NEW")
		{
			jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_txtNewFolderName").show();
			ValidatorEnable(document.getElementById("main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_rfvNewFolderName"),true);
            ValidatorEnable(document.getElementById("main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_CustomValidatorUniqueCategory"),true);
		}
		else
		{
			jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_txtNewFolderName").hide();
			ValidatorEnable(document.getElementById("main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_rfvNewFolderName"),false);
            ValidatorEnable(document.getElementById("main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_CustomValidatorUniqueCategory"),false);
		}
		
		jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_SaveRecipePanel").show();
        jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_ConfirmationPanel").hide();
		
    });
    
	function SaveBCSpotLightTag() {
		var axel = Math.random();
		var a = axel * 10000000000000;
		document.getElementById('SpotLightContainer').innerHTML = '<IFRAME SRC="http://fls.doubleclick.net/activityi;src=1869704;type=betty077;cat=saveb643;ord=1;num=' + a + '?" WIDTH=1 HEIGHT=1 FRAMEBORDER=0></IFRAME>';
        fireMarin('Save_BC');
	}

	function AddToRecipeBox() {
		SaveBCSpotLightTag();

		var recipeId = jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRecipeId").val();
		var recipeCategory = jQuery("#ddlFolders").val().toUpperCase();
		var recipeNewCategory = jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_txtNewFolderName").val();;
		var isValid = false;
		
		switch (recipeCategory) {
			case "SPACER": alert("Please Select a Folder Name or 'Create New Folder'"); break;
			case "SELECT ONE": alert("Please Select a Folder Name or 'Create New Folder'"); break;
			case "NEW":
				if (Page_ClientValidate("vgRBDSaveRecipe")) {
					isValid = true;
				}
				 break;
			default: isValid = true;
		}

		if (isValid) {
  			GeneralMills.BettyCrocker.RecipeBox.AddToRecipeBoxCategory(recipeId, recipeCategory, recipeNewCategory, AddRecipeBoxCategoryResult, AddRecipeBoxCategoryError);
            var recipeTitle = jQuery('meta[name="TITLE"]').attr("content");
            ntptEventTag('ev=RecipeAdd_BT&' + 'RecipeId=' + recipeId + '&RecipeTitle=' + recipeTitle);	   
		}
	}

	function AddRecipeBoxCategoryResult(result) {	
	    jQuery("#divRecipeSaved").html(result[0]);
	    jQuery("#recipeBoxGoToLink").html(result[1]);
	    			
		jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_SaveRecipePanel").hide();
		jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_ConfirmationPanel").show();
		
		if ('True' == 'True')
		{
			RecipeBoxWebTrendsSave(jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRecipeId").val(),jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnMemberGenerated").val());

		}
	}

	function AddRecipeBoxCategoryError(error) {
		alert("Error occured saving recipe, please try again");
	}
	
	jQuery.expr[':'].Contains = function(a, i, m) {
		return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase()) >= 0;
	};

	function ValidateUniqueCategory(source, args) {
		var isValid = true;

		var newCategoryText = jQuery("#main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_txtNewFolderName").val();
		jQuery("#ddlFolders").find("option:Contains('" + newCategoryText + "')").each(function() {
			if (jQuery(this).text().toUpperCase() == newCategoryText.toUpperCase()) {
				isValid = false;
			}
		});
		
		args.IsValid = isValid;
	}
   		
</script>



<script type="text/javascript" language="javascript">

    function addSpotlightTrackingIFrame() {
        var axel = Math.random() + "";
        var a = axel * 10000000000000;
        //var d = $get('emailIframe');
        document.getElementById('Iframe').innerHTML = '<IFRAME SRC="http://fls.doubleclick.net/activityi;src=1869704;type=betty077;cat=email688;ord=1;num=' + a + '?" WIDTH=1 HEIGHT=1 FRAMEBORDER=0></IFRAME>';
    }
    
    var emailMemberGeneratedPrefix = '/usergen';
    var emailKitchenGeneratedPrefix = '/recipes';

    function sendEmailWebTrendCall() {
               
            var recipeId = jQuery("#main_0_leftcolumn_0_ucEmailRecipe1_hdnEmailRecipeId").val();
            var emailMemberGenerated = jQuery("#main_0_leftcolumn_0_ucEmailRecipe1_hdnMemberGenerated").val();
            if (emailMemberGenerated == "true") {
                dcsMultiTrack('DCS.dcsuri', emailMemberGeneratedPrefix + '/emailrecipethankyou.aspx', 'DCS.dcsqry', 'RecipeId=' + recipeId, 'WT.ti', 'Thank You For Forwarding a Recipe from BettyCrocker.com');
                }
            else {
                dcsMultiTrack('DCS.dcsuri', emailKitchenGeneratedPrefix + '/emailrecipethankyou.aspx', 'DCS.dcsqry', 'RecipeId=' + recipeId, 'WT.ti', 'Thank You For Forwarding a Recipe from BettyCrocker.com');
            }
        
    }

     function openEmailRecipe(recipeId, memberGenerated) {
        jQuery("#main_0_leftcolumn_0_ucEmailRecipe1_hdnEmailRecipeId").val(recipeId);
         jQuery("#main_0_leftcolumn_0_ucEmailRecipe1_hdnMemberGenerated").val(memberGenerated);
        OpenVariableDialog('main_0_leftcolumn_0_ucEmailRecipe1_RecipeEmailDialog_EmailDialog1','/EmailModalPopup','Email Modal Popup',false);;
    }

