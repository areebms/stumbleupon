            <script type="text/javascript"> 
			var pageUrl = new Array();			
			pageUrl[1] = "tab1.php";
			pageUrl[2] = "tab2.php";
			pageUrl[3] = "tab3.php";
			
			var curr = "tab2";	
		
			function loadTab(id)
			{
				var url = "tab" + id;
				//var location = document.getElementsByTagName('base')[0].href + pageUrl[id];
				document.getElementById(curr).style.backgroundImage = "url(../images/big_bg.jpg)";
				document.getElementById(curr).style.color = "#FFF";
				curr = url;
				
				//document.getElementById(url).className = "tabbedPanelsTabSelected";	
				document.getElementById(url).style.backgroundImage = "url(../images/tab_selbg.jpg)";
				document.getElementById(url).style.color = "#000";
				if (pageUrl[id].length > 0)
				{
					$("#preloader").show();
					$.ajax(
			        {
			            url: pageUrl[id], 
			            cache: false,
			            success: function(message) 
			            {			            	
			                $("#tabcontent2").empty().append(message);
			                $("#preloader").hide();             
			            }
			        });	
			 	        
				}
			}
		
			$(document).ready(function()
			{
				$("#preloader").hide();
	
				
				
				$("#tab1").click(function()
				{
					loadTab(1);
				});
				
				$("#tab2").click(function()
				{
					loadTab(2);
				});
				
				$("#tab3").click(function()
				{
					loadTab(3);
				});

			});
