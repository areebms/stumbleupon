<h1><div class="title entry-title">Expensive mountain search finds stoned teen</div></h1>
<script language="JavaScript" type="text/javascript">
	function createQString(s) {
		return escape(s);
	}
	function stripHTML(s){
		var re= /<\S[^><]*>/g
		return s.replace(re, "")
	}
	var Heading = "Expensive%20mountain%20search%20finds%20stoned%20teen";
	var strippedHeading = stripHTML(Heading);
	var tempTitle = createQString(strippedHeading);
	var ArticleTitle = "&t="+tempTitle;
